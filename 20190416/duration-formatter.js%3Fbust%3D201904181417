define(["common/moment","vendors/domReady"],function(t,o){"use strict";function n(){var t=this;o(function(){return t.format()})}return n.prototype.format=function(){var o=arguments.length>0&&void 0!==arguments[0]?arguments[0]:document,n=o.querySelectorAll(".js-article__duration");Array.prototype.forEach.call(n,function(o){var n=o.dataset.duration;if(n>0){var r=t.duration(1,"hours").asMilliseconds(),s=t.duration(parseInt(n),"seconds").asMilliseconds(),a=s>r?"HH:mm:ss":"mm:ss";o.innerHTML=t.utc(s).format(a),o.classList.add("show")}})},new n});