define(["vendors/twitter-widget"],function(){"use strict";function t(){this.init()}return t.prototype.init=function(){var t=document.querySelectorAll(".twitter-block");Array.prototype.forEach.call(t,function(t){t.setAttribute("data-init",1);var e=t.getAttribute("tweet-id"),i=t.getAttribute("tweet-type");"twitter-moment"==i?twttr.widgets.createMoment(e,t,{limit:5}):twttr.widgets.createTweet(e,t,{linkColor:"#55acee",conversation:t.dataset.conversation})})},t});