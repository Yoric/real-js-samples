(function($, cfg) {
    var $name = $('.sku-name,#name h1')
    var text = $.trim($name.text())
    if (text === '') {
        $.ajax({
            url: '//yx.3.cn/service/info.action?ids='+ cfg.skuid +'&u_source=weidian',
            dataType: 'jsonp',
            success: function(r) {
                if (r && r[cfg.skuid]) {
                    $name.text(r[cfg.skuid].name)
                }
            }
        })
    }
})(jQuery, pageConfig.product);

