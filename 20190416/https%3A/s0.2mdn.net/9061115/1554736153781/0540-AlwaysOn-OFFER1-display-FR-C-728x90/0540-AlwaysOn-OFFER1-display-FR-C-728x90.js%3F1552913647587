(lib.CTABtnComp = function(mode,startPosition,loop) {
	this.initialize(mode,startPosition,loop,{});

	// timeline functions:
	this.frame_0 = function() {
		this.stop();
		this.flashState = true;
		this.count;
		this.timer;
		this.limit;
		this.stopNextTimeVisible;
		
		this.flash = function(i){
			this.count = 0;
			this.limit = i||-1;
			this.stopNextTimeVisible = false;
			this.timer = window.setInterval(this.doFlash.bind(this), 250);
		}
		
		this.stopFlashing = function(){
			this.stopNextTimeVisible = true;
		}
		
		this.clearFlashInterval = function(){
			window.clearInterval(this.timer);
		}
		
		this.doFlash = function(){
			this.flashState = !this.flashState;
			if(this.flashState){
				if(this.stopNextTimeVisible){
					this.clearFlashInterval();
				}
				this.count++;
			}
			
			this.btn_label.visible = this.flashState;
			
			if(this.limit > -1 && this.count == this.limit-1)
				this.stopFlashing();
		}
	}

	// actions tween:
	this.timeline.addTween(cjs.Tween.get(this).call(this.frame_0).wait(1));

	// Label
	this.btn_label = new lib.cta_label();
	this.btn_label.name = "btn_label";
	this.btn_label.parent = this;
	this.btn_label.setTransform(70,12.3,1,1,0,0,0,70,8.3);
	this.btn_label.shadow = new cjs.Shadow("rgba(0,0,0,1)",0,1,0);

	this.timeline.addTween(cjs.Tween.get(this.btn_label).wait(1));

	// BG Fill
	this.shape = new cjs.Shape();
	this.shape.graphics.lf(["#00661E","#00B049"],[0,1],-21.8,17,-21.8,-10.5).s().rr(-82.25,-13,164.5,26,3);
	this.shape.setTransform(59.9988,12.9882,0.7297,1.0019);

	this.timeline.addTween(cjs.Tween.get(this.shape).wait(1));

}).prototype = getMCSymbolPrototype(lib.CTABtnComp, new cjs.Rectangle(-2,0,128,29), null);


