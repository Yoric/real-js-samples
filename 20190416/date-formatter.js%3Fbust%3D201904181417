define(["common/moment","vendors/domReady"],function(t,e){"use strict";function d(){e(this.format.bind(this))}return d.prototype.format=function(){var e=arguments.length>0&&void 0!==arguments[0]?arguments[0]:document,d=e.querySelectorAll("time"),a=/(\d{4}-[01]\d-[0-3]\dT[0-2]\d:[0-5]\d:[0-5]\d\.\d+)|(\d{4}-[01]\d-[0-3]\dT[0-2]\d:[0-5]\d:[0-5]\d)|(\d{4}-[01]\d-[0-3]\dT[0-2]\d:[0-5]\d)/;Array.prototype.forEach.call(d,function(e){var d=e.getAttribute("data-format");if(a.test(e.getAttribute("datetime"))){var r;if("fromNow"===d||"calendar"===d)r=t(e.getAttribute("datetime"))[d]();else if(r=t(e.getAttribute("datetime")).format(e.getAttribute("data-format")),e.classList.contains("js-article__date-credit__date")){var i=t(e.getAttribute("datetime")).add(2,"days"),o=t();o>i&&"DD MMM HH:mm"===e.getAttribute("data-format")&&(r=t(e.getAttribute("datetime")).format("DD MMM YYYY HH:mm"))}e.innerHTML=r}e.classList.add("show")})},new d});