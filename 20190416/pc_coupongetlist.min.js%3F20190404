(function(){var a="/api/coupongetlist/getlist";var b='クーポン<a class="elMyTraCouponHeaderCouponLink" href="https://my.travel.yahoo.co.jp/couponlist/?sc_i=trvmhd_couponlist">';$.ajax({type:"GET",url:a,timeout:"5000",cache:false,dataType:"json",success:function(c){if(c.ResultSet.Status!="000000"){}else{if(c.ResultSet.Result.tac>0){if(c.ResultSet.Result.coupon_alert==""){text=b+c.ResultSet.Result.tac+"枚</a>"}else{text=b+c.ResultSet.Result.tac+'枚</a><span class="tPointWarn">'+c.ResultSet.Result.coupon_alert+"</span>"}document.getElementById("coupon_link").innerHTML=text;document.getElementById("coupon_link").style.display="inline"}else{document.getElementById("coupon_link").style.display="inline"}}},error:function(){}})})();