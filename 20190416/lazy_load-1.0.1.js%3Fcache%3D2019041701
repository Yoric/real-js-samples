var LazyLoadImage=function(){"use strict";var e=this;e.init=function(a){e.params=a,e.replaceImage(),e.replaceBackground(),e.replace_section()},e.replaceImage=function(){void 0!==e.params&&void 0!==e.params.class_name&&void 0!==e.params.class_name&&jQuery("."+e.params.class_name).each(function(){var a=jQuery(this).data(e.params.dataSrc);jQuery(this).attr("src",a)})},e.replaceBackground=function(){void 0!==e.params&&void 0!==e.params.classBkg&&e.params.dataBkg&&jQuery("."+e.params.classBkg).each(function(){var a=jQuery(this).data(e.params.dataBkg);jQuery(this).css("background-image",'url("'+a+'")')})},e.replace_section=function(){if(void 0!==e.params&&void 0!==e.params.sections&&void 0!==e.params.thumbUrl)for(var a=0,r=e.params.sections.length;a<r;a++)jQuery("#"+e.params.sections[a]+" li img").each(function(){jQuery(this).attr("src",jQuery(this).data(e.params.thumbUrl))})}};if("function"==typeof jQuery(document).ready)jQuery(document).ready(function(){(new LazyLoadImage).init(page_params.lazyLoad)});else{var myRawLazy=new LazyLoadImage;myRawLazy.init(page_params.lazyLoad)}