var YAHOO=YAHOO||{};YAHOO.JP=YAHOO.JP||{};YAHOO.JP.tracking={tracking_list:tracking_list=new Array(),tracking_keys:tracking_keys=new Array(),tracking_key_max:0,set_tracking_key:function(a){this.tracking_keys=a.slice();this.tracking_key_max=this.tracking_keys.length;return this},get_tracking_parameter:function(){var f=window.location.search.substring(1);if(!f){return this}var e=f.split("&");var d=e.length;for(var c=d;c--;){for(var a=this.tracking_key_max;a--;){var b=e[c].split("=");if(this.tracking_keys[a]!==b[0]){continue}if(b[1]){this.tracking_list[this.tracking_keys[a]]=b[1]}}}return this},update_localStorage:function(){if(!this.is_available_localStorage()){return this}for(var b=this.tracking_key_max;b--;){if(this.tracking_list[this.tracking_keys[b]]){var c={tracking_param:this.tracking_list[this.tracking_keys[b]],timestamp:new Date().getTime()};localStorage.setItem(this.tracking_keys[b],JSON.stringify(c))}else{var d=JSON.parse(localStorage.getItem(this.tracking_keys[b]));if(d==null){continue}if(a(d.timestamp)){var c={tracking_param:d.tracking_param,timestamp:new Date().getTime()};localStorage.setItem(this.tracking_keys[b],JSON.stringify(c))}else{localStorage.removeItem(this.tracking_keys[b])}}}return this;function a(e){var f=new Date().getTime()-e;if(f<3600000){return true}else{return false}}},get_localStorage:function(){if(!this.is_available_localStorage()){return this}for(var a=this.tracking_key_max;a--;){var b=JSON.parse(localStorage.getItem(this.tracking_keys[a]));if(b!=null){this.tracking_list[this.tracking_keys[a]]=b.tracking_param}else{this.tracking_list[this.tracking_keys[a]]=null}}return this},add_link_paramaeter:function(d){if(d==undefined){return this}try{$("body").prepend('<div id="tracking_parameter"></div>');var b="https://travel.yahoo.co.jp/";var g="slk:_";var a="";for(var c=this.tracking_key_max;c--;){if(this.tracking_list[this.tracking_keys[c]]!==null){g="slk:"+this.tracking_list[this.tracking_keys[c]]}else{g="slk:_"}a="rsec:"+this.tracking_keys[c];$("#tracking_parameter").append('<a href="'+b+'" data-ylk="'+g+";"+a+';pos:1"  ></a>')}$("#tracking_parameter").hide();d.addModules("tracking_parameter")}catch(f){}return this},is_available_localStorage:function(){try{localStorage.setItem("is_available","value");var b=localStorage.getItem("is_available");localStorage.removeItem("is_available");return true}catch(a){return false}}};