_n");return true}));if(new_links.length){E.stats.inc("found_links_n",new_links.length);E.stats.inc("found_n");E.emit("update",new_links)}var overrides=[];links.forEach(function(l){if(l.type!="video")return;E.stats.inc("type_video_n");if(!l.page_url||l.url==l.page_url)return;E.stats.inc("type_video_override_n");overrides.push(assign({},l,{type:"page",url:l.page_url}))});etask([function(){return set_links_db(links.concat(overrides),E.conf)},function finally$(){submit_scan()}]);return links.length};function clear_scan(){scan_timeout=clearTimeout(scan_timeout)}function submit_scan(){clear_scan();var diff=Date.now()-E.first_scan;scan_timeout=setTimeout(E.scan,diff<5e3?500:diff<15e3?1e3:3e3)}function listen_href_updates(links,opt){var href_prop="hola-links-href";links.forEach(function(l){var link=l.link,$link=$(link);if($link.prop(href_prop))return;$link.prop(href_prop,l.url);util.dom_listen(link,function(){var new_href=E.get_href(link,opt);if($link.prop(href_prop)==new_href)return;$link.prop(href_prop,new_href);E.emit("detach",[link]);scanned(link,0);E.scan()})})}E.get_link_info=function(a,opt){var spark_selectors={watch_later:".watch_later_list_item a",preview_over_player:".hola_preview_over_player",widget:".hola_widget_list_item, .hola_widget_poster, "+".hola_widget_tile",previews_widget:".hola_previews_widget_link",playlist:"[data-hola-playlist-link]",popin_popular:".spark_popin_popular_thumbnail"};var $a=$(a),video_url,allow_video;var v_url=$a.attr("spark_ve_preview")||$a.attr("spark_video_url")||$a.attr("hola_ve_preview")||$a.attr("hola_spark_video_url");if(allow_video=v_url!="none")video_url=v_url;var adaptive,cache_v_url;if(video_url){adaptive=!!util.get_adaptive_type(video_url);cache_v_url=E.spark.video2cache(video_url,adaptive)}var page_url=get_href(a,opt);var url=allow_video?E.get_href(a,opt):page_url;var cache_url=E.spark.page2cache(url);var unite_sel=opt.unite_img_and_link=="a"&&a.tagName!="A"?a:opt.unite_img_and_link;var unite,links=$([]);if(unite_sel){var u=$a.closest(unite_sel);if(u.length&&u[0]!=a)unite=u;links=u.find("a[href]").not($a).filter(function(el){return E.get_href(el,opt)==url})}var by_spark=Object.keys(spark_selectors).find(function(s){return $a.is(spark_selectors[s])});var img=get_img(a),size,poster,nat_size;if(img.length){var size_el=E.get_img_size_el(img,opt);var s=E.size_util.get_size(size_el[0]);size=E.size_util.serialize(s);var img_src=E.get_img_src(img,opt);var natural_size=E.size_util.get_natural_size(img[0],img_src,by_spark);if(E.size_util.calc(s)<400||s.width>2e3||s.height>2e3){perr({id:"poster_size",throttle:1,info:{s:s,size:size,img:img_src,nat_size:E.size_util.serialize(natural_size),reason:s.width>2e3||s.height>2e3?"too_big":"too_small"}})}if(by_spark||zutil.is_mocha())poster=img_src;else if(!E.is_poster_allowed(img_src,opt))E.stats.inc("poster_not_allowed");else if(!natural_size.width)E.stats.inc("nat_size_missing");else if(E.size_util.calc(natural_size)>400){E.stats.inc("nat_size_ok");poster=img_src;nat_size=E.size_util.serialize(natural_size)}else E.stats.inc("nat_size_small")}var text_links=opt.text_links||[];return{link:a,links:links,unite:unite,url:video_url||url,page_url:page_url,cache_url:cache_v_url||cache_url,type:video_url?"video":"page",img:img,poster:poster,size:size,nat_size:nat_size,description:get_link_text(a,links,img,opt),video_url:cache_v_url,adaptive:adaptive,root:/.*\/\/[^\/]*\/$/.test(url),self:cache_url==E.spark.cache_top_url,by_spark:by_spark,force_video:allow_video&&!!v_url,no_video:!allow_video,text_link:text_links.includes(a),playlist:is_playlist_link(a)}};function get_links(opt){E.stats.inc("get_n");var cur_links=[],conf_links=opt.links_selector||"a";var sel_ar=Array.isArray(conf_links)?conf_links:[conf_links];if(opt.text_links_selector)sel_ar.push(opt.text_links_selector);sel_ar.push("a[spark_ve_preview], a[spark_video_url], a[hola_ve_preview], "+"a[hola_spark_video_url], a[spark_page_url], a[hola_spark_page_url], "+".hola_preview_over_player, .watch_later_list_i