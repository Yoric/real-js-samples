deo_error/,all_failed:/^all failed$/};for(var key in errors){if(errors[key].test(err))return void E.stats.inc(prefix+"fail_"+key+"_n")}E.stats.inc(prefix+"fail_error_n");return}var status;if(!(status=res&&res.status)){E.stats.inc(prefix+"fail_n");E.stats.inc(prefix+"fail_no_status_n");return}if(status=="not_requested"&&route&&route.no_generate)return void E.stats.inc(prefix+"skip_no_generate_n");var elapsed=res.elapsed;var limit=["downloading","generating"].includes(status)?PROCESSING_LIMIT_ACTIVE:PROCESSING_LIMIT;if(elapsed<limit){E.stats.inc(prefix+"processing_short_n");E.stats.inc(prefix+"processing_short_"+status+"_n");return}E.stats.inc(prefix+"fail_n");E.stats.inc(prefix+"fail_processing_"+status+"_n")}function get_stats_cb(prefix,route){prefix=prefix?prefix+"_":"";return{on_req:function(){E.stats.inc(prefix+"try_n")},on_res:function(n,data){check_status(prefix,data,route)}}}Thumbnails.prototype.get_thumb_err_msg=function(data){if(data.res&&data.res.error)return"Error: "+data.res.error;if(!data.res||!data.res.status)return"Network failure";switch(data.res.status){case"downloading":case"generating":case"download_queue":return"Generation in progress";case"disabled":return"Feature disabled";case"generate_queue_full":return"Servers overloaded";case"unknown":return"Unknown problem"}};Thumbnails.prototype.load_thumb_info=function(){if(this.loading_task||this.thumbs)return this.loading_task||this.thumbs;var _this=this;return this.loading_task=etask({name:"init_thumbnails"},[function(){E.stats.inc("info_fetch_try_n");return E.spark.fetch_from_singles("get_thumb_info",_this.route,assign({cdnlist:E.conf.cdns||E.conf.cdn,zone:E.spark.zone},get_stats_cb("info_req",_this.route)))},function(data){if(!check_status("info_fetch_",data,_this.route))return;var res=data.res||{};var info=res.info,urls=res.urls,cdns=res.cdns;var rows=Math.sqrt(info.group_size),thumbs={};urls.forEach(function(url,_id){thumbs[_id*info.group_size*info.interval]={url:url,id:_id+1,sprites:{width:info.wi