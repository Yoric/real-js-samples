 E.stats.inc("info_fetch_no_data_n");_this.heatmap_data=data;E.stats.inc("info_fetch_have_data_n")},function catch$(e){E.stats.inc("info_fetch_fail_n");perr({id:"heatmap_info_fail",throttle:1,info:{error:""+e}});log.warn("error during fetch heatmap info: "+e)},function finally$(){delete _this.loading_task}])};Heatmap.prototype.detach=function(){if(this.loading_task)this.loading_task.return();if(this._on_timeupdate)this.player.off("timeupdate",this._on_timeupdate);this.hide()};Heatmap.prototype.show=function(){if(!this.viewer&&this.heatmap_data)return this.attach()};Heatmap.prototype.hide=function(){if(!this.viewer)return;this.viewer.detach();this.viewer=null};Heatmap.prototype.check_attached=function(){if(!this.viewer||!this.viewer.is_node_detached())return;log.info("reattach");this.hide();this.show()};Heatmap.prototype.is_allowed=function(){if(this.disabled||this.player.is_any_ad_mode()||this.player.is_live_stream()||E.spark.is_hidden()){return false}return true};function on_player_attach(player,video_ctx,reason){var controls=player.controls,heatmap=video_ctx.attached_heatmap;if(!controls)return log.err("player.controls is undefined");if(!heatmap){heatmap=new Heatmap(player,video_ctx);video_ctx.attached_heatmap=heatmap;heatmap.on_float=heatmap.hide.bind(heatmap);heatmap.on_float_end=heatmap.show.bind(heatmap);heatmap.check_attached=heatmap.check_attached.bind(heatmap)}else if(reason=="restore"){if(!player.is_floating())heatmap.show()}controls.on("persistent.start_floating",heatmap.on_float);controls.on("persistent.end_floating",heatmap.on_float_end);controls.on("play",heatmap.check_attached);controls.on("pause",heatmap.check_attached);player.once("detach",function(_video_ctx,_reason){controls.off("persistent.start_floating",heatmap.on_float);controls.off("persistent.end_floating",heatmap.on_float_end);controls.off("play",heatmap.check_attached);controls.off("pause",heatmap.check_attached);if(_reason=="suspend")return heatmap.hide();heatmap.detach();_video_ctx.attached_heatmap=undefined})}E.init=function(spark,conf){log=spark.log(id);E.spark=spark;E.conf=util.get_feature_conf(conf,E.default_conf);E.stats=E.spark.stats.alloc(id);E.spark.players.on("attach",on_player_attach);E.inited=true;log.info("inited");E.stats.inc("init_n")};return E});
define("/svc/cdn/pub/popin_popular.js",["cash","/util/util.js","/svc/cdn/pub/util.js","/util/etask.js","/svc/cdn/pub/preview.js","/util/string.js"],function($,zutil,util,etask,preview,string){var assign=Object.assign,log={},E={},id="popin_popular";var pr_class="spark_popin_popular";E.default_conf={description_length:90,header_text:"Popular now:",min_scroll_perc:30,playlist_length:10,playlist_filter:null,playlist_period:"1d",playlist_position:0};var def_styles={".spark_popin_popular":{position:"fixed",bottom:"50px",left:"10%",background:"#e2e2e2",width:"25%","max-width":"300px","z-index":1e3,border:"1px solid #424242","border-radius":"4px","font-size":"12px",transition:"left 1.5s ease-out"},".spark_popin_popular_big.spark_popin_popular":{width:"40%","max-width":"500px"},".spark_popin_popular_small.spark_popin_popular_hidden":{left:"-310px"},".spark_popin_popular_thumbnail":{width:"50%","float":"left",display:"block",height:"120px"},".spark_popin_popular_poster":{height:"100%","background-size":"contain","background-repeat":"no-repeat","background-position":"center"},".spark_popin_popular_big .spark_popin_popular_thumbnail":{display:"none"},".spark_popin_popular_header":{width:"50%","float":"left",display:"block",padding:"8px"},".spark_popin_popular_big .spark_popin_popular_header":{display:"none"},".spark_popin_popular_description":{width:"50%","float":"left",display:"block",padding:"8px"},".spark_popin_popular_big .spark_popin_popular_description":{width:"100%"},".spark_popin_popular_video":{width:"100%"},".spark_popin_popular_small .spark_popin_popular_video":{display:"none"}};function apply_styles(){if(E.styles)return;E.styles=util.get_stylesheet(def_styles);$("head").append("<style>"+E.styles+"</style>")}function PopInPopular(opt){if(!(this instanceof PopInPopular))return new PopInPopular(opt);this.opt=opt;this.shown=false;this.create_elem();E.links.scan();var _this=this;this.timeout=setTimeout(function(){_this.create_video();preview.init_autoplay({autoplay_el:pr_class+"_thumbnail"});_this.attach_listeners()},200)}PopInPopular.prototype.create_elem=function(){var div=this.div=$("<div/>").addClass(pr_class+" "+pr_class+"_small "+pr_class+"_hidden");this.thumbnail=$("<div/>").addClass(pr_class+"_thumbnail").attr("spark_page_url",this.opt.url).appendTo(div);$("<div/>").addClass(pr_class+"_poster").css("background-image","url("+this.opt.poster+")").attr("data-src",this.opt.poster).appendTo(this.thumbnail);$("<div/>").addClass(pr_class+"_header").text(E.conf.header_text).appendTo(div);this.description=$("<div/>").addClass(pr_class+"_description").text(this.opt.description).appendTo(div);div.appendTo("body")};PopInPopular.prototype.create_video=function(){var video_url=this.opt.video_url;var video_type=util.get_video_type(video_url);this.can_play=video_type=="mp4"||video_type=="dash"&&window.dashjs;if(!this.can_play)return;var video=$("<video/>").addClass(pr_class+"_video").attr({src:video_url,controls:true}).prependTo(this.div);this.video=video[0];if(video_type=="dash")window.dashjs.MediaPlayerFactory.create(this.video)};PopInPopular.prototype.attach_listeners=function(){this.div.on("click",this.on_click=this.click.bind(this));this.description.on("click",this.on_close=this.close.bind(this));$(window).on("scroll",this.on_scroll=util.debounce(this.scroll.bind(this)))};PopInPopular.prototype.click=function(){if(this.is_big)return;if(!this.can_play){E.stats.inc("navigated_n");window.location=this.opt.url;return}this.is_big=true;this.div.removeClass(pr_class+"_small");this.div.addClass(pr_class+"_big");this.thumbnail.remove();this.video.play();E.stats.inc("played_n")};PopInPopular.prototype.close=function(){if(!this.is_big)return;this.destroy();E.stats.inc("closed_n")};PopInPopular.prototype.scroll=function(){var scroll_perc=window.scrollY/(document.body.scrollHeight-window.innerHeight)*100;var should_show=scroll_perc>E.conf.min_scroll_perc;if(should_show==this.shown)return;E.stats.inc(should_show?"shown_n":"hidden_n");this.shown=should_show;this.div.toggleClass(pr_class+"_hidden",!should_show);if(!should_show)return;this.timeout=setTimeout(function(){preview.emit("start_preview")},2e3);if(this.ever_shown)return;E.stats.inc("shown_unique_n");this.ever_shown=true};PopInPopular.prototype.destroy=function(){clearTimeout(this.timeout);this.div.off("click",this.on_click);this.description.off("click",this.on_close);$(window).off("scroll",this.on_scroll);if(this.video)this.video.pause();this.div.remove()};function load_data(){var playlist,links,db_info;etask([function(){return etask.sleep(5e3)},function(){return E.spark.get_playlists([E.conf.playlist_period],E.conf.playlist_length,{})},function(res){playlist=util.conv_playlist_for_widget(res[Object.keys(res)[0]]||[]);links=playlist.filter(function(d){return E.spark.page2cache(d.page_url)!=E.spark.cache_top_url}).map(function(d){return{url:d.page_url,type:"page"}});db_info=E.links.get_db(this.links);var has_preview=db_info.find(function(d){return d.preview&&d.preview.url});if(!has_preview)return E.links.force_get_db(links)},function(res){if(res&&res.res)db_info=E.links.get_db(links);db_info=db_info.filter(function(d){return d.full_video_url&&d.preview&&d.preview.url&&(d.title||d.description)&&(d.poster||d.video_poster)});if(!db_info.length){E.stats.inc("playlist_empty_n");return this.return()}var list=db_info.map(function(db){return assign(db,playlist.find(function(d){return db.url==d.page_url}))}).sort(function(a,b){return b.total-a.total});if(E.conf.playlist_map)list=list.map(E.conf.playlist_map);if(E.conf.playlist_filter)list=list.filter(E.conf.playlist_filter);var sel=list[E.conf.playlist_position];if(!sel){E.stats.inc("playlist_no_element_n");return this.return()}var description=string.longest(sel.description,sel.title);var max_len=E.conf.description_length;if(description.length>max_len)description=description.substr(0,max_len-4)+" ...";new PopInPopular({url:sel.page_url,description:description,poster:sel.poster||sel.video_poster,preview:sel.preview,video_url:sel.full_video_url});E.stats.inc("created_n")}])}E.init=function(spark,conf){E.spark=spark;E.links=spark.links;log=E.spark.log(id);E.conf=util.get_feature_conf(conf,E.default_conf);E.stats=E.spark.stats.alloc(id);apply_styles();load_data();E.inited=true;log.debug_spark("inited");E.stats.inc("init_n")};return E});
