;for(var name in cache){if(name.endsWith("_playlist")&&cache[name])cache[name].playlist=undefined}return h}function host_init(url){E.host=zurl.parse(url).hostname}function conf_init(conf,url){var r,params=E.spark.params||{};E.conf=util.get_platform_conf(conf);if(E.conf&&(r=E.conf.conf)&&r.length)match_conf(r);E.conf=zutil.defaults_deep({},E.conf,E.default_conf);E.conf.link_tags.push({name:hash_param,type:E.conf.disable_hash?"query":"auto"});var ptile_conf=E.conf.ptile_conf,name="spark_ve_playlist_";if(Object.keys(params).length){["on_pause","on_end"].forEach(function(e){if(params[name+e])E.conf[e]=params[name+e]});if(params[name+"show_play_icon"])ptile_conf.show_play_icon=1;if(params[name+"autoclick"]){E.conf.autoclick.mode="inline";E.conf.autoclick.count=+params[name+"autoclick"]}if(params.spark_playlist_inline){zutil.extend_deep(E.conf,{on_pause:"popup",autoclick:{mode:"inline"},pnext_conf:{next_by:"popular"}})}if(params.debug_hints)debug_hints=true}if(ptile_conf.num_mode=="manual"){ptile_conf.cols=ptile_conf.cols||3;ptile_conf.rows=ptile_conf.rows||3}}E.init=function(spark,conf){E.spark=spark;E.links=spark.links;log=E.spark.log(id);conf_init(conf,E.spark.get_top_url());debug_hints=util.has_ext&&E.conf.debug_hints||spark.conf.debug;play_icon.init();t=l10n.get_t(id);var strings=assign(E.conf.text?{text:E.conf.text}:{},zutil.get(E.conf,"strings",{}));l10n.override(strings,id);E.playlist=[];E.frames=[];E.stats=spark.stats.alloc(id);E.msg=E.spark.Msg("pl",{child_frames:true});setup_msg_handlers();if(!E.conf.disable_css)dom_util.preload_css(util.player_url+require.zdot("playlist_css"));add_conf_func("is_ad_playing",E.conf.ad_detect);add_conf_func("is_floating_mode",E.conf.persistent_detect);add_conf_func("is_ended",E.conf.is_ended);add_conf_func("close_plugins",E.conf.close_plugins);E.spark.api.playlist_widget=playlist_widget;E.spark.on("history_synced",history_init);E.spark.on("set_top_url",host_init);history_init();host_init(E.spark.get_top_url());if(!E.conf.disable_ui){if(!zutil.is_mocha())find_next_video(E,E.conf);E.spark.players.on("attach",on_player_attach);E.spark.players.on("attach_ad",on_player_ad_attach)}E.inited=true;log.debug_spark("inited")};E.t={find_next_video:find_next_video,cache:cache,history_init:history_init};return E});
