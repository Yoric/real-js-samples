opt=assign({},this.preview.opt,opt);this.update=this.update.bind(this);E.links.on("update_db",this.update);this.update()}Purge.prototype.update=function(){var info=this.preview.get_info()||{};if(this.removed||info.error||info.status=="no_mapping")this.hide();else this.show()};Purge.prototype.show=function(){if(this.elem||this.off_img_load)return;var img=$(this.preview.img)[0],_this=this;if(!is_img_loaded(img)){this.off_img_load=on_img_load(img,function(){_this.off_img_load=null;_this.show()});return}var pos=this.preview.get_position(true);var elem=$("<div class=hola_preview_purge></div>");elem.title="Purge this video preview. It will be regenerated next time "+"video is viewed";var margin=10;elem.css({top:pos.height+pos.top-purge_icon_size-margin+"px",left:pos.width+pos.left-purge_icon_size-margin+"px",transform:pos.transform});elem.on("click",function(e){e.stopImmediatePropagation();e.preventDefault();_this.purge()});var outer_container;if(outer_container=this.preview.create_relative_container()){$(img).after(outer_container);$(outer_container).append(elem);elem.css({top:0,left:0});elem=$(outer_container)}else $(img).after(elem);this.elem=elem};Purge.prototype.hide=function(){if(!this.elem)return;if(this.off_img_load)this.off_img_load();this.off_img_load=null;this.elem.remove();this.elem=null};Purge.prototype.purge=function(){if(!util.conf.ext_api_key)return;var _this=this;var url=zescape.uri("https://client.h-cdn.com/api/purge_page",{customer:E.spark.customer,key:util.conf.ext_api_key,url:this.url});etask({name:"preview_purge",cancel:true},[function(){return util.fetch_bin({url:url,response_type:"json",timeout:5e3})},function(res){var data=res&&res.status==200?{url:url,res:res.data}:null;if(!data)return;_this.removed=true;_this.preview.clear_cache();_this.update();_this.preview.update()}])};Purge.prototype.destroy=function(){E.links.off("update_db",this.update);this.hide();this.preview=this.url=this.opt=null};function is_img_loaded(img){return img.width>1&&img.naturalWidth>1}function on_img_load(img,cb){var off,unwatch;var on_load=function(){off();if(img.style.display!="none")return void cb();unwatch=util.attr_watch(img,{attr:"style",watch_fn:function(){if(img.style.display=="none")return;unwatch();unwatch=null;cb()}})};off=function(){$(img).off("load",on_load)};$(img).on("load",on_load);return function(){off();if(unwatch)unwatch($(img).off("load",on_load))}}function get_mid_rect(check_x){var opt=E.conf.autoplay||{};var vp=dom_util.visual_viewport();var mid_y=vp.top+vp.height/2,mid_x=vp.left+vp.width/2;var height=vp.height*(opt.area_height||10)/100;var width=check_x?vp.width*(opt.area_width||10)/100:vp.width;return{top:mid_y-height/2,left:mid_x-width/2,bottom:mid_y+height/2,right:mid_x+width/2,height:height,width:width}}function is_new_link(opt,l){if(l.no_video||l.current)return false;if(!l.force_video){if(l.root)return false;var href=$(l.link).attr("href");if(href&&href[0]=="#"&&!opt.href_keep_hash)return false}if($(l.link).hasClass("hola_preview_poster"))return false;return!$(l.link).prop("hola-video-preview-id")}function get_new_links(opt){return E.links.get(opt).filter(is_new_link.bind(null,opt))}function get_previews(new_links,opt){if(!new_links.length)return;return etask([function(){var count=E.stats.inc("get_previews_n");if([5,10,20,50].indexOf(count)>-1)E.stats.inc("get_previews_over_"+count+"_n");E.stats.inc("get_previews_url_n",new_links.length);return E.links.force_get_db(new_links,assign({full:opt.debug||opt.ext_info_enabled},opt))},function(res){var data;if(!(data=res&&res.res))throw new Error("no links data");new_links.forEach(function(p){handle_links_db_entry(data[p.url])});E.previews.update();return{cdn:res.cdn}},function catch$(e){E.stats.inc("get_previews_fail_n");var msg=e.message||"unknown";log.info("get_previews error: %s",msg);perr({id:"get_previews_error",err:msg})}])}function within(outer,inner){var o=outer.getBoundingClientRect();var i=inner.getBoundingClientRect();return i.left>=o.left&&i.top>=o.top&&i.right<=o.right&&i.bottom<=o.bottom}function mouse_within(el,ev){if(ev&&ev.type=="mousemove")return E.spark.pointer_within(el,ev)}function mouse_in_parent(el,ev){if(!ev||ev.reused)return;if(ev.currentTarget==el||ev.srcElement==el)return true;if(!ev.srcElement)return;return $(ev.srcElement).parents().filter(el).length}function force_update_links(opt){var nl=E.links.filter(get_new_links(opt),opt);etask([function(){return get_previews(nl,opt)},function(res){if(!res||!res.cdn)return;var _opt=assign({},opt,res);nl.forEach(function(l){if(l.text_link&&util.is_mobile)return;var p=new Preview(l,_opt);E.stats.add_preview(p);E.previews.add(p);p.init();E.stats.inc("preview_n")});E.emit("has_previews")}])}function update_links(opt,links){var nl;if(links&&E.conf.avoid_links_rescan)nl=links.filter(is_new_link.bind(null,opt));else nl=get_new_links(opt);nl=E.links.filter(nl,opt);if(!nl.length)return;var used_cache;nl.forEach(function(l){if(l.text_link&&util.is_mobile)return;var p=new Preview(l,opt),cache;E.stats.add_preview(p);E.previews.add(p);p.init();E.stats.inc("preview_n");if(E.conf.disable_map_cache||!(cache=E.storage.get(["previews",p.href]))){return}previews_map[p.href]=assign({},cache,{from_cache:true});if(E.conf.check_lazy_img_load)p.update_size(l);p.update();E.stats.set_ms_since("first_ready_from_cache_ms");E.stats.set_ms_since("first_ready_ms");E.stats.inc("map_cache_use_n");used_cache=true});E.emit("has_previews");if(used_cache)E.emit("has_previews_db")}function update_links_db(opt,links){(links||E.links.get_db(null,opt)).forEach(function(d){if(!d.db)return;handle_links_db_entry(d)});E.previews.update();E.storage.sync();E.emit("has_previews_db")}function handle_links_db_entry(d){if(!d)return;var url_fail=function(reason){E.stats.inc("get_previews_url_fail_n");E.stats.inc("get_previews_url_fail_"+reason+"_n")};var preview;if(!(preview=d.preview)){if(!E.conf.disable_map_cache&&E.storage.get(["previews",d.url])&&previews_map[d.url]&&previews_map[d.url].from_cache){delete previews_map[d.url];E.storage.unset(["previews",d.url]);E.stats.inc("map_cache_delete_n")}url_fail("processing");return}if(typeof preview=="string")preview={url:preview};if(preview.error){if(typeof preview.error!="string")url_fail("unknown");else if(preview.error.includes("disconnected agent"))url_fail("disconnected_agent");else if(preview.error.startsWith("Error:"))url_fail("exception");else url_fail(preview.error.replace(/[^a-zA-Z0-9_]/g,"_"));log.info("get_previews error: %s %s",preview.error,d.url)}preview.cdn=d.cdn;previews_map[d.url]=preview;apply_pixel_ratio(d.url);if(!E.conf.disable_map_cache){E.storage.set(["previews",d.url],assign({update:Date.now()},zutil.pick(preview,"url","cdns","cdn")))}E.stats.set_ms_since("first_ready_from_web_ms");E.stats.set_ms_since("first_ready_ms")}function apply_pixel_ratio(href){var preview=previews_map[href],ratio=E.pixel_ratio;if(!preview||ratio==1)return;var url=preview.url||url;var cur_size=/&size=(\d{1,4}x\d{1,4})/.exec(url);cur_size=cur_size&&cur_size[1];if(!cur_size)return;var size_util=E.links.size_util;cur_size=size_util.parse(cur_size);var new_size=size_util.serialize({width:cur_size.width*ratio,height:cur_size.height*ratio});url=url.replace(/&size=\d{1,4}x\d{1,4}/,"&size="+new_size);if(preview.url)preview.url=url;else previews_map[href]=url}function draw_debug_viewport(check_x){if(!E.conf.debug)return;var $viewport=$(".hola_preview_viewport_debug");if(!$viewport||!$viewport.length){$viewport=$("<div>");$viewport.addClass("hola_preview_debug hola_preview_viewport_debug");$(document.body).append($viewport)}var mid_rect=dom_util.client_rect2page_rect(get_mid_rect(check_x));$viewport.css({top:mid_rect.top+"px",left:mid_rect.left+"px",width:mid_rect.width+"px",height:mid_rect.height+"px"})}function draw_debug_active(_uniq_id,active){if(E.conf.debug)$("#hola_preview_debug_"+_uniq_id).toggleClass("active",!!active)}E.PreviewOverPlayer=function(player,use_top_url,video_url){this.opt=E.conf.preview_over_player;this.player=