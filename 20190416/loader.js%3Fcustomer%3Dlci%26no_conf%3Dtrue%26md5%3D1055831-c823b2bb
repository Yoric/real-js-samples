(function(){var __hola_stub_define;var is_node=typeof module=="object"&&module.exports&&module.children;if(!is_node)define=define||self.define;else define=require("./require_node.js").define(module,"../");define("/util/csv.js",[],function(){var E={};var assign=Object.assign;E.to_arr=function(data,opt){opt=assign({field:",",quote:'"',line:"\n"},opt);var line=opt.line,field=opt.field,quote=opt.quote;var i=0,c=data[i],row=0,array=[];while(c){while(opt.trim&&(c==" "||c=="	"||c=="\r"))c=data[++i];var value="";if(c==quote){c=data[++i];do{if(c!=quote){value+=c;c=data[++i]}if(c==quote){if(data[i+1]==quote){value+=quote;i+=2;c=data[i]}}}while(c&&(c!=quote||data[i+1]==quote));if(!c)throw"Unexpected end of data, no closing quote found";c=data[++i]}else{while(c&&c!=field&&c!=line&&(!opt.trim||c!=" "&&c!="	"&&c!="\r")){value+=c;c=data[++i]}}if(array.length<=row)array.push([]);array[row].push(value);while(opt.trim&&(c==" "||c=="	"||c=="\r"))c=data[++i];if(c==field);else if(c==line)row++;else if(c)throw"Delimiter expected after character "+i;c=data[++i]}if(i&&data[i-1]==field)array[row].push("");return array};E.to_obj=function(data,opt){var arr=E.to_arr(data,opt);if(!arr.length)return arr;var i,result=[],headers=arr[0];if((i=headers.indexOf(""))!=-1)throw new Error("Field "+i+" has unknown name");for(i=1;i<arr.length;i++){var obj={};if(arr[i].length>headers.length)throw new Error("Line "+i+" has more fields than header");for(var j=0;j<arr[i].length;j++)obj[headers[j]]=arr[i][j];result.push(obj)}return result};E.escape_field=function(s,opt){if(s==null&&opt&&opt.null_to_empty)return"";s=""+s;if(!/["'\n,]/.test(s))return s;return'"'+s.replace(/"/g,'""')+'"'};E.to_str=function(csv,opt){var s="",i,j,a;opt=assign({field:",",quote:'"',line:"\n"},opt);var line=opt.line,field=opt.field;function line_to_str(vals){var s="";for(var i=0;i<vals.length;i++)s+=(i?field:"")+E.escape_field(vals[i],opt);return s+line}if(!csv.length&&!opt.keys)return"";if(Array.isArray(csv[0])){if(opt.keys)s+=line_to_str(opt.keys);for(i=0;i<csv.length;i++)s+=line_to_str(csv[i]);return s}var keys=opt.keys||Object.keys(csv[0]);if(opt.print_keys===undefined||opt.print_keys)s+=line_to_str(keys);for(i=0;i<csv.length;i++){for(j=0,a=[];j<keys.length;j++){var v=csv[i][keys[j]];a.push(v===undefined?"":v)}s+=line_to_str(a)}return s};E.to_blob=function(csv,opt){return new Blob([E.to_str(csv,opt)],{type:"application/csv"})};return E})})();
define("/svc/cdn/pub/spark_analysis.js",["/svc/cdn/pub/util.js"],function(util){var E={},id="spark_analysis";function Spark_analysis(spark,conf,video_ctx){if(!(this instanceof Spark_analysis))return new Spark_analysis(spark,video_ctx);this.log=spark.log(id);this.conf=conf;this.video_ctx=video_ctx;this.spark=spark;this.attach_mode=spark.is_stats_only?"spark_stats":"spark";this.analysis_once=this.try_wrapper(this.analysis_once);this.snippets=util.conf_get(conf,"analysis.js",[]);if(this.snippets.length)this.analysis_int=setInterval(this.analysis_once,1e3)}Spark_analysis.prototype.is_snippet_enabled=function(s){return s.enable_stats&&this.attach_mode=="spark_stats"||!s.disable_spark&&this.attach_mode=="spark"};Spark_analysis.prototype.is_running=function(){return!!this.analysis_int};Spark_analysis.prototype.analysis_once=function(){var run=function(path,sid,stats_only){var ex;try{if(!util.conf_apply(this.conf,path,[],undefined,this.video_ctx,true)){return false}}catch(e){ex=e.message}util.log_report({exception:ex,id:"spark_analysis",add_log:!ex,video_ctx:this.video_ctx,analysis_tag:Object.assign({},util.conf_get(this.conf,"analysis.tag"),{sid:sid}),extra:{stats:this.spark.stats.get_stats()},zone:this.spark.zone});this.log.debug_spark("sending log report %s",sid);return true}.bind(this);var snippets_left=0;this.snippets.forEach(function(s,i){if(!this.is_snippet_enabled(s)||s.mode!="multi"&&s.count>0)return;if(run("analysis.js."+i+".js",s.id))s.count=s.count+1||1;snippets_left++},this);if(!snippets_left)this.analysis_int=clearInterval(this.analysis_int)};Spark_analysis.prototype.checkpoint=function(){if(this.is_running())this.analysis_once()};Spark_analysis.prototype.try_wrapper=function(func){return util.try_wrapper({prefix:"spark_analysis"},func.bind(this))};Spark_analysis.prototype.uninit=function(){this.checkpoint();this.analysis_int=clearInterval(this.analysis_int)};E.Spark_analysis=Spark_analysis;return E});
define("/svc/cdn/pub/heatmap.js",["/svc/cdn/pub/log.js","cash","/svc/cdn/pub/util.js","/util/util.js","/util/etask.js"],function(_log,$,util,zutil,etask){var E={};var id="heatmap",log;var perr=util.cdn_perr;var assign=Object.assign;E.default_conf={map_color:"rgba(60, 60, 60, 0.6)",fill_color:"rgba(210, 210, 100, 0.6)",height_percent:10,min_height:15,min_views:10};function Viewer(heatmap){this.heatmap=heatmap;this.player=heatmap.player;this.spark_player=heatmap.spark_player;var data=heatmap.heatmap_data;if(data.total<E.conf.min_views)return log.err("to little views");this.container=this.get_container();if(!this.container||!this.container.length)return log.err("no container");if(this.native_controls)return;this.progress_bar=this.get_progress_bar();if(!this.progress_bar||!this.progress_bar.length)return log.debug_spark("progress bar is required");this.wrapper_div=$("<div>");var points=[];var size=this.get_size();var height_percent=E.conf.height_percent/100;var height=this.container.height()*height_percent;var width=size.width;var x_points=Math.ceil(data.dur/data.interval)||data.view_sec.length;var x_diff=width/(x_points-1);var max=data.view_sec.reduce(function(a,b){return Math.max(a,b)},0);var y_diff=height/max;for(var i=0;i<x_points;i++)points.push(i*x_diff+","+(height-(data.view_sec[i]||0)*y_diff));points.push(width+","+height);points.push("0,"+height);var map_color=E.conf.map_color||"rgba(33, 33, 33, 0.6)";var fill_color=E.conf.fill_color||"yellow";var svg=$('<svg viewBox="0 0 '+width+" "+height+'" '+'preserveAspectRatio="none"><defs>'+'<clipPath id="spark_heatmap" clipPathUnits="userSpaceOnUse"><polygon'+' points="'+points.join(" ")+'"></polygon></clipPath></defs><g '+'clip-path="url(#spark_heatmap)"><rect class="spark_heatmap_bg" '+'x="0" y="0" width="100%" height="100%" fill="'+map_color+'"></rect>'+'<rect class="spark_heatmap_fill" x="0" y="0" width="100%" '+'height="100%" fill="'+fill_color+'" style="transform: '+'translate3d(100%, 0px, 0px);"></rect></g></svg>');svg.appendTo(this.wrapper_div);this.wrapper_div.appendTo(this.progress_bar);this.set_size(assign({height:height},size));this.watch_resize();this.inited=true}Viewer.prototype.get_container=function(){var container=$(this.player.get_ui().fullscreen);if(this.player.html5&&(this.player.html5.controls&&container[0]==this.player.html5||E.conf.native_controls)){this.native_controls=1;var wrapper=container.closest(".hola_video_wrapper");if(wrapper.length)return wrapper;log.info("wrap native html5 player");wrapper=$(document.createElement("div"));var position=container.css("position");wrapper.insertAfter(container).css({position:position=="static"?"relative":position,"z-index":container.css("z-index"),width:container.width()+"px",height:container.height()+"px",margin:container.css("margin")}).addClass("hola_video_wrapper");container.appendTo(wrapper);return wrapper}return container};Viewer.prototype.get_size=function(){var c_width=this.c_width=this.container.width();var bar_width=this.progress_bar.width();var ret={width:bar_width||c_width,left_padding:0,top_padding:0};if(this.player.name=="hola")ret.width=bar_width||c_width*.965;else if(this.player.vjs){ret.width=bar_width?bar_width*.95:c_width*.63;ret.left_padding=.03*ret.width}else if(this.player.jwplayer){ret.width=(bar_width||c_width)-24;ret.left_padding=12;ret.top_padding=7}return ret};Viewer.prototype.set_size=function(size){var display="";if(size.height<E.conf.min_height){display="none";log.notice("to small player - hidding")}this.current_size=size;this.wrapper_div.css({position:"absolute",height:size.height+"px",width:size.width+"px",top:-size.height+size.top_padding+"px",left:size.left_padding+"px",background:"none",display:display})};Viewer.prototype.resize=function(){var size=this.get_size();size.height=this.current_size.height/this.current_size.width*size.width;this.set_size(size)};Viewer.prototype.update_pos=function(pos){if(!this.inited)return;var left=100-pos/this.player.get_duration()*100;this.wrapper_div.