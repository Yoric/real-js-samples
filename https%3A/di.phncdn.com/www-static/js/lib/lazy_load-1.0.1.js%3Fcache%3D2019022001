var LazyLoadImage=function(){"use strict";var a=this;a.init=function(r){a.params=r,a.replaceImage(),a.replaceBackground(),a.replace_section()},a.replaceImage=function(){void 0!==a.params&&void 0!==a.params.class_name&&void 0!==a.params.class_name&&jQuery("."+a.params.class_name).each(function(){var r=jQuery(this).data(a.params.dataSrc);jQuery(this).attr("src",r)})},a.replaceBackground=function(){void 0!==a.params&&void 0!==a.params.classBkg&&a.params.dataBkg&&jQuery("."+a.params.classBkg).each(function(){var r=jQuery(this).data(a.params.dataBkg);jQuery(this).css("background-image",'url("'+r+'")')})},a.replace_section=function(){if(void 0!==a.params&&void 0!==a.params.sections&&void 0!==a.params.thumbUrl)for(var r=0,e=a.params.sections.length;r<e;r++)jQuery("#"+a.params.sections[r]+" li img").each(function(){jQuery(this).attr("src",jQuery(this).data(a.params.thumbUrl))})}};if("function"==typeof jQuery(document).ready)jQuery(document).ready(function(){(new LazyLoadImage).init(page_params.lazyLoad)});else{var myRawLazy=new LazyLoadImage;myRawLazy.init(page_params.lazyLoad)}