==""){return false}if(a.trim(c)==""){return false}return(a.date.add(a.date.createBy(e,b),d,f,b)>=c)};a.date.compare=function(d,c,b){b=(b)||"YMD";if(a.trim(d)==""){return false}if(a.trim(c)==""){return false}tempFirst=a.date.createBy(d,b);tempSecond=a.date.createBy(c,b);if(tempFirst>tempSecond){return 1}else{if(tempFirst<tempSecond){return -1}else{return 0}}};a.fn.date=function(b,c){return a(this).val(a.date(b,c))};a.fn.createDate=function(b){b=(b||"YMD");return a.date.createBy(a(this).val(),b)};a.fn.addDate=function(c,d,b){b=(b||"YMD");return a(this).val(a.date.add(a(this).createDate(b),c,d,b))};a.fn.between=function(c,d,e,b){return a.date.between(a(this).val(),a(c).val(),d,e,b)};a.fn.compare=function(d,c,b){alert(123);return a.date.compare(d,c,b)}})(jQuery);