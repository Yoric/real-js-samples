(function(a){a.xss=function(f,e){e=(e||[]);for(var d=0,b=e.length;d<b;d++){f=f.replace("<"+e[d]+">","##"+e[d]+"##");f=f.replace("<"+e[d]+"/>","@@"+e[d]+"@@")}f=f.replace(/"/g,"&#34;");f=f.replace(/'/g,"&#39;");f=f.replace(/</g,"&#60;");f=f.replace(/>/g,"&#62;");f=f.replace(/javascript\:(.*)/g,"");f=f.replace(/script/g,"");for(var c=0,b=e.length;c<b;c++){f=f.replace("##"+e[c]+"##","<"+e[c]+">");f=f.replace("@@"+e[c]+"@@","<"+e[c]+"/>")}return f};a.xssRe=function(f,e){e=(e||[]);for(var d=0,b=e.length;d<b;d++){f=f.replace("<"+e[d]+">","##"+e[d]+"##");f=f.replace("<"+e[d]+"/>","@@"+e[d]+"@@")}f=f.replace(/&#34;/g,'"');f=f.replace(/&#39;/g,"'");f=f.replace(/&#38;/g,"&");f=f.replace(/&#60;/g,"<");f=f.replace(/&#62;/g,">");f=f.replace(/&quot;/g,'"');f=f.replace(/&apos;/g,"'");f=f.replace(/&amp;/g,"&");f=f.replace(/&lt;/g,"<");f=f.replace(/&gt;/g,">");for(var c=0,b=e.length;c<b;c++){f=f.replace("##"+e[c]+"##","<"+e[c]+">");f=f.replace("@@"+e[c]+"@@","<"+e[c]+"/>")}return f};a.removeTag=function(b){return b};a.fn.xss=function(b){b=(b||[]);return a.xss(a(this).val(),b)};a.fn.xssRe=function(b){b=(b||[]);return a.xssRe(a(this).val(),b)};a.fn.removeTag=function(){return a.removeTag(a(this).val())}})(jQuery);