			$(document).keydown(function(event) {
					
				if(event.keyCode==27) {
					if($('#dropdown_cntry').hasClass('open')) {
						$('#dropdown_cntry').removeClass('open');
						$('#btn_cntry').focus();
					}
					else if($('#oly_2').length) {
						$("#close").click();
					} else if ($('#videooverlay').is(':visible')) {	
						$("#clsvd").click();
					} else if ($('#faqdeskoly').length) {	
						$("#clsfaq").click();
					} else if ($('#faqoverlay').length) {
						$("#faqclose").click();
					} 
					if ($('#srvydeskolyin').attr("aria-hidden") == "false") {
						$("#clssrvy").click();
					} 
				}
				inactivityPeriod = 0;
			});
			
			
			
			
			
			function increment () {
			
				
				
				if(thresholdInactivityPeriod <=0) {
					return;
				}
				if (!$("#chatpopupovr").hasClass("hdn")) {
					inactivityPeriod = 0;
					return;
				}
				inactivityPeriod = inactivityPeriod + inactivityStep;
				if(inactivityPeriod > thresholdInactivityPeriod*1000) {
					inactivityPeriod = 0;
					var channelslist =$(".chnls"); 
					var isActive=false;
					$.each(channelslist,function(index,obj) {
						  if($(obj).hasClass("sel") ) { 
								isActive = true;
						  }  
					});
					if(isActive || ($('#mask').length > 0)) {
						
						return;
					}
					if ($(window).width() > 767 && !isProactiveDisplayed) {
						clearInterval(timerId);
						setProactiveDisplayed(true);
						focusedElement= $("*:focus");
						callProactiveInflowHelp();
					}	
				}
			}
			
			function getChannelNumber (channel, channelRecords) {
				
				var channelNumber = 0;
				for (var i in channelRecords) {
					
					if(channelRecords[i].channelType === "CALL_US") {
						channelNumber = channelNumber + 1; 
						if(channel.toLowerCase() === "callus") {
							if(channelRecords[i].available) {
								return (channelNumber);
							} 
							return -1;
						}
			
					} else if(channelRecords[i].channelType === "CALL_ME") {
						channelNumber = channelNumber + 1;
						if(channel.toLowerCase() === "callme") {
							
							if(channelRecords[i].available) {
								return (channelNumber);
							} 
							return -1;
						}
			
					}
					
					else if(channelRecords[i].channelType === "LIVE_HELP" || channelRecords[i].channelType === "CHAT") {
						channelNumber = channelNumber + 1;
						if(channel.toLowerCase() === "chat") {
							
							if(channelRecords[i].available) {
								return (channelNumber);
							} 
							return -1;
						}
			
					}
				}
					
				return -1;	
			}
			
			
			function openProactiveChatOvly(preferredChannelRecords, channelRecords,pageId, siteId) {
				var channel = "";
				var isBoltDe = (pageId == 2380506 || pageId == 2491901) && (siteId == 77 || siteId == 101);
				
				var otherNorbPages = ((!(pageId == 2380506 || pageId == 2491901)) && (siteId == 0 || siteId == 3 || siteId == 77) && (segmentid == 10010 || segmentid == 10011));

				var onBordingPages = (pageId == 2543372 || pageId == 2543374 || pageId == 2543376 || pageId == 2543383) && (siteId == 0);

				var onKYCPages = (pageId == 2538350 || pageId == 2543385 || 
						pageId == 2543375 || pageId == 2543377 || pageId == 2544426 || 
						pageId == 2544428 || pageId == 2539918 || pageId == 2539916 || 
						pageId == 2539915 || pageId == 2544677 || pageId == 2544678 || 
						pageId == 2544680 || pageId == 2544679) && (siteId == 0);
		
				var onXOPages = (pageId == 2507978 || pageId == 2368479 || pageId == 2500857) && (siteId == 0);
				
				if (isBoltDe || otherNorbPages || onBordingPages || onKYCPages || onXOPages) {     
					
					$("#chatpopupdeskovr").remove();
					$("#chatpopupovr").addClass('cstm');
					$("#chatotcstm").text(contentJsonObj.inflowcomponent.custHeading);
					$("#chatwuscstm").text(contentJsonObj.inflowcomponent.custChat);
					$("#calluscstm").text(contentJsonObj.inflowcomponent.custCallUs);
					$("#callmecstm").text(contentJsonObj.inflowcomponent.custCallMe);

					if(otherNorbPages && siteId == 77 && contentJsonObj.inflowcomponent.custMsgNorb && contentJsonObj.inflowcomponent.custHeadingNorb){
						$(".promsg").text(contentJsonObj.inflowcomponent.custMsgNorb);
						$("#chatotcstm").text(contentJsonObj.inflowcomponent.custHeadingNorb);
					} else {
						$(".promsg").text(contentJsonObj.inflowcomponent.custMsg);
					}
						

					if (siteId == 77 || siteId == 101) {
						$("#agent-gl").css({"display":"none"});
					}else {
						$("#agent-de").css({"display":"none"});
						$("#chatdesc").css({"margin-bottom": "16px"});
					}
					
					for (var i=0;i<preferredChannelRecords.length;i++) {
						channel = preferredChannelRecords[i].value;
						var channelNumber = getChannelNumber(channel, channelRecords);
						if (channelNumber!=-1) {
							$("#clsfaq").click();
							$("#chatpopupovr").removeClass("hdn").addClass("shw");
							var proactiveMessage = $("#chatotcstm").text() +  $(".promsg").text() + $("#chatwuscstm").text() + $("#callmecstm").text();
							if ($("#proactiveAlert").length == 0) {
								$('<p id="proactiveAlert" class="infclipped" role="alert">'+ proactiveMessage + '</p>').appendTo("#inflowcontainer");
							}
							positionProactiveChatOverlay(0);
							if(channel.toLowerCase() === "callus") {
								$("#calluscstm").removeClass("hdn").addClass('shw');
							} else if(channel.toLowerCase() === "callme") {
								$("#callmecstm").removeClass("hdn").addClass('shw');
							} else  if (channel.toLowerCase() === "chat") {
								$("#chatwuscstm").removeClass("hdn").addClass('shw');
							}
						}
					}
					
				} else {
					$("#chatpopupdeskovrcstm").remove();
					channel = preferredChannelRecords[0].value;
				
					var top = 50;
					var channelNumber = getChannelNumber(channel, channelRecords);
				
					if(channelNumber < 0 ) {
					
						return;
					}
					top = top * channelNumber ;
					if($("#chatpopupovr").hasClass("hdn")) {
					
						localStorage.setItem(proactiveCountPropertyName, Number(proactiveCount)+1);
						if(channel.toLowerCase()=== "chat")  {
							$("#chatwus").text(contentJsonObj.inflowcomponent.chatwithus);
							$("#chatwus").attr("channelid","i-chat");
						} else if(channel.toLowerCase() === "callus")  {
							$("#chatwus").text(contentJsonObj.inflowcomponent.letstalk);
							$("#chatwus").attr("channelid","i-callus");
						} else if(channel.toLowerCase() === "callme")  {
							$("#chatwus").text(contentJsonObj.inflowcomponent.letstalk);
							$("#chatwus").attr("channelid","i-callme");
						}
						$("#clsfaq").click();
						if (options.proactiveMsg) {
							$("#chatot").html(options.proactiveMsg);
						} else {
							$("#chatot").text(contentJsonObj.inflowcomponent.needsomehelp);
						}
						if (options.proactiveLinkMsg) {
							$("#chatwus").html(options.proactiveLinkMsg);
						}
						$("#chatpopupovr").removeClass("hdn").addClass("shw");
						positionProactiveChatOverlay(top);
						var proactiveMessage = $("#chatot").text() +  $("#chatwus").text();
						positionProactiveChatOverlay(top);
						if ($("#proactiveAlert").length == 0) {
							$('<p id="proactiveAlert" class="infclipped" role="alert">'+ proactiveMessage + '</p>').appendTo("#inflowcontainer");
						}
					}	
				}
			
			
			}
			
			
			parentEl.on("click", '.chnls', function() {
				
				closeProactive(true);
				
			});
			
				
				function positionProactiveChatOverlay(top) {
					
					if ($("#chatpopupovr").hasClass('cstm')) {
						var rightPos = 50;
						var toppos= options.posTop;
						$("#chatpopupdeskovrcstm").attr("style", "");
						$("#chatpopupdeskovrcstm").css({
							"right" : (Number(options.posRight) + rightPos) + 'px',
							"top" : toppos + 'px',
							"position" : "fixed",
						});
						
					} else {
						
						$("#chatpopupovr").attr("style", "");
						$("#chatpopupovr").css({
							"right" : (Number(options.posRight) + 315) + 'px',
							"top" : (Number(options.posTop) + top) + 'px',
							"position" : "fixed"
						});
					}
					
				}
			
				
			
			$("#chatwus").on("click", function (event) {
				var channelId = $("#chatwus").attr("channelid");
				$("#"+channelId).click();
			});
			
			$(".chncstm").on("click", function (event) {
				var channelId = $(this).attr("channelid");
				$("#"+channelId).click();
			});
			
			
			
			
			$("#clschatpopup").on("click", function (event) {
				closeProactive(false);
			});
			
			
			$("#clschatpopupcstm").on("click", function (event) {
				
				closeProactive(false);
				
			});
			
			function closeProactive (isChannelClicked) {
				inactivityPeriod = 0;
				 
				 
				if(!$("#chatpopupovr").hasClass("hdn")) {
					setProactiveDisplayed(false);
					$("#chatpopupovr").remove();
					$("#proactiveAlert").remove();
				} else {
					$("#chatpopupovr").addClass("hdn").removeClass("shw");
				}	
			}			
			
			function callProactiveInflowHelp () {
				
				var serviceArguments = getInflowParameterURL(sectionName,topicid,contextid,true,errorCode, errorMessage, errorString, checkoutSessionId);
				callInflowService(serviceArguments,false);
				
			}
			

			function closeInflowHelp () {
				
				$('body').removeClass("noscroll");
				if (window.history && window.history.pushState) {
					if (history.state == 'video2') {
						window.history.go(-3);
					} else {
						window.history.go(-2);
					}	
				}	  
				
					closeinflowoverlays();
					var faqovly = $('#faqoverlay');
					
					if (faqovly.is(':visible')) {
						$('.content').addClass('hdn');
						$('#faqoverlay').addClass('hdn');
						 setTimeout(function() {
								$("#mfaqclk").focus();
							},100);
						
						
					}
					
					
					$.when($("#callusoverlay").remove()).then((function() {
						$('#mask').remove();
					})());
					var srvyovly = $('#surveyovly');
					
					if (srvyovly.is(':visible')) {
						
						$('#surveyovly').addClass('hdn');
						 setTimeout(function() {
								$("#mfaqclk").focus();
							},100);
					}
					var faqsecovly = $('#faqsecoverlay');
					
					if (faqsecovly.is(':visible')) {
						
						$('#faqsecoverlay').addClass('hdn');
						 setTimeout(function() {
								$("#mfaqclk").focus();
							},100);
					}
			}
			

			ocsinflowoverlays.on("click", '#cuphoneno', function() {
				if ($("#deviceType").val() == "DEVICE_PHONE") {
					var phoneNumber = $("#phno").val();
					window.location.href = "tel:"+phoneNumber;
				}	
				
			});

			
			parentEl.on("click", '#mclssecinf', function(event) {
				closeInflowHelp();
			});
			ocsinflowoverlays.on("click", '#mvcancel', function(event) {
				closeInflowHelp();
			});
			
			ocsinflowoverlays.on("click", '#mclscallme', function(event) {
				closeInflowHelp();
			});
			
			ocsinflowoverlays.on("click", '#mclscallus', function(event) {
				closeInflowHelp();
			});
			
			
			
			$(parentEl).on("click",'#callmechn', function (event) {
				
				if (window.history && window.history.pushState) {
					history.replaceState("selcallme", "selcallme", "selcallme");
					history.pushState("callme", "callme", "callme");
				}
				showSpinnerMobile();
				callme(callmemobileTemplate,true);
				
			});
			
			
			$(parentEl).on("click",'#chtchn', function (event) {
				
				openChatOnclick("#chtchn");
				
			});
			
			
			$(parentEl).on("click",'#calluschn', function (event) {
				
				if (window.history && window.history.pushState) {
					history.replaceState("selcallus", "selcallus", "selcallus");
					history.pushState("callus", "callus", "callus");
				}
				showSpinnerMobile();
				callus(callusmobileTemplate,true);
				
			});
			
			
			
			 if (options.hideOnScroll == "true" || options.hideOnScroll == true) { 
			
				 $(document).on("scroll",function(){	
					 
					 
					 
					 if ($(window).scrollTop() == 0 && (mobilePlaceHolderEl.length == 0 || ($(document).width() > 767 && ($("#mobileinflow:hidden"))))) {
						 
					                                                                                                                                                                                                                                                                                                          
						 $("#ocs-inflow").addClass ("shw").addClass("ovrhdn").removeClass("hdn");
					 
					 }	 else {
						 
							if (!($ ('#faqdeskovr').hasClass("shw") || $ ("#srvydeskovr").hasClass("shw") || isSurveyProgress) ) {
								
								$("#ocs-inflow").removeClass ("shw").removeClass("ovrhdn").addClass("hdn");
							} 
					 }
				 
				 });
			 }	 

			
			$(window).resize(function(event) {
				
				
				var winheight = $(window).height() - 70 - options.posTop;
				$("#faqdeskoly").css({
					"max-height" : winheight
				});
				
				$("#srvydeskolyin").css({
					"max-height" : winheight
				});
				
				
				
				if($("#callusoverlay").hasClass("mbovly")) {
					
					return;
				}
				
				var faqovly = $('#faqoverlay');
				
				if ($(window).width() > 967 && $("#ocsmobilenobar").val() != 'true' ) {
					$("#mclsinf").click();
					if (faqovly.is(':visible')) {
						document.body.style.overflow = 'visible';
						faqovly.find('.content').addClass('hdn').appendTo('#faqenclose');
						$(faqovly).addClass('hdn');
						$("#" + options.mobileid).css({

							"top" : '',
							"position" : '',
							"width" : ''

						});
						if (typeof options.mobileshowcallback == 'function') {
							options.mobilehidecallback.call();
						}
						$('#i-faq').addClass("sel");
						$("#faqdeskovr").removeClass("hdn").addClass("shw");
					}
					
				} else {

					
					if ($(document).width() < 767) { 
						if ($('#faqdeskovr').is(':visible')) {
							hideFaqVideos();
						}
					}	
					

				}
				
				
				if ($("#callusoverlay").is(':visible')) {
						setPosition();
				}
				
				if ($('#videooverlay').is(':visible')) {
					if (!$('#videooverlay').hasClass("mobile")) {
						positionVideo();
					} else {
						
						positionMobileVideo();
						
					}
				}

				
				if (mobilePlaceHolderEl.length == 0) {
					if ($("#ocs-inflow").hasClass ("shw")) {
						
						$("#ocs-inflow").addClass("ovrhdn");
					}
					if ($("#faqdeskovr").hasClass("shw")) {
						
						$("#faqdeskovr").addClass("ovrhdn");
					}
					if($("#srvydeskovr").hasClass("shw")) {
						
						$("#srvydeskovr").addClass("ovrhdn");
					}
					
				} else {
					$("#ocs-inflow").removeClass("ovrhdn");
					$("#faqdeskovr").removeClass("ovrhdn");
					$("#srvydeskovr").removeClass("ovrhdn");
				}
				

			});
			
			var serviceArguments = getInflowParameterURL(options.sectionId);
			
			serviceArguments = serviceArguments + "&customId=" + "inactivity";
			
			if (options.serviceResponse != null) {
				infscallback (options.serviceResponse);
			} else {
			
				callInflowService(serviceArguments,false);
			}	
		
		});				
	};

})(jQuery); 
(function($){
 
    $.fn.dragger = function(settings){

        var opts = $.extend({}, $.fn.dragger.defaults, settings);
        return this.each(function(settings){
           
           var options = $.extend({}, opts, $(this).data());
           var temp = $(this);
           var target = opts.target;
           var callback = opts.callback;
           var clientTop;
           var clientLeft;
           var dragstart = 0;
           
           
           $(this).mousedown(function (event) {
				dragstart = 1;
				var offset = target.offset();
				clientTop = event.clientY - offset.top;
				clientLeft = event.clientX - offset.left;
				
			 });
           
           $(document).mousemove(function (event) {
        	   if(dragstart) {
        		   
        		   if(callback)callback.fire();
        		   target.css("opacity",0.7);
        		   var offset = target.offset();
        		   target.offset({top:event.clientY - clientTop,left:event.clientX - clientLeft});
        	   }   
			 });
           
           $(document).mouseup(function (event) {
				dragstart = 0;
				var offset = target.offset();
				target.css("opacity",1);
				 
			 });
			
        });
    };
    
 
})(jQuery);
