				showSpinnerMobile();
				if (window.history && window.history.pushState) {
					history.replaceState("faqsecclick", "faqsecclick", "faqsecclick");
					history.pushState("faqsecopen", "faqsecopen", "faqsecopen");
				}
				handleSelect($(this).find("a").attr("inflow"),true);
				
				
			});

			
			function openMobileFaqSectionOverlay() {
				
				var faqsecoverlay = $('#faqsecoverlay');
				$("#mclssecinf").html(contentJsonObj.inflowcomponent.cancel);
				$("#fsittl").html(contentJsonObj.inflowcomponent.faq);
				faqsecoverlay.removeClass('hdn');
				var width = $(window).width();
				faqsecoverlay.css({
					"top" : 0,
					"position" : 'fixed',
					"left" : width
				});
				
				$('#faqsecoverlay').animate({'left': '0px'}, 500);
				setTimeout(function() {
					$.trapKeyboard('#faqsecoverlay');
					$("#mclssecinf").focus();
				},1000);

			}
			
			

			$('div[inflow]').on("click", function(event) {
				if (propagatedEvent == true) {
					propagatedEvent = false;
					return;
				}
				if ($(this).parents('div[inflow]').length > 0) {
					propagatedEvent = true;
				}
				handleSelect($(this).attr("inflow"),false);
				
				//event.stopPropagation();
			});

			
			
			ocsinflowoverlays.on("click", '#btn_cntry', function(event) {
				if(closedialog == true) {
					closedialog = false;
					return;
				}
				$('#dropdown_cntry').addClass('open');
				$( "ul#options_cntry li").removeClass('ddselect');
				event.stopPropagation();

			});
			
			ocsinflowoverlays.on("keydown", '#btn_cntry', function(event) {
				if($('#dropdown_cntry').hasClass('open')){
					return;
				}
				if(event.keyCode == 40 ) {
					$( "ul#options_cntry li").removeClass('ddselect');
					var listElement = $( "ul#options_cntry li:nth-child(1)");
					$('#dropdown_cntry').addClass('open');
					listElement.focus();
					listElement.addClass('ddselect');
					event.stopPropagation();
					
				}
				else if (event.keyCode==13) {
					 event.preventDefault();
				}

			});

			

			$(document).click(function() {
				$('#dropdown_cntry').removeClass('open');
				inactivityPeriod = 0;
			});

			
			ocsinflowoverlays.on('click touchstart', 'ul#options_cntry li', function(event) {

				$('#span_cntry').text($(event.target).text());

				selectedCountry = $(event.target).attr("op-value");
				$('.landline').text($(event.target).attr("landLineTip"));
				$('.mobile').text($(event.target).attr("cellTip"));
			});

			ocsinflowoverlays.on('change', '#sel_cntry', function(event) {
				selectedCountry = $( this ).val();
				$('.landline').text($(this.options[this.selectedIndex]).attr("landLineTip"));
				$('.mobile').text($(this.options[this.selectedIndex]).attr("cellTip"));
			});
						
			ocsinflowoverlays.on('keydown', 'ul#options_cntry li', function(event) {
				
				var listElement = null;
				if(event.keyCode == 40 ) {
					setTimeout(function() {
						var curIndex = $('ul#options_cntry li').index(event.target) +1 ;
						$( "ul#options_cntry li:nth-child("+ curIndex +")").removeClass('ddselect');
						
						if($(event.target).is(":last-child")) {
							
							listElement = $( "ul#options_cntry li:nth-child(1)");
						} else {
							curIndex++;
							listElement = $( "ul#options_cntry li:nth-child("+ curIndex +")");
						}	
						listElement.focus();
						listElement.addClass('ddselect');
					}, 300);
					event.preventDefault();
					event.stopPropagation();
					
					return;
					
					
				}	
				else if(event.keyCode == 38 ) {
					var listElement = null;
					setTimeout(function() {
						var curIndex = $('ul#options_cntry li').index(event.target);
						curIndex++;
						$( "ul#options_cntry li:nth-child("+ curIndex +")").removeClass('ddselect');
						curIndex--;
						if($(event.target).is(":first-child")) {
							listElement = $( "ul#options_cntry li:last-child");
						} else {
							listElement = $( "ul#options_cntry li:nth-child("+ curIndex +")");
						}	
						listElement.focus();
						listElement.addClass('ddselect');
					},300);	
					event.preventDefault();
					event.stopPropagation();
					return;
					
					
				}
				else if (event.keyCode==13) {
					var indexElement = event.target;
					closedialog = true;
					$('#span_cntry').text($(indexElement).text());
					selectedCountry = $(indexElement).attr("op-value");
					$('.landline').text($(indexElement).attr("landLineTip"));
					$('.mobile').text($(indexElement).attr("cellTip"));
					$('#dropdown_cntry').removeClass('open');
					$(indexElement).removeClass('ddselect');
					$('#btn_cntry').focus();
					
				}	
				
				
				
			});
			
			
			function checkSurveyAnswered() {
				

				var isAnswered = false;
				var questions = currentSurvey.surveyContent.survey.questions;
				
				
			    for (i=0;i<questions.length;i++) 
			    {
			    	var type = questions[i].type;
			    	var questionid = questions[i].qid;
			    	questionid = "_" + questionid;
			    	if (type === "single-select" || type === "ratings") {
			    		if($('input[name='+questionid+']:checked').val() !== undefined)
			    			 isAnswered = true;
			    	} else if (type === "multiple-select") {
			    		 $.each($('.' + questionid +  ':checked'), function(){
			    			 isAnswered = true;
				         });
			    	}
			    	
			    	else if (type === "textarea" || type === "text") {
			    		var value = $('#' + questionid).val();
			    		var maxlength = $('#' + questionid).attr("maxlength");
			    		var charleft =  maxlength - value.length;
			    		$('#max' + questionid).text(charleft);
			    		
			    		if (value != null && value != "undefined" && value != "") {
			    			 isAnswered = true;
			    			
			    		}
			    		
			    	}
			    }
			    if (isAnswered) {
			    	$("#sbtsurvey").removeAttr("disabled","false");
			    	$("#sbtsurvey").attr("aria-disabled","false");
			    	$("#sbtsurvey").removeClass("btnd");
			    } else {
			    	
			    	$("#sbtsurvey").attr("disabled","true");
			    	$("#sbtsurvey").attr("aria-disabled","true");
			    	$("#sbtsurvey").addClass("btnd");
			    }
			     
			} 
			
			
			ocsinflowoverlays.on("click", '#sbtcallme', function(event) {
				
				if ($('#sbtcallme').attr('sclicked') != 1) {
					$('#sbtcallme').attr('sclicked', "1");
					if (showConsent) {
						if ($("input:radio[name=oconsent]:checked").val() != 'yes' && $("input:radio[name=oconsent]:checked").val() != 'no') {
							$('#consenttext').addClass("redconsent");
							return;
						}
					}
					$('#consenttext').removeClass("redconsent");
					clickcallme(callmemobileTemplate);
					
				}
			});
			
			
