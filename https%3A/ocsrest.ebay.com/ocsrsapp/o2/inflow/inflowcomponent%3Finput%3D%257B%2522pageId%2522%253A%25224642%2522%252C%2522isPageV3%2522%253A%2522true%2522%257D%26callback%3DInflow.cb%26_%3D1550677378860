(function($) {
	
	
	$.fn.inflowcomponent = function(settings,parameters) {
		var windowWidth = $(window).width();

		
		$.fn.inflowcomponent.defaults = {
			posRight : '2',
			posTop : '100',
			mobileid : 'phoneSidebar',
			address : $("#ocsServiceUrl").val(),
			timeout : 60000,
			deviceId : windowWidth <= 580 ? 'DEVICE_PHONE' : windowWidth < 980 ? 'DEVICE_TABLET': 'DEVICE_DESKTOP'
		};
		
		
		
		
				 
			var opts = $.extend({}, $.fn.inflowcomponent.defaults, settings);
			if ($.browser && $.browser.msie && $.browser.version <= 8.0) {
				$("#phoneSidebar, #callmeclose , #" + opts.mobileid , "#" + opts.mobilePlaceHolder).hide();
				$("#oly_2").css("width", "460px");

			}
			
		
		_.templateSettings = {
			interpolate : /\[@=([\s\S]+?)@\]/g,
			evaluate : /\[@([\s\S]+?)@\]/g,
			escape : /\[@-([\s\S]+?)@\]/g
		};

		
		var focusedElement = null;
		
		
	return  this.each(function(settings) {
			
			
			
			
			
			
	            
			var isSurveyProgress = false;
			var parentEl = $(this);
			var isTouchDevice = is_touch_device();
			var ocsinflowoverlays = $('<div id="ocsinflowoverlays" class="span5"/>').appendTo(document.body);
			var options = $.extend({}, opts, $(this).data()), idletime = 0,  timestamp = new Date().getTime();
			var contextid = null, topicid = null, section = null, sectionName = null, scrolltop = null, showConsent = false, previousFocusId ="", currentVideoId= "", closedialog = false, selectedCountry = -1;
			var pageId = null;
			var phoneNo = null;
			var speechImpairedText = null;
			 
			var inflowParameterUrl = null;
			if ($("#inflowParameterUrl").length) {
				inflowParameterUrl = $("#inflowParameterUrl").val();
			} else {
				inflowParameterUrl = options.inflowParameterUrl;
			}	
			
			
			var mobileParentEl = $('#' + options.mobileid);
			
			var siteId = options.siteId;
			
			var mobilePlaceHolderEl = $('#' + options.mobilePlaceHolder);
			var couponPlaceHolderEl = $('#' + options.couponPlaceHolder);
			


			var contentJson = $("#inflow-template-content").html().replace("//<![CDATA[", "").replace("//]]>", "");
			var contentJsonObj = $.parseJSON(contentJson);

			var callMETemplate = getTemplate($("#callmetemplate")), callUSTemplate = getTemplate($("#callustemplate")),  videoTemplate = null, mvideoTemplate = null; 
			var chatTemplate = getTemplate($("#chattemplate"));
			var	mobileInflowTemplate = getTemplate($("#mobile-inflow-template"));
			var mobileInflowHelpOverLayTemplate = getTemplate($("#mobile-inflow-helpovr-template")); 
			var mobileInflowFaqOverLayTemplate = getTemplate($("#mobile-inflow-faqovr-template"));
			var mobileInflowSecOverLayTemplate = getTemplate($("#mobile-inflow-secovr-template"));
			var callmemobileTemplate =null;
			var callusmobileTemplate = null;
			var inflowInlineSrvyTemplate = null;
			
			var couponTemplate = null;
			
	
			var inflowInlineFaqsTemplate = null, inflowComponentTemplate = null;
			
			var inactivityStep = 500;
			var inactivityPeriod = 0;
			var thresholdInactivityPeriod = -1;
			var timerId = 0;
			var isProactiveDisplayed = false;
			var channelProviderCallMe = "NONE";
			var channelProviderCallUs = "NONE";
			var channelProviderChat = "NONE";
			var surveyId="none";
			var inProgress = false;
			var myLiveHelpWin;
			var delay1 = 0;
			var delay2 = 1200;
			var propagatedEvent = false;
			var proactiveCountPropertyName =  "inflow." + options.presentationUserId + "." + options.pageId +  ".proactivecount";
			var proactiveCount  = localStorage.getItem(proactiveCountPropertyName);
			var maxProactiveCount = -1;
			var errorCode, errorMessage, errorString, checkoutSessionId;
			if (proactiveCount == null || proactiveCount == undefined) {
				proactiveCount = 0;
			}
			
			if(document.location.protocol == "https:") {
				options.address = options.address.replace("http","https");
			}
			
			initializeTemplates();
			
		
			
			function initializeTemplates () {
				
			
				
				if ($("#inflow-inline-srvy-template").length != 0) {
					inflowInlineSrvyTemplate = getTemplate($("#inflow-inline-srvy-template"));
				}
				
				if ($("#videotemplate").length != 0) {
					videoTemplate = getTemplate($("#videotemplate"));
				}
				
				if ($("#mvideotemplate").length != 0) {
					mvideoTemplate = getTemplate($("#mvideotemplate"));
				}
				if ($("#inflow-template").length != 0) {
					inflowComponentTemplate = getTemplate($("#inflow-template"));
					//delay1= 1000;
					delay2=1200;
				} else if ($("#inflow-inline-template").length != 0) {
					inflowComponentTemplate = getTemplate($("#inflow-inline-template"));
					inflowInlineFaqsTemplate = getTemplate($("#inflow-inline-faqs-template"));
				}
				if ($("#callmemobiletemplate").length != 0) {
					
					callmemobileTemplate = getTemplate($("#callmemobiletemplate")); 
				}

				if ($("#inflow-coupon-template").length != 0) {
					
					couponTemplate = getTemplate($("#inflow-coupon-template")); 
				}

				if ($("#deviceType").val() == "DEVICE_PHONE") {
					if ($("#callusmobiletemplate").length != 0) {
					
						callusmobileTemplate = getTemplate($("#callusmobiletemplate"));
					}
				} else {	
					if ($("#callusmobilewithpintemplate").length != 0) {
					
						callusmobileTemplate = getTemplate($("#callusmobilewithpintemplate"));
					}
				}	
				
			}
			
			function getTemplate(templateElementObject) {
				var templateObjectHtml = $.trim(templateElementObject.html().replace("//<![CDATA[", "").replace("//]]>", ""));
				return _.template(templateObjectHtml);
			}
			
			
			
			
			
			var y = window.scrollY;
			window.addEventListener("orientationchange", function() {
					window.scrollTo(0,y);
			}, false);
				

			function setProactiveDisplayed(proactiveDisplayed) {
				
				isProactiveDisplayed = proactiveDisplayed;
				
			}
			
			
			function is_touch_device() {  
			  try {  
			    document.createEvent("TouchEvent");  
			    return true;  
			  } catch (e) {  
			    return false;  
			  }  
			}			

			var whoopcu = null, whoopcm = null, whoopchat = null;
			
			options.timeout = $("#ocsTimeOut").val();
			
			
			
			var timeoutduration = options.timeout;
			
			if (!options.timeout || options.timeout <= 0) {
				timeoutduration = 10000;
			}
			

			setInterval(function() {
				timestamp = new Date().getTime();
			}, 1200000);

			function hideFaqVideos() {
				positionFaqVideos();
				$('#i-faq').removeClass("sel");
				$("#faqdeskovr").removeClass("shw").removeClass("ovrhdn").addClass("hdn");

			}

			
			
			function hideDsktpSrvy() {
				positionFaqVideos();
				$('#i-srvy').removeClass("sel");
				$("#srvydeskovr").removeClass("shw").removeClass("ovrhdn").addClass("hdn");
				$("#srvydeskolyin").attr("aria-hidden", "true");

			}
			
			function isSafari () {

				 return (navigator.userAgent.indexOf("Safari") > -1);
					  
			}
			
			
			
			parentEl.on("click", '#i-faq', function() {
				closeSurveyAndFAQ();
				$("#i-srvy").removeClass("sel");
				if ($('#i-faq').hasClass('sel')) {
					
					hideFaqVideos();
				} else {
					previousFocusId = $('#i-faq');
					$('#i-faq').addClass("sel");
					$("#faqot").html(contentJsonObj.inflowcomponent.faq);
					$("#faqdeskovr").removeClass("hdn").addClass("shw");
					if ($(document).width() > 767 && mobilePlaceHolderEl.length == 0) {
						$("#faqdeskovr").addClass("ovrhdn");
					}
					setTimeout(function() {
						$.trapKeyboard('#faqdeskoly');
						$("#clsfaq").focus();
					},400);	
				}
				
			});
			
			parentEl.on("click", '#i-srvy', function() {
				
				closeSurveyAndFAQ();
				$('#i-faq').removeClass("sel");
				previousFocusId = $('#i-srvy');
				if ($('#i-srvy').hasClass('sel')) {
					$('#i-srvy').removeClass('sel');
					
					if ($("#srvydeskovr").hasClass("shw")) {
						hideDsktpSrvy();
					}
					
				} else {
					startSurvey(true);
						
				}

			});

			parentEl.on("click", '#clsfaq', function() {
				positionFaqVideos();
				hideFaqVideos();
				$(previousFocusId).focus();
				
			});
			
			parentEl.on("click", '#clssrvy', function() {
				hideDsktpSrvy();
				setTimeout(function() {
					$(previousFocusId).focus();
				},400);	

			});

			$("body").on("click", '#mask', function() {
				if ($("#clsvd").is(':visible')) {
					$("#clsvd").focus();
				} else if ($("#close").is(':visible')) {
					$("#close").focus();
				}
			});

			function positionFaqVideos() {
				$("#faqdeskoly").attr("style", "");
				var rightPos = 50;
				/*if (options.inflowHelpMin) {
					rightPos = 30;
				}*/
				
				
					
				var toppos= options.posTop;
				var winheight = $(window).height() - 70 - toppos;
				$("#faqdeskoly").css({
					"right" : (Number(options.posRight) + rightPos) + 'px',
					"top" : toppos + 'px',
					"position" : "fixed",
					"max-height" : winheight
				});
			}
			
			function positionSrvy() {
				$("#srvydeskolyin").attr("style", "");
				var rightPos = 50;
				/*if (options.inflowHelpMin) {
					rightPos = 30;
				}*/
				
				var winheight = $(window).height() - 70 - options.posTop;
				
				$("#srvydeskolyin").css({
					"right" : (Number(options.posRight) + rightPos) + 'px',
					"top" : options.posTop + 'px',
					"position" : "fixed",
					"max-height" : winheight
				});
				
			}

			
			$.fn.refreshInflowOnError  = function (sectionId, topicId, contextId, isInactive, errorCode, errorMessage, errorString, checkoutSessionId) {
				
				var params = getInflowParameterURL (sectionId, topicId, contextId, isInactive, errorCode, errorMessage, errorString, checkoutSessionId);
				callInflowService(params,false);
				
			};

			$.fn.refreshInflow  = function (jsonString) {
				
				var params = getInflowParameterURLFromJSON (jsonString);
				callInflowService(params,false);
				
			};
			
			function getQueryParameters(str) {
				 return (str || document.location.search).replace(/(^\?)/,'').split("&").map(function(n){return n = n.split("="),this[n[0]] = n[1],this}.bind({}))[0];
			}
			
			 function getInflowParameterURLFromJSON (jsonString) {

				 var paramObject = getQueryParameters(inflowParameterUrl.substring(0, 1) === '&' ? inflowParameterUrl.substring(1) : inflowParameterUrl);
					
				 if (paramObject.deviceId === undefined && options.deviceId)
				     paramObject.deviceId = options.deviceId;
				 if (options.pageId)
				     paramObject.pageId = options.pageId;
				 if (options.createdBy)
				     paramObject.createdBy = options.createdBy;
				 if (options.errorId)
				     paramObject.errorId = options.errorId;
				 if (options.clientId)
				     paramObject.clientId = options.clientId;
				 if (options.userSegmentation)
				     paramObject.userSegmentation = options.userSegmentation;
				 if (options.categoryId)
				     paramObject.categoryId = options.categoryId;
				 if (options.partId)
				     paramObject.partId = options.partId;
	
				 if (options.trackParameters){
					 for (var key in options.trackParameters) {
						 paramObject[key] = options.trackParameters[key];
					 }
				 }
				 
				 try {
					 jsonString = JSON.parse(jsonString);
				     for (var key1 in jsonString) {
				    	 if (jsonString.hasOwnProperty(key1)) {
				    		 paramObject[key1] = jsonString[key1];
			    	    }
				     }
				 }catch( e ){
					 
				 }
				 
				 var newURL = "";
	
				 for (var key1 in paramObject) {
				     newURL += "&" + key1 + "=" + paramObject[key1];
				 }
								 
				return newURL;
			}
			 function getInflowParameterURL (sectionId, topicId, contextId, isInactive, _errorCode, _errorMessage, _errorString, _checkoutSessionId) {
//				var URL = inflowParameterUrl;
//				
//				if(URL.indexOf("deviceId")<0) {
//					if (options.deviceId) {
//						
//						URL = URL + "&deviceId=" + options.deviceId;
//					}	
//				}
//				if (options.pageId)
//					URL = URL + "&pageId=" + options.pageId;
//				if (options.createdBy)
//					URL = URL + "&createdBy=" + options.createdBy;
//				if (options.errorId)
//					URL = URL + "&errorId=" + options.errorId;
//				if (options.clientId)
//					URL = URL + "&clientId=" + options.clientId;
//				if (options.userSegmentation)
//					URL = URL + "&userSegmentation=" + options.userSegmentation;
//				if (contextId)
//					URL = URL + "&contextid=" + contextId;
//				if (sectionId)
//					URL = URL + "&sectionId=" + sectionId;
//				if(isInactive)
//					URL = URL + "&isInactive=" + isInactive;
//				if (topicId)
//					URL = URL + "&tp=" + topicId;
//				if (options.categoryId)
//					URL = URL + "&categoryId=" + options.categoryId;
//				if (options.partId)
//					URL = URL + "&partId=" + options.partId;
//				if(options.trackParameters) {
//					for (var key in options.trackParameters) {
//						URL = URL + "&" + key +  "=" + options.trackParameters[key];
//					}
//				}
//				if(errorCode) {
//					URL = URL + "&errorCode=" + errorCode;
//				}
//				
//				if(errorMessage) {
//					URL = URL + "&errorMessage=" + errorMessage;
//				}
//				
//				if(errorString) {
//					URL = URL + "&errorString=" + errorString;
//				}
//				
//				if(checkoutSessionId) {
//					URL = URL + "&checkoutSessionId=" + checkoutSessionId;
//				}
//				
				 
				 //new
				 errorCode = _errorCode;
				 errorMessage = _errorMessage;
				 errorString = _errorString;
				 checkoutSessionId = _checkoutSessionId;
				 
				 var paramObject = getQueryParameters(inflowParameterUrl.substring(0, 1) === '&' ? inflowParameterUrl.substring(1) : inflowParameterUrl);
				 
				 if (paramObject.deviceId === undefined && options.deviceId)
					 paramObject.deviceId = options.deviceId;
				 if (options.pageId)
					 paramObject.pageId = options.pageId;
				 if (options.createdBy)
					 paramObject.createdBy = options.createdBy;
				 if (options.errorId)
					 paramObject.errorId = options.errorId;
				 if (options.clientId)
					 paramObject.clientId = options.clientId;
				 if (options.userSegmentation)
					 paramObject.userSegmentation = options.userSegmentation;
				 if (contextId)
					 paramObject.contextid = contextId;
				 if (sectionId)
					 paramObject.sectionId = sectionId;
				 if (isInactive)
					 paramObject.isInactive = isInactive;
				 if (topicId)
					 paramObject.tp = topicId;
				 if (options.categoryId)
					 paramObject.categoryId = options.categoryId;
				 if (options.partId)
					 paramObject.partId = options.partId;
				 if (errorCode)
					 paramObject.errorCode = errorCode;
				 if (errorMessage)
					 paramObject.errorMessage = errorMessage;
				 if (errorString)
					 paramObject.errorString = errorString;
				 if (checkoutSessionId)
					 paramObject.checkoutSessionId = checkoutSessionId;
				 
				 if (options.trackParameters)
					 for (var key in options.trackParameters) {
						 paramObject[key] = options.trackParameters[key];
					 }
				 
				 var newURL = "";
				 
				 for (var key1 in paramObject) {
					 newURL += "&" + key1 + "=" + paramObject[key1];
				 }
				 
//				 console.log(URL, newURL);
				 
				 return newURL;
			 }
			
			function mask() {

				if ($('#mask').length > 0) {
					return false;
				}
				var mask = $('<div id="mask" class="ioly-m fade" />').appendTo(document.body);
				mask.addClass('in');
				return true;

			}

			function showSpinnerMobile() {

				if ($('#spin').length > 0) {
					return false;
				}
				var mask = $('<div id="spin" class="infspinner fade"> <div class = "spinimg" />  </div>').appendTo(document.body);
				mask.addClass('in');
				return true;

			}
			
			
			function removeSpinnerMobile() {

				if ($('#spin').length > 0) {
					$('#spin').remove();
				}
				
			}

			
			function trackContent(documentid, isVideo) {

				var serviceArguments = "&query=" + documentid + "&isVideo=" + isVideo
						+ getInflowParameterURL(sectionName, topicid, contextid);

				var url = options.address + "/trackContent?callback=?" + serviceArguments;
				$.getJSON(url, function(data) {
					contextid = data.sessionAttributes.contextId;
				});

			}

			

			function setPosition() {
				
				
				if ($('#callusoverlay').hasClass("mbovly")) {
					return;
				}
					
				var desktopVersion = false;
				
				
				$( ".chnls" ).each(function( index ) {
					  if($( this ).hasClass("sel") &&  $("#ocsmobilenobar").val() == 'true' ) { 
						  desktopVersion = true;
					  }  
					  
				});
				
				
				if (desktopVersion && $(window).width() < 768 ) {
					
					
				} 
				

				
				var elem = $('#callusoverlay'); 
				var oly2El= elem.find('#oly_2');
				var winEl = $(window);
				$('.oly-b').css({
					"margin-bottom" : ''
				});

				if (!desktopVersion) {
				
					elem.css({
						"width" : '',
						"height" : ''
					});
				}	

				
				var wth = oly2El.outerWidth();
				var ht = oly2El.outerHeight();
				var oW = $(window);
				
				if ($.browser && $.browser.msie && $.browser.version <= 8.0) {
					oly2El.css("width", "460px");
					elem.find('#callmeclose').css ({
						
						"display" :'none'
					});
				}	
				if (winEl.width() > 967 || desktopVersion) {
					mask();
					elem.css({
						"height" : ht,
						"width" : wth
					});
				}
				
				
				
				
				var width = elem.outerWidth(), height = elem.outerHeight(), client = {
					width : oW.width(),
					height : oW.height()
				}, top = Math.round((client.height - height) / 2), left = Math.round((client.width - width) / 2);

				if (winEl.width() > 967 || desktopVersion) {
					elem.css({
						"top" : Math.max(top, 0),
						"left" : Math.max(left, 0),
						"position" : 'fixed'

					});

				} else {
					
					var isovlywithclose = true;
					if (oly2El.find('#callmediv').is(':visible')) {
						isovlywithclose = false;
					}
					if ($('#' + options.mobileid).is(':visible')) {
						openCallmeOverlay(options.mobileid, isovlywithclose);
					}
				}

			}

			function callus(callustemplate,animate) {
				var serviceArguments = getInflowParameterURL(sectionName, topicid, contextid);
				var url = options.address + "/callus?callback=?" + serviceArguments + "&channelProvider=" + channelProviderCallUs + "&overlayType=-1";
				
				if(Inflow && options.presentationUserId){
					Inflow.initPresence(options.presentationUserId);
				}
				
				 
				$.getJSON(url, function(data) {
					data.content = contentJsonObj;
					if ($('#callusoverlay').is(':visible')) {
						return;
					}
					if (!(data.hasOwnProperty("genEWT")) ){
						data.genEWT = null;
					}
					

					if (data.overlayType == 0) {
						
						if (window.history && window.history.pushState) {
							 history.replaceState("selcallus", "selcallus", "selcallus");
							 history.pushState("callus", "callus", "callus");
						 }
					}
					if (callustemplate == null || data.overlayType ==0) { 
						$.when($("#callusoverlay").remove()).then(populateCallUSOverlay(data,callustemplate,animate));
					} 	 else {
						if ($("#deviceType").val() == "DEVICE_PHONE") {
							window.location.href = "tel:"+ data.phoneNo;
						} else {
							$.when($("#callusoverlay").remove()).then(populateCallUSOverlay(data,callustemplate,animate));
						}
					}
				});
				
			}
			
			
			function clickcallus(submitwophno) {
				if (submitwophno) {
					
					var serviceArguments = getInflowParameterURL(sectionName, topicid, contextid);
					var url = options.address + "/callus?callback=?" + serviceArguments + "&channelProvider=" + channelProviderCallUs + "&overlayType=1" + "&withoutPhNo=true&submitPOTP=true";
					
					window.location.href = "tel:"+ $("#cmphno").val();
					$.getJSON(url);
					
				
				} else {
					var phoneNumber = $('#cmprimaryphno').val();
					var serviceArguments = getInflowParameterURL(sectionName, topicid, contextid);
					var url = options.address + "/callus?callback=?" + serviceArguments + "&channelProvider=" + channelProviderCallUs + "&overlayType=1" + "&phoneNumber=" + phoneNumber + "&selectedCountry=" + selectedCountry + "&submitPOTP=true";
					$.getJSON(url, function(data) {
						if (data.status == 1) {
						$("#infpherror").addClass("hdn");	
						window.location.href = "tel:"+ $("#cmphno").val();
					}  else {
						$("#infpherror").removeClass("hdn");
					}
					});
				}	
				
			}

			var currentSurvey;
			function startSurvey(isDesktop) {
				
				var serviceArguments = getInflowParameterURL(sectionName, topicid, contextid);
				var url = options.address + "/startSurvey?callback=?" + serviceArguments + "&query=" + surveyId;
				
				if (!isDesktop) {
					
					showSpinnerMobile();
				}
				
				if (isSurveyProgress == false) {
					isSurveyProgress = true;
					$.getJSON(url, function(data) {
						isSurveyProgress = false;
						data.content = contentJsonObj;
						if (data.status ==1) {
							surveyid =  data.surveyContent.survey.id;
						}	
						currentSurvey = data;
						removeSpinnerMobile();
						$.when($("#surveycontentovly").remove()).then(populateSurveyOverlay(data,isDesktop));
					});
				}
				
			}
			
			
			function populateSurveyOverlay(data, isDesktop) {
				
				if (isDesktop==true) {
					data.isDesktop = true;
					var temp = $(inflowInlineSrvyTemplate(data));
					$.when($("#srvy-vid").empty()).then($(inflowInlineSrvyTemplate(data)).appendTo($("#srvy-vid")));
					
					positionSrvy();
					
					previousFocusId = $('#i-srvy');
					$('#i-srvy').addClass("sel");
					
					if (options.surveyTitle) {
						$("#srvyot").html(options.surveyTitle);
					} else {
						
						$("#srvyot").html(contentJsonObj.inflowcomponent.technicalissues);
					}	
					
					$("#srvydeskovr").removeClass("hdn").addClass("shw");
					$("#srvydeskolyin").attr("aria-hidden", "false");
					
					if ($(document).width() > 767 && mobilePlaceHolderEl.length == 0) {
						
						$("#srvydeskovr").addClass("ovrhdn");
					}
					setTimeout(function() {
						$.trapKeyboard('#srvydeskolyin');
						$("#clssrvy").focus();
					},400);
				
				
				} else {	
					data.isDesktop = false;
					$(inflowInlineSrvyTemplate(data)).appendTo($("#srvyenclose"));
					openMobileSurveyOverlay(options.mobileid);
					setTimeout(function() {
						$.trapKeyboard('#surveyovly');
						$("#mclssurvey").focus();
					},1000);
				}	
				
			}
			
			
			function populateCallUSOverlay(data, callustemplate, animate) {

				
				
				var callusolvy;
				data.channel = "callus";
				data.pageId = pageId;
				data.phoneNo = phoneNo;
				data.speechImpairedText = speechImpairedText;
				data.siteId = siteId;
				if (animate) {
					
					removeSpinnerMobile();
				}
				//data.supportedInflowCountriesEnum = data.supportedCountriesEnum;
				if (pageId == 'ResCenterHome') {
					data.tempPIN = null;
				}
				if (callustemplate == null) {
					$(callUSTemplate(data)).appendTo(ocsinflowoverlays);
					 callusolvy = $('#callusoverlay');
					 
					 
						$('#dragger').dragger({
							"target" : callusolvy
						});
						
					callusolvy.find('#phno').text(data.phoneNo);

					
					
					if (data.tempPIN) {
						callusolvy.find('#pscd').text(data.tempPIN);
						callusolvy.find('#pin').removeClass('hdn');
					}
					if (data.expiryTime) {
						callusolvy.find('#exp').text(data.expiryTime);
						callusolvy.find('#pinexpire').removeClass('hdn');

					}
					
					if (data.genEWT !=null) {
						callusolvy.find('#wait').text(data.genEWT);
						
					}


				} else {
					
					if ($("#deviceType").val() == "DEVICE_PHONE") {
						$(callustemplate(data)).appendTo(ocsinflowoverlays);
						callusolvy = $('#callusoverlay');
						waitTime = data.waitTime;
						cutopicName = data.topicName;
					} else {
						$(callustemplate(data)).appendTo(ocsinflowoverlays);
						callusolvy = $('#callusoverlay');
						callusolvy.find('#phno').text(data.phoneNo);
						if (data.tempPIN) {
							callusolvy.find('#pscd').text(data.tempPIN);
							callusolvy.find('#pin').removeClass('hdn');
						}
						if (data.expiryTime) {
							callusolvy.find('#exp').text(data.expiryTime);
							callusolvy.find('#pinexpire').removeClass('hdn');

						}
						
						if (data.genEWT !=null) {
							callusolvy.find('#wait').text(data.genEWT);
							
						}
					}	
				}	
				
				if (!mask()) {
					return;
				}
				
				var hoopel =  $('#hoop');
				var spitxtel =  $('#spitxt');

				if (data.callUsOverlayText) {
					callusolvy.find(hoopel).html(data.callUsOverlayText).removeClass('hdn');
				}
				if (data.speechImpairedText) {
					callusolvy.find(spitxtel).text(data.speechImpairedText).removeClass('hdn');
				}
				$(".whoopcu").html(whoopcu);
				callusolvy.find('#callustitle').removeClass('hdn');
				callusolvy.removeClass('hdn');
				callusolvy.find('#callusdiv').removeClass('hdn');
				
				if (data.overlayType == 0) {
				
					var countrylist = data.supportedInflowCountriesEnum;
					selectedCountry = data.selectedCountry;
					

					$.each(countrylist, function(key1, val) {
						var splited = key1.split("=");
						var countryId = splited[1];
						if (selectedCountry == countryId) {
							callusolvy.find('.landline').text(val.landLineTip);
							callusolvy.find('.mobile').text(val.cellTip);
							return;
						}
					});
				}
				
				if (animate) {
					var width = $(window).width();
					$('#callusoverlay').css({
						"left" : width,
						"position" : 'fixed',
						"top" : 0
					});
					$('#callusoverlay').removeClass("hdn");
					$('#callusoverlay').animate({'left': '0px'}, 200);
				}	
				setPosition();
				if($(window).width() > 967) {
					if (navigator.userAgent.indexOf("MSIE") != -1) {
						callusolvy.find('#oly_2').focus();
					} else {
						callusolvy.find('#close').focus();
					}
				}	
				
				if ($("#deviceType").val() == "DEVICE_PHONE") {
					
					callusolvy.find("#pin").addClass("hdn");
					callusolvy.find("#pinexpire").addClass("hdn");
				}

			}
			

			function showCallMeInvalidPhone() {
				$('#error').removeClass("hdn");
				$('#cmphno').attr("aria-labelledby","error").focus();
			}
			
			function clickcallme(callmeTemplate) {

				var phoneNumber = $('#cmphno').val();
				var isConsent = false;
				if (showConsent) {
					if ($("input:radio[name=oconsent]:checked").val() != 'yes') {

						isConsent = false;
					} else {

						isConsent = true;
					}

				}
				var serviceArguments = "&overlayType=1" + "&phoneNumber=" + phoneNumber + "&selectedCountry=" + selectedCountry + "&isMemberConsent ="
						+ isConsent + getInflowParameterURL(sectionName, topicid, contextid) + "&channelProvider=" + channelProviderCallMe;
				var url = options.address + "/callme?callback=?" + serviceArguments;
				$.getJSON(url, function(data) {
					data.content = contentJsonObj;
					$('#clkcallme').attr('sclicked', "0");
					$('#sbtcallme').attr('sclicked', "0");
					contextid = data.sessionAttributes.contextId;
					
					if (data.callMeChannelStateEnum.id == 2) {
						var phoneTitle = $("#phtitle").val();
						phoneTitle = phoneTitle + data.phoneNo;
						$('#cmphno').attr("title",phoneTitle);
						
						showCallMeInvalidPhone();
					} else {
						$.when($("#callusoverlay").remove()).then((function() {
							mask();
							data.waitTime = waitTime;
							if (callmeTemplate == null) {
								$(callMETemplate(data)).appendTo(ocsinflowoverlays);
							} else {
								
								$(callmeTemplate(data)).appendTo(ocsinflowoverlays);
								$('#callusoverlay').removeClass("hdn");
							}
							var cmsuccessEl = $('#cmsuccess');
							if ($('#cmsuccess').length ==0) {
								
								cmsuccessEl = $('.cnfmbox');
								cmsuccessEl.find('#wait').text(waitTime);
									
							} else {  
								cmsuccessEl.find('#cmwait').text(waitTime);
								cmsuccessEl.find('#cmwait').text(waitTime);
								cmsuccessEl.find('#cmphoneno').text(phoneNumber);
								cmsuccessEl.find('#cmtopic').text(cutopicName);
							}	
							setPosition();
							$('#dragger').dragger({
								"target" : $('#callusoverlay')
							});
							$('#spancmcap').focus();
								

						})());
						
						
					}
					
					});
			}

			function callme(callmeTemplate,animate) {

				var serviceArguments = "&overlayType=0" + getInflowParameterURL(sectionName, topicid, contextid) + "&channelProvider=" + channelProviderCallMe ;

				var url = options.address + "/callme?callback=?" + serviceArguments;
				
				if(Inflow && options.presentationUserId){
					Inflow.initPresence(options.presentationUserId);
				}

				$.getJSON(url, function(data) {
					data.content = contentJsonObj;
					data.isTouchDevice = isTouchDevice;
					if ($('#callusoverlay').is(':visible')) {
						return;
					}
					waitTime = data.waitTime;
					cutopicName = data.topicName;
					$.when($("#callusoverlay").remove()).then(populateCallMeOverlay(data,callmeTemplate,animate));
				});

			}
			
			function populateCallMeOverlay(data,callmeTemplate, animate) {
				
				
				
				if (animate) {
					
					removeSpinnerMobile();
				}
				if (callmeTemplate == null) {
					$(callMETemplate(data)).appendTo(ocsinflowoverlays);
				} else {
					
					$(callmeTemplate(data)).appendTo(ocsinflowoverlays);
				}	
				var callusovly= $('#callusoverlay');

				
				$('#dragger').dragger({
					"target" : callusovly
				});
				
				callusovly.find(".whoopcm").html(whoopcm);
				if (!mask()) {
					return;
				}
				contextid = data.sessionAttributes.contextId;
				if (data.status == 2) {
					refreshInflow();
					setPosition();
					return;
				}

				if (data.callMeChannelStateEnum.id == 4) {
					callusovly.find('#cmtopicname').text(data.topicName);
					callusovly.find("#wait").text(data.waitTime);
				} else if (data.callMeChannelStateEnum.id == 0 || data.callMeChannelStateEnum.id == 2) {
					$("#sel_cntry").css("height", $("#cmphno").outerHeight());
					
					callusovly.find("#phtitle").find("#cmtphno").replaceWith(data.phoneNo);
					callusovly.find("#cmphno").attr("title", callusovly.find("#phtitle").text());
					
					callusovly.find("#tpname").text(data.topicName);
					if (data.waitTime>=0) {
						callusovly.find("#wait").text(data.waitTime);
					}
					callusovly.find("#cmtphno").val(data.phoneNo);
					

					var countrylist = data.supportedInflowCountriesEnum;
					selectedCountry = data.selectedCountry;

					waitTime = data.waitTime;
					cutopicName = data.topicName;

					$.each(countrylist, function(key, val) {
						var splited = key.split("=");
						var countryName = splited.length == 3 ? splited[2] : splited[0];
						var countryId = splited[1];
						if (selectedCountry == countryId) {
							callusovly.find("#span_cntry").text(countryName + " +" + val.countryCode);
							callusovly.find('.landline').text(val.landLineTip);
							callusovly.find('.mobile').text(val.cellTip);
							return;
						}
					});

				}
				
				if (animate) {
					var width = $(window).width();
					$('#callusoverlay').css({
						"left" : width,
						"position" : 'fixed',
						"top" : 0
					});
					$('#callusoverlay').removeClass("hdn");
					$('#callusoverlay').animate({'left': '0px'}, 200);
				}	
				setPosition();
				if($(window).width()>967) {
					if (navigator.userAgent.indexOf("MSIE") != -1) {
						$('#oly_2').focus();
					} else {
						$('#close').focus();
					}
				}	
			}

			function showPopupBlocked() {

			}

			function showChatUnavailable(isError) {
				var status=2;
				if (isError) {
					status = 0;
				} else {
					status = $('#dchat').length > 0 ? $('#dchat').attr('ur') : $('#i-chat').attr('ur');
				}
				
				
				if (!mask()) {
					return;
				}
				var data = {};
				data.errors = [];
				data.status = status;
				data.content = contentJsonObj;
				$.when($("#callusoverlay").remove()).then((function() {
					$(chatTemplate(data)).appendTo(ocsinflowoverlays);
					$('#dragger').dragger({
						"target" : $('#callusoverlay')
					});
					$(".whoopchat").html(whoopchat);
					setPosition();
					if($(window).width() > 967) {
						if (navigator.userAgent.indexOf("MSIE") != -1) {
							$('#oly_2').focus();
						} else {
							$('#close').focus();
						}
					}	
				})());
			}

			function showCallUsUnavailable(status) {
				
				if (!mask()) {
					return;
				}
				var data = {};
				data.status = status;
				data.content = contentJsonObj;
				data.pageId = pageId;
				data.phoneNo = phoneNo;
				data.speechImpairedText = speechImpairedText;
				data.siteId = siteId;
				
				$.when($("#callusoverlay").remove()).then((function() {
					
					$(callUSTemplate(data)).appendTo(ocsinflowoverlays);
					$('#dragger').dragger({
						"target" : $('#callusoverlay')
					});
					$("#phno").html(phoneNo);
					$(".whoopcu").html(whoopcu);
					setPosition();
					if($(window).width() > 967) { 
						if (navigator.userAgent.indexOf("MSIE") != -1) {
							$('#oly_2').focus();
						} else {
							$('#close').focus();
						}
					}	
				})());
			}

			function showCallMeUnavailable(status, errors) {
				if (!mask()) {
					return;
				}
				var data = {};
				data.errors = errors ? errors : [];
				data.status = status;
				data.callMeChannelStateEnum = {};
				data.content = contentJsonObj;
				$.when($("#callusoverlay").remove()).then((function() {
					$(callMETemplate(data)).appendTo(ocsinflowoverlays);
					$('#dragger').dragger({
						"target" : $('#callusoverlay')
					});
					if(status==11) {
						$(".whoopchat").html(whoopchat);
					} else {
						$(".whoopcm").html(whoopcm);
					}
					setPosition();
					if($(window).width() > 967) {
						if (navigator.userAgent.indexOf("MSIE") != -1) {
							$('#oly_2').focus();
						} else {
							$('#close').focus();
						}
					}	
				})());

			}

			parentEl.on("click", '#dcallus,#dcallus-i', function(event) {
				hideFaqVideos();
				hideDsktpSrvy();
				previousFocusId = event.target;
				checkCallUsStatusAndAct();
				

			});

			parentEl.on("click", '#i-callus', function(event) {
				previousFocusId = event.target;
				hideFaqVideos();
				hideDsktpSrvy();
				if (!$('#i-callus').hasClass("sel")) {
					$('#i-callus').addClass("sel");
				}
				if ($(this).attr("available") == "true")
					callus();
				else
					checkCallUsStatusAndAct();

			});

			function checkCallUsStatusAndAct() {
				var status = $('#dcallus').length > 0 ? $('#dcallus').attr('ur') : $('#i-callus').attr('ur');
				if (status) {
					showCallUsUnavailable(status);
					
				}
				
				
			}

			parentEl.on("click", '#dcallme,#dcallme-i', function(event) {
				hideFaqVideos();
				hideDsktpSrvy();
				previousFocusId = event.target; 
				checkCallMeStatus();
			});

			function checkCallMeStatus() {

				var status = $('#dcallme').length > 0 ? $('#dcallme').attr('ur') : $('#i-callme').attr('ur');
				if (status) {
					showCallMeUnavailable(status, null);
				}
			}

			parentEl.on("click", '#dchat,#dchat-i', function(event) {
				hideFaqVideos();
				hideDsktpSrvy();
				previousFocusId = event.target;
				showChatUnavailable(false);
			});



			function closeOverlay() {
				
				$.when($("#callusoverlay").remove()).then((function() {
					$('#mask').remove();
					$('#i-callus, #i-callme, #i-chat').removeClass('sel');
					$(previousFocusId).focus();
				})());
			}

			
			function populateVideos(videorecords, parents) {

				$('.vsection').addClass('hdn');
				if (videorecords != null || videorecords.length > 0 ) {
					$('.vsection').removeClass('hdn');
				}
			}
			
			
		

		
			
			function infscallback (data) {
				
				var section;
				var prop;
				
				for (prop in data.responseMap) {
					section = data.responseMap[prop];
				}
				
				if (!section) {
					return;
				}
				var channelRecords = section.channelRecords;
				var surveyRecords = section.surveyRecords;
				var channelRecordsArray = new Array(6);
				var lookupRecords = section.lookupRecords;
				if (section.faqRecords) {
					section.faqrecords = section.faqRecords;
				}
				var faqRecords = section.faqrecords;
				var lookupRecord = null;
				var preferredChannelRecords = section.preferredChannelRecords;
				var callusChannelRecord = null, chatChannelRecord = null, callmeChannelRecord= null;
				
				
				var offerRecords = section.offerRecords;
				
				
				pageId = section.pageId;
				var j =0;
				var faqlen = faqRecords.length; 
				for(i=0;i<faqlen ;i++) {
					
					if(faqRecords[j].hasOwnProperty("status")) {
						if(faqRecords[j].status == 6) {
							faqRecords.splice(j,1);
						} else {
							
							j = j+1;
						}
					}	
				}

				
				section.faqrecords = faqRecords;
				
				if (faqRecords.length == 0) {
					
					return;
				}
				$("#chatpopupovr").addClass("hdn").removeClass("shw");;
				
				data.islink = options.isMobileLink;
				
				var videorecords = section.videoRecords;
				j = 0;
				var vidlen = videorecords.length;
				for ( var i = 0; i < vidlen; i++) {
					if (videorecords[j].hasOwnProperty("status")) {
						if(videorecords[j].status == 6) {
							videorecords.splice(j,1);
							continue;
						} else {
							
							j =j+1;
						}
					}	
				}
				
				section.videoRecords = videorecords;

				
					for (var i=0;i<lookupRecords.length;i++ ) {
					
						
						if(options.deviceId !== "DEVICE_PHONE") {
							if (i==0) {
								thresholdInactivityPeriod = lookupRecords[i].value;
							}	
							if (i==1) {
								maxProactiveCount = lookupRecords[i].value;
							} 
							if (maxProactiveCount == -1  ||  Number (proactiveCount) < Number(maxProactiveCount)) {
								timerId = setInterval(increment, inactivityStep);
							}	
						}	
					}
					
				

				var i=0;
				var numChannels = 0;
				
				
				
				
				
				var j =0;
				var srvylen = surveyRecords.length; 
				for(i=0;i<srvylen ;i++) {
					
					if(surveyRecords[j].hasOwnProperty("status")) {
						if(surveyRecords[j].status == 6) {
							surveyRecords.splice(j,1);
						} else {
							
							j = j+1;
						}
					}	
				}

				
				section.surveyRecords =  surveyRecords

				
				for(i=0;i<channelRecords.length;i++) {
					if(!(channelRecords[i].status ==6)) {
						
						numChannels = numChannels + 1;
						
						if(channelRecords[i].channelType==="LIVE_HELP" || channelRecords[i].channelType==="CHAT") {
								channelProviderChat = channelRecords[i].channelProvider;
								if(channelRecords[i].available) {
									channelRecordsArray[0] = channelRecords[i];
								} else {
									channelRecordsArray[3] = channelRecords[i];
								
								}
								chatChannelRecord =  channelRecords[i];
							
						}
						
						else if(channelRecords[i].channelType==="CALL_US") {
							channelProviderCallUs = channelRecords[i].channelProvider;
							if(channelRecords[i].available) {
								channelRecordsArray[1] = channelRecords[i];
							} else {
								channelRecordsArray[4] = channelRecords[i];
								
							} 
							callusChannelRecord =  channelRecords[i];
							phoneNo = channelRecords[i].phoneNumber;
							speechImpairedText = channelRecords[i].speechImpairedText;
						}
						
						else if(channelRecords[i].channelType==="CALL_ME") {
							channelProviderCallMe = channelRecords[i].channelProvider;
							if(channelRecords[i].available) {
								channelRecordsArray[2] = channelRecords[i];
								
							} else {
								channelRecordsArray[5] = channelRecords[i];
								
							} 
							
							callmeChannelRecord =  channelRecords[i];
							
						}
					}
					
				}
				if (numChannels==0 && options.hideFAQWhenNoChannels) {
					
					section.faqrecords = null;
				}
				if(i>0) {
					data.responseMap[prop].channelRecords = cleanArray(channelRecordsArray);
				}	
				
				data.content = contentJsonObj;
				var openFaqOverlay = false;
				if (inflowInlineFaqsTemplate) {
					if ($('#i-faq').hasClass("sel")) {
						openFaqOverlay = true;
					}
				}

			
				
				
				$(".fltlft").removeClass("flt");
				$(".videoicon").css({"position": "" });
				
				$("#ocs-comp").empty();
					
				
				
				//removed delay added for Gandalf
				
				hideDsktpSrvy();
				/*if (options.inflowHelpMin) {
					data.inflowHelpMin = true;
				}*/
				setTimeout(function() {
					
				$(inflowComponentTemplate(data)).appendTo($("#ocs-comp"));
				
				
				
				$(".fltlft").addClass("flt");
				$(".videoicon").css({"position": "absolute" });
				$("#ocs-inflow").css({
				"right" : options.posRight + 'px',
				"top" : options.posTop + 'px'
				}).addClass("shw").removeClass("hdn");
				if ($(document).width() > 767 && mobilePlaceHolderEl.length == 0) {
					$("#ocs-inflow").addClass("ovrhdn");
				}

				if (options.surveyTitle) {
					$("#i-srvy").attr("title",options.surveyTitle);
				} else {
					$("#i-srvy").attr("title", contentJsonObj.inflowcomponent.technicalissues);
				}
						
				//removed delay added for Gandalf
				},0);
				
				
				
				$.when($("#mobileinflow").remove()).then($(mobileInflowTemplate(data)).appendTo(mobileParentEl));
				handleMobileHelp();
				
				
				


				var sec = (sectionName ? sectionName : options.sectionId);
				var seclen = $('div[inflow]').length;
				var actualseclen = 0;
				if (seclen > 1) {
					$(".videoiconhr").css({"display" : "none"});
					
						
						$.when($(".mobile-faqovr").remove()).then($(mobileInflowHelpOverLayTemplate(data)).appendTo($("#faqenclose")));
						$.when($("#seccontent").remove()).then($(mobileInflowSecOverLayTemplate(data)).appendTo($(".mfaqttlenclose")));
						for ( var i = 0; i < seclen && seclen > 1 ; i++) {

							var inflowstr = $($('div[inflow]').get(i)).attr('inflow');
							if(inflowstr!=null && inflowstr.length!=0) {
								actualseclen = actualseclen + 1;
								var temp = $('.sl');
								if (actualseclen > 1) {
									temp = $('.sl').first().clone();
//									$(temp).find('.sectionTitle').attr('role',"button");
									$(temp).appendTo('#sectionlist');
								}
								var jsonobj = $.parseJSON(inflowstr);
								if (jsonobj.property_name == "sectionId") {
									var sttl = jsonobj.section_title;
									$(temp).find('.sectionTitle').html(sttl).attr('inflow', inflowstr);
								}
							}
						}
						
						
							
					
				} else {
					
					$.when($(".mobile-faqovr").remove()).then($(mobileInflowHelpOverLayTemplate(data)).appendTo($("#faqenclose")));
					$.when($("#infcontent").remove()).then($(mobileInflowFaqOverLayTemplate(data)).appendTo($(".mfaqttlenclose")));
										
				}
				
				if (inflowInlineFaqsTemplate) {
					$.when($("#faq-vid").empty()).then($(inflowInlineFaqsTemplate(data)).appendTo($("#faq-vid")));
					
					
					if (openFaqOverlay) {
						$('#i-faq').click();
					} else {
						positionFaqVideos();
					}
				}
				
				$.each(data.responseMap, function(key, val) {
					section = val;
					sectionName = key;
				});
				
				populateVideos(videorecords, [ ('.infhelp'), ('#ovlyfaq'), ('#faq-vid') ]);
				contextid = section.contextId;
				topicid = section.topicId;
				segmentid = section.segmentId;
				
				for ( var i in section.channelRecords) {
					var channelRecord = section.channelRecords[i];
					var channelType = channelRecord.channelType;
					if (channelType == 'CALL_US' && channelRecord.hooptext) {
						whoopcu = channelRecord.hooptext;
					} else if (channelType == 'CALL_ME' && channelRecord.hooptext) {
						whoopcm = channelRecord.hooptext;

					} else if ((channelType == 'LIVE_HELP' || channelType == 'CHAT' ) && channelRecord.hooptext) {
						whoopchat = channelRecord.hooptext;

					}
				}
				
				
				
				
				if (preferredChannelRecords.length > 0) {
					
					
					openProactiveChatOvly(preferredChannelRecords, channelRecordsArray,options.pageId, options.siteId);
					
				}
				
				populateMobileCallusAndChat(callusChannelRecord, chatChannelRecord, callmeChannelRecord );
				
				
				 
				
				for(i=0;i<surveyRecords.length;i++) {
					if(surveyRecords[i].hasOwnProperty("status")) {
						if (surveyRecords[i].status == 6) {
							break;
						} 
						
					}	
					
					surveyId = surveyRecords[i].surveyId;
					
					if (options.surveyTitle) {
						$("#msurveylnk").html(options.surveyTitle);
					} 
					$("#msurvey").removeClass("hdn");
					break;
				}	
				
				if (offerRecords != null) {
				
					handleOfferRecords(offerRecords);
				}

				
				if (openFaqOverlay) {
					$('#i-faq').click();
				}
				
				if (typeof options.initcallback == 'function') {
					options.initcallback.call();
				}
				
				

			};
			
			var svcWin;
			var  myebayWin;
			function openWin (url,isPopup) {
				
				if (isPopup) {
					if (svcWin!==undefined &&! svcWin.closed && svcWin.focus){
						svcWin.focus();
					} else {
			    	   
			    	   svcWin= window.open(url, "svcWin", "width=460,height=665,screenX=50,left=50,screenY=50,top=50,location=no,menubar=no,scrollbars=yes,resizable=yes,status=no");
			    	   
					}
				}  else {
					if (myebayWin!==undefined &&! myebayWin.closed && myebayWin.focus){
						myebayWin.focus();
					} else {
			    	   
						myebayWin= window.open(url, "myebayWin");
			    	   
					}
				}	
			}
			
			
			$(document).on ("click",".ifhcagmt", function (e) {
				
				
				var url =  $(".ifhcagmt").attr("href");
				
				openWin (url,true);
				 e.preventDefault();
			});
			
			
			
			
			
			
			function handleOfferRecords(offerRecords) {
				
				
				
				for(var i=0;i<offerRecords.length;i++) {
					
					var offerRecord = offerRecords[i].offer;
			
					
					var data = {};
					var fenddate = new Date (offerRecord.endDate);
					fdate =   contentJsonObj.inflowcomponent.monthList[fenddate.getMonth ()]  + " " + fenddate.getDate() + ", " +  fenddate.getFullYear();
					data.offerRecord = offerRecord;
					data.content=contentJsonObj;
					
										
					$.when($(couponPlaceHolderEl).empty()).then($(couponTemplate(data)).appendTo(couponPlaceHolderEl));
					setTimeout(function() {
						$("#ccode").text(offerRecord.offerCode);
						$("#expiry").text(fdate);
						$("#camt").text("$" + offerRecord.offerDetails.computationalRules[0].action.actionValue);
						$("#minamt").text("$" + offerRecord.condition.values[0]);
						

					},1);
					
					
					break;
					
				}	
				
			}

			
			function callInflowService(serviceArguments, isHandleSelect) {
				
				if (inProgress == true ){
					
					return;
				}
				inProgress = true;
				var infrescallback = "infscallback";
				if (isHandleSelect) {
					
					infrescallback = "handleSelectCallback";
				}
				
				
				var url = options.address + "/single?" + serviceArguments + "&timestamp=" + timestamp;

				var deferred = $.ajax({
					url : url,
					crossDomain : true,
					cache : true,
					dataType : "jsonp",
					jsonpCallback : infrescallback,
					timeout : timeoutduration,
					success: function(data) { 
						 	if (isHandleSelect) {
						 		handleSelectCallback(data);
						 	} else {
						 		infscallback(data);
						 	}	
				    }
				});
				
				
				deferred.always(function() {
					
					inProgress = false;
					if (typeof options.initcallback == 'function') {
						options.initcallback.call();
					}
					
					
					
				});

			}
			
			function handleSelectCallback(data) {
				
				var section;
				var prop;
				
				for (prop in data.responseMap) {
					section = data.responseMap[prop];
				}
				
				if (!section) {
					return;
				}
				if (section.faqRecords) {
					section.faqrecords = section.faqRecords;
				}
				removeSpinnerMobile();
				$.when($("#infcontent").remove()).then($(mobileInflowFaqOverLayTemplate(data)).appendTo($("#faqsecenclose")));
				openMobileFaqSectionOverlay();
			}
			
			function cleanArray(actual){
				  var newArray = new Array();
				  for(var i = 0; i<actual.length; i++){
				      if (actual[i]){
				        newArray.push(actual[i]);
				    }
				  }
				  return newArray;
			}
			
			function  handleMobileHelp () {
				if (options.helpTitle) {
					$('#mfaqclk').html(options.helpTitle);
				}	
				if (options.mobilePlaceHolder) {
					$('#' + options.mobilePlaceHolder).append($('#mobileinflow'));
					
				}	
			}

						
			var dragtarget = $('#faqdeskoly');
			$('#faqdragger').dragger({
				"target" : dragtarget,
			});
			

			var dragtarget = $("#srvydeskolyin");
			$('#srvydragger').dragger({
				"target" : dragtarget,
			});
			
			ocsinflowoverlays.on("click", '#clsvd', function() {

				closeVideoOverlay();
				
			});

			ocsinflowoverlays.on("click", '#vdmclose', function() {

				closeVideoOverlay();
			});

			function getScrollTop() {
				scrolltop = $(window).scrollTop();
				
			}

			function setScrollTop() {
				window.scrollTo(0, scrolltop);
			}

			function openFAQOverlay(element) {
				
				var faqovly = $('#faqoverlay');
				var parentEL = $("#" + element);
				if (faqovly.is(':visible')) {
					
					closeFAQOverlay();
					return;
				}
				
				$('#rmfaqclk').parent().addClass('active');
				if (typeof options.mobileshowcallback == 'function') {
					options.mobileshowcallback.call();
				}
				
				$.when(window.scrollTo(0, 0)).then((function() {
					parentEL.css({
						"top" : 0,
						"position" : 'fixed',
						"width" : '100%'
					});
					var offset ={}; offset.top = document.getElementById(element).offsetTop;
					
					faqovly.removeClass('hdn');
					faqovly.css({
						"top" : parentEL.outerHeight() + (offset.top - $(window).scrollTop()),
						"position" : 'fixed',
						"left" : 0
					});

					if (onlyOneSection)
						faqovly.find('.content').removeClass('hdn').addClass('shw');
					
					var temp = parentEL.outerHeight() + (offset.top - $(window).scrollTop());
					faqovly.find('#faqenclose').css({
						"margin-bottom" : temp + 40
					});
				})());
				
				
				
			}

			
			
			function openMobileFAQOverlay(element) {
				
				var faqovly = $('#faqoverlay');
				//var ht = $(window).height();
				//var wth = $(window).width();
				//$('#faqoverlay').css({"height": ht,"width" : wth});
				var parentEL = $("#" + element);
				if (options.helpTitle) { 
					$("#hittl").html(options.helpTitle);
				} else {
					if (options.isMobileLink) {
						$("#hittl").html(contentJsonObj.inflowcomponent.needhelp);
					} else {
						$("#hittl").html(contentJsonObj.inflowcomponent.faq);
					}
				}	
				
				if (options.isMobileLink) {
					
					$("#faqttlm").html(contentJsonObj.inflowcomponent.faqlong);
				}
				
				$("#mclsinf").html(contentJsonObj.inflowcomponent.cancel);
				faqovly.removeClass('hdn');
				var ht = $(window).height();
				faqovly.css({
					"top" : ht,
					"position" : 'fixed',
					"left" : 0
				});
				
				
				
				
				$('#faqoverlay').animate({'top': '0px'}, 500);
				
				$('body').addClass('noscroll');
			}
			
			
			function openMobileSurveyOverlay(element) {
				var faqovly = $('#surveyovly');
				//var ht = $(window).height();
				//var wth = $(window).width();
				//$('#surveyovly').css({"height": ht,"width" : wth});
				
				$("#sbtsurvey").removeClass("ibtn").removeClass("ibtn-ter").removeClass("sbtdsk").addClass("sbtn");
				if (options.surveyTitle) {
					$("#sittl").html(options.surveyTitle);
				} else {
					$("#sittl").html(contentJsonObj.inflowcomponent.technicalissues);
				}	
				$("#mclssurvey").html(contentJsonObj.inflowcomponent.cancel);
				faqovly.removeClass('hdn');
				var width = $(window).width();
				faqovly.css({
					"top" : 0,
					"position" : 'fixed',
					"left" : width
				});
				
				$('#surveyovly').animate({'left': '0px'}, 500);
			}
			
			
			function populateMobileCallusAndChat (data, chatdata, callmedata) {
				
				if (data == null && chatdata == null && callmedata == null) {
					
					return;
					
				}
				var hoopel =  $('#mhoop');
				
				var callmehoopel =  $('#cmhoop');
				
				
				var spitxtel =  $('#mspitxt');
				var mewt = $("#mewt");
				var contactdiv = $("#contactdiv");
				var chathoopel = $('#chatmhoop');

				
				contactdiv.removeClass("hdn");
				
				if (data!=null) {
				
					if (data.status == 2) {
						
						if (data.hooptext) {
							contactdiv.find (".whoopcu").html(data.hooptext);
						} else {
							
							contactdiv.find (".whoopcu").text("");
						}	
					}
					
					if (data.hooptext) {
						contactdiv.find(hoopel).html(data.hooptext).removeClass('hdn');
					}
					if (data.genEWT && data.genEWT >=0) {
						$("#mewt").find("#wait").text(data.genEWT);
						$("#mewt").removeClass("hdn");
					}
				}
				
				if (callmedata!=null) {
					
					if (callmedata.status == 2) {
						
						if (callmedata.hooptext) {
							contactdiv.find (".whoopcm").html(callmedata.hooptext);
						} else {
							
							contactdiv.find (".whoopcm").text("");
						}	
					}
					if (callmedata.hooptext) {
						contactdiv.find(callmehoopel).html(callmedata.hooptext).removeClass('hdn');
					} 
					if (callmedata.waitTime && callmedata.waitTime >=0) {
							$("#cmewt").find("#wait").text(callmedata.waitTime);
							contactdiv.find("#cmewt").removeClass("hdn");
					}
				}
				
				
				
				if(chatdata!=null) {
				
					if (chatdata.hooptext) {
						contactdiv.find(chathoopel).html(chatdata.hooptext).removeClass('hdn');
					}
					
					
					if (chatdata.status == 2) {
						
						if (chatdata.hooptext) {
							contactdiv.find (".whoopchat").html(chatdata.hooptext);
						} else {
							
							contactdiv.find (".whoopchat").text("");
						}	
					}
					
					if (chatdata.waitTime && chatdata.waitTime >=0) {
						
						
						$("#chatmewt").find("#wait").text(chatdata.waitTime);
						contactdiv.find("#chatmewt").removeClass("hdn");
						
					}
				
				}
				
				
				
			}
			
			function openCallmeOverlay(element, isovlywithclose) {

				$('#mask').remove();

				getScrollTop();
				
				if (typeof options.mobileshowcallback == 'function') {
					options.mobileshowcallback.call();
				}

				$.when(window.scrollTo(0, 0)).then((function() {
					$("#" + element).css({
						"top" : 0,
						"position" : 'fixed',
						"width" : '100%'
					});

					$('#callusoverlay').css({
						"width" : '100%',
						"height" : '100%'
					});

					var offset = $("#" + element).offset();
					if (offset) {
						$('#callusoverlay').css({
							"top" : $("#" + element).outerHeight() + (document.getElementById(element).offsetTop - $(window).scrollTop()),
							"left" : 0,
							"position" : 'fixed'
						});
						var temp = $("#" + element).outerHeight() + (offset.top - $(window).scrollTop());
						$('.oly-b').css({
							"margin-bottom" : temp + 40
						});
					}
					if (isovlywithclose) {

						$('.closebar').removeClass('hdn');

					}
				})());
			}

			function refreshInflow() {
				timestamp = new Date().getTime();


				var serviceArguments = null;
				serviceArguments = getInflowParameterURL(sectionName,topicid,contextid,false);
					

			}

			function closeFAQOverlay() {
				
				var contentEL = $('#faqoverlay').find('.content');
				$('#rmfaqclk').parent().removeClass('active');
				document.body.style.overflow = 'visible';
				var elem = options.mobileid;
				if ($('#tblinflow').is(':visible')) {
					elem = options.tabletid;
				}
				contentEL.addClass('hdn');
				if (onlyOneSection)
					contentEL.removeClass('shw');
				contentEL.appendTo('#faqenclose');
				$('#faqoverlay').addClass('hdn');
				$('body').removeClass("noscroll");
				$("#" + elem).css({

					"top" : '',
					"position" : '',
					"width" : ''

				});
				
				
				
				$('.sectionTitle').removeClass('stselect');
				if (typeof options.mobileshowcallback == 'function') {
					options.mobilehidecallback.call();
				}
			}

			function closeCallmeOverlay() {
				$('#rmcallclk').parent().removeClass('active');
				$('#rmcallmeclk').parent().removeClass('active');
				document.body.style.overflow = 'visible';
				htmlTags = document.getElementsByTagName("html");
				for ( var i = 0; i < htmlTags.length; i++) {
					htmlTags[i].style.overflowY = "visible";
				}
				var elem = options.mobileid;
				if ($('#tblinflow').is(':visible')) {
					elem = options.tabletid;
				}
				$('#faqoverlay').find('.content').addClass('hdn');
				$('#callusoverlay').addClass('hdn');
				$("#" + elem).css({

					"top" : '',
					"position" : '',
					"width" : ''

				});
				if (typeof options.mobileshowcallback == 'function') {
					options.mobilehidecallback.call();
				}
				closeOverlay();
			}

			ocsinflowoverlays.on("click", '#callmeclose', function(event) {
				$('#rmcallmeclk').parent().removeClass('active');
				closeCallmeOverlay();
				setScrollTop();
			});

			ocsinflowoverlays.on("click", '#callusclose', function(event) {
				$('#rmcallclk').parent().removeClass('active');
				$('#rmcallmeclk').parent().removeClass('active');
				closeCallmeOverlay();
				setScrollTop();
			});

			parentEl.on("click", '#faqclose', function(event) {
				var faqovly = $('#faqoverlay');
				if (faqovly.is(':visible')) {
					$('#faqoverlay').find('.content').addClass('hdn');
					closeFAQOverlay();
					setScrollTop();
					$('#rmfaqclk').focus();
				}	
			});
			
			parentEl.on("click", '#mclsinf', function(event) {
				
				var faqovly = $('#faqoverlay');
				
				if (faqovly.is(':visible')) {
					if (window.history && window.history.pushState) {
						window.history.go(-1);
					}	else {
						
						var faqovly = $('#faqoverlay');
						
						if (faqovly.is(':visible')) {
							$('#faqoverlay').find('.content').addClass('hdn');
							
							$('#faqoverlay').addClass('hdn');
							$('body').removeClass("noscroll");
							 setTimeout(function() {
									$("#mfaqclk").focus();
								},100);
							
							
						}
					}
					 
					
				}
				
			});
			
			parentEl.on("click", '#mclssurvey', function(event) {
				
				closeSurveyAndFAQ();
			});
			
			
			
			
			function closeSurveyAndFAQ () {
				
				var faqovly = $('#faqoverlay');
				if (faqovly.is(':visible')) {
					if (window.history && window.history.pushState) {
						window.history.go(-2);
						var faqovly = $('#faqoverlay');
						
						if (faqovly.is(':visible')) {
							$('#faqoverlay').find('.content').addClass('hdn');
							$('#faqoverlay').addClass('hdn').removeClass("shw").removeClass("ovrhdn");
							$('body').removeClass('noscroll');
							 setTimeout(function() {
									$("#mfaqclk").focus();
								},100);
							
							
						}
					}	else {
						
						var faqovly = $('#faqoverlay');
						
						if (faqovly.is(':visible')) {
							$('#faqoverlay').find('.content').addClass('hdn');
							$('#faqoverlay').addClass('hdn').removeClass("shw").removeClass("ovrhdn");
							$('body').removeClass('noscroll');
							 setTimeout(function() {
									$("#mfaqclk").focus();
								},100);
							
							
						}
					}
				}
				$("#surveyovly").addClass('hdn').removeClass("ovrhdn");
				
				setTimeout(function() {
					$("#mfaqclk").focus();
				},100);
				
				$("#faqdeskovr").removeClass("shw").removeClass("ovrhdn").addClass("hdn");
				$("#srvydeskovr").removeClass("shw").removeClass("ovrhdn").addClass("hdn");
				
				
			}

			
			parentEl.on ("keyup cut blur mouseup click", ".infta" , function (event) {

				var scrollHeight = $(this).prop ('scrollHeight');
			    var clientHeight = $(this).prop ('clientHeight');
			    if(scrollHeight!=clientHeight) {
			    	$(this).height(scrollHeight);
			    }
					
			    checkSurveyAnswered();
			});
		
			parentEl.on ('change', ".infcbox__control, .infradio__control", function (event) {
				checkSurveyAnswered();
			});
		
			parentEl.on ("paste", ".infta" , function (event) {
			    $("#sbtsurvey").removeAttr("disabled","false");
		    	$("#sbtsurvey").attr("aria-disabled","false");
		    	$("#sbtsurvey").removeClass("btnd");
			});

			
			
			parentEl.on ("click", ".back" , function (event) {
				closeInflowHelp();
			});

			
			parentEl.on ("keypress", ".back" , function (event) {
				closeInflowHelp();
			});

						
			mobileParentEl.on("click", '#rmfaqclk', function(event) {
				
				$('#rmchatclk').parent().removeClass('active');
				closeCallmeOverlay();
				
				if (typeof options.closeovlycallback == 'function') {
					options.closeovlycallback.call();
				}
				
				setTimeout(function() {
					window.scrollTo(0, scrolltop);
				}, 50);
				if (!$('#faqoverlay').is(':visible')) {
					getScrollTop();
				}
				openFAQOverlay(options.mobileid);
				previousFocusId = event.target;
				setTimeout(function() {
					$('#roly_2').focus();
				}, 300);
			});
			
			
			
			window.onpopstate = function(event) {
				
				
				if(event.state === "intermediate") {
					closeinflowoverlays();
					//history.replaceState("inflow","inflow","inflow");
				} else if(event.state === "inflow") {
					closeinflowoverlays();
					window.history.go(-1);
					
				} else if (event.state === "survey") {
					removeSpinnerMobile();
					var srvyovly = $('#surveyovly');
					
					if (srvyovly.is(':visible')) {
						
						$('#surveyovly').addClass('hdn');
					}
					
				} else if (event.state === "faqsecclick") {
					removeSpinnerMobile();
					$('#faqsecoverlay').addClass("hdn");
					
				}else if (event.state === "selcallme") {
					$.when($("#callusoverlay").remove()).then((function() {
						$('#mask').remove();
						$("#callmechn").focus();
					})());
					history.replaceState("inflow","inflow","inflow");
				} else if (event.state === "selcallus") {
					$.when($("#callusoverlay").remove()).then((function() {
						$('#mask').remove();
						$("#calluschn").focus();
					})());
					history.replaceState("inflow","inflow","inflow");
				} else {
					var faqovly = $('#faqoverlay');
				
					if (faqovly.is(':visible')) {
						$('#faqoverlay').find('.content').addClass('hdn');
						$('#faqoverlay').addClass('hdn');
						$('body').removeClass("noscroll");
						 setTimeout(function() {
								$("#mfaqclk").focus();
							},100);
						
						
					}
					
					var srvyovly = $('#surveyovly');
					
					if (srvyovly.is(':visible')) {
						
						$('#surveyovly').addClass('hdn');
						 setTimeout(function() {
								$("#mfaqclk").focus();
							},100);
					}
				}	
					
			};
			
			
			function closeinflowoverlays() {
				
				closeVideoOverlay();
				
				
			}
			
			mobilePlaceHolderEl.on("click", '#mfaqclk', function(event) {
				if (window.history && window.history.pushState) {
					history.pushState("inflow", "inflow", "inflow");
				}
				
				openMobileFAQOverlay(options.mobileid);
				setTimeout(function() {
					$.trapKeyboard('#faqoverlay');
					$("#mclsinf").focus();
				},1000);
				
				
				
			});

			
			$("#faqoverlay").on("click", '#msurvey', function(event) {
				if (window.history && window.history.pushState) {
					history.replaceState("survey", "survey", "survey");
					history.pushState("surveyopen", "surveyopen", "surveyopen");
				}
				startSurvey();
			});
			
			$("#faqoverlay").on("keypress", '#msurvey', function(event) {
				if (event.keyCode == 13) {
					if (window.history && window.history.pushState) {
						history.replaceState("survey", "survey", "survey");
						history.pushState("surveyopen", "surveyopen", "surveyopen");
					}
					startSurvey();
				}	
			});

			
			
			ocsinflowoverlays.on("click", '#close', function(event) {
				closeOverlay();
			});

			ocsinflowoverlays.on("click", '#cancel', function(event) {
				closeOverlay();
			});

			parentEl.on("click", '#callus, #callus-i', function(event) {
				previousFocusId= event.target;
				callus();
			});

			mobileParentEl.on("click", '#rmcallclk', function(event) {
				var rmcallclkEl = $('#rmcallclk');
                previousFocusId = event.target;
				rmcallclkEl.parent().removeClass('active');
				$('#rmchatclk').parent().removeClass('active');
				if ($("#callusoverlay").is(':visible') && $("#callustitle").length > 0) {
					closeFAQOverlay();
					closeCallmeOverlay();
					if (typeof options.closeovlycallback == 'function') {
						options.closeovlycallback.call();
					}
					$('#i-callus').removeClass("sel");
					setTimeout(function() {
						window.scrollTo(0, scrolltop);
					}, 50);

					return;

				}

				closeFAQOverlay();
				closeCallmeOverlay();
				if (typeof options.closeovlycallback == 'function') {
					options.closeovlycallback.call();
				}
				rmcallclkEl.parent().addClass('active');
				setTimeout(function() {
					window.scrollTo(0, scrolltop);
				}, 50);
				if (rmcallclkEl.attr("available")) {

					callus();
				} else {
					var status = rmcallclkEl.attr('ur');
					if (status) {
						showCallUsUnavailable(status);
					}
				}
				if (navigator.userAgent.indexOf("MSIE") != -1) {
					setTimeout(function() {
						$('#oly_2').focus();
					},300);
				} else {
					setTimeout(function() {
	                    $('#dlc').focus();
	                },300);
				}	
				if (!$('#i-callus').hasClass("sel"))
					$('#i-callus').addClass("sel");
			});

			mobileParentEl.on("click", '#rmcallmeclk', function(event) {
				
				var rmcallmeclkEl = $('#rmcallmeclk');
				previousFocusId = event.target;
				$('#rmchatclk').parent().removeClass('active');
				if ($("#callusoverlay").is(':visible') && $("#callmetitle").length > 0) {
					closeFAQOverlay();
					closeCallmeOverlay();
					$('#i-callme').removeClass("sel");
					setTimeout(function() {
						window.scrollTo(0, scrolltop);
					}, 50);
					return;
				}

				closeFAQOverlay();
				closeCallmeOverlay();
				if (typeof options.closeovlycallback == 'function') {
					options.closeovlycallback.call();
				}
				rmcallmeclkEl.parent().addClass('active');
				setTimeout(function() {
					window.scrollTo(0, scrolltop);
				}, 50);

				if (rmcallmeclkEl.attr("available")) {
					callme();
				} else {
					var status = rmcallmeclkEl.attr('ur');
					if (status) {
						showCallMeUnavailable(status, null);
					}
				}
				if (navigator.userAgent.indexOf("MSIE") != -1) {
					setTimeout(function() {
						$('#oly_2').attr("tabindex",1);
						$('#oly_2').focus();
					},1000);
				} else {
					setTimeout(function() {
	                    $('#dlc').focus();
	                },1000);
				}
				if (!$('#i-callme').hasClass("sel"))
					$('#i-callme').addClass("sel");
			});

			parentEl.on("click", '#callme,#callme-i', function(event) {
				previousFocusId = event.target;
				callme();
			});

			parentEl.on("click", '#i-callme', function(event) {
				hideFaqVideos();
				hideDsktpSrvy();
				previousFocusId = event.target;
				if (!$('#i-callme').hasClass("sel")) {
					$('#i-callme').addClass("sel");
				}
				if ($(this).attr("available") == "true")
					callme();
				else
					checkCallMeStatus();
			});

			var call, win = null, callevent = null;
			call = function() {
				if (win && win.closed) {
					clearInterval(callevent);
					$('#i-chat').removeClass("sel");
					win = null;
				}
			};
			
			callevent = setInterval(call, 100);
			
			function openchatWindow (t, chaturl) {

				if (options.callingFlow) {
					
					chaturl = chaturl + "&callingFlow=" + options.callingFlow; 
				}
				
				if ($(t).hasClass("clkd")) {
					return;
				}
				
				if(Inflow && options.presentationUserId){
					Inflow.initPresence(options.presentationUserId);
				}
				
				if (myLiveHelpWin!==undefined &&! myLiveHelpWin.closed && myLiveHelpWin.focus){
					myLiveHelpWin.focus();
			       } else {
			    	   $(t).addClass("clkd");
			    	   myLiveHelpWin= window.open(chaturl, "myLiveHelpWin", "width=460,height=665,screenX=50,left=50,screenY=50,top=50,location=no,menubar=no,scrollbars=yes,resizable=yes,status=no");
			    	   setTimeout(function(){
			    		   $(t).removeClass("clkd");
			    		 }, 1000);
			      }
				
			}

			var openChatOnclick = function(t) {
				var serviceArguments = getInflowParameterURL(sectionName, topicid, contextid);
				var reurl = $(t).attr("redirecturl");
				if (reurl && reurl.length > 0) {
					openchatWindow (t,reurl);
				} else {
					
					showChatUnavailable(true);
				}

				var url = options.address + "/chat?callback=?" + serviceArguments + "&channelProvider=" + channelProviderChat;

				$.getJSON(url, function(data) {
					contextid = data.sessionAttributes.contextId;
				});
			};

			parentEl.on("click", '#i-chat', function(event) {
				
				previousFocusId = event.target;
				hideFaqVideos();
				hideDsktpSrvy();
				$('#i-chat').addClass("sel");
				setInterval(call, 100);
				if ($(this).attr("available") == "true")
					openChatOnclick(this);
				else
					showChatUnavailable(false);
			});
			
			
			parentEl.on("click", '#chati, #chatia', function(event) {
				previousFocusId = event.target;
				openChatOnclick(this);
			});

			parentEl.on("click", '#chat,#chat-i', function() {
				openChatOnclick(this);
			});

			mobileParentEl.on("click", '#rmchatclk', function(event) {
				previousFocusId = event.target;
				var rmchatclkEl = $('#rmchatclk');
				rmchatclkEl.parent().addClass('active');
				closeFAQOverlay();
				closeCallmeOverlay();
				if (typeof options.closeovlycallback == 'function') {
					options.closeovlycallback.call();
				}
				setTimeout(function() {
					window.scrollTo(0, scrolltop);
				}, 50);
				rmchatclkEl.parent().addClass('active');
				if (rmchatclkEl.attr("available")) {
					var serviceArguments = getInflowParameterURL(sectionName, topicid, contextid);
					var reurl = $(this).attr("redirecturl");
					if (reurl && reurl.length > 0) {
						openchatWindow (event.target, reurl);
					}
					var url = options.address + "/chat?callback=?" + serviceArguments + "&channelProvider=" + channelProviderChat;;
					$.getJSON(url, function(data) {
						contextid = data.sessionAttributes.contextId;
					});
				} else {

					showChatUnavailable(false);
					if (navigator.userAgent.indexOf("MSIE") != -1) {
						setTimeout(function() {
							$('#oly_2').attr("tabindex",1);
							$('#oly_2').focus();
						},1000);
					} else {
						setTimeout(function() {
		                    $('#dlc').focus();
		                },1000);
					}
				}
			});
			
			function closeVideoOverlay() {
				$.when($("#videooverlay").remove()).then((function() {
					$('#mask').remove();
					$(currentVideoId).focus();
				})());
			}

			function collapseAll() {
				$(".expand").each(function() {
					collapseFAQ(this);
				});
				$(".mexpand").each(function() {
					collapseFAQ(this);
				});
			}

			function collapseFAQ(collapse) {
				var parent = $(collapse).parents('.activefaq');
				
				if ($(collapse).hasClass("mcollapse") ||  $(collapse).hasClass("mexpand")) {
					parent.find('.mquestion').removeClass('expanded');
					parent.find('.mquestion').attr("aria-expanded","false");
				} else {
					parent.find('.question').removeClass('expanded');
					parent.find('.question').attr("aria-expanded","false");
				}	
				parent.find('.answer').addClass('hdn');
				parent.find('.expand').removeClass('hdn');
				parent.find('.expand').css ({"display":"inline-block"});
				parent.find('.collapse').addClass('hdn');
				parent.find('.collapse').css ({"display":"none"});
				parent.find('.mexpand').removeClass('hdn');
				parent.find('.mcollapse').addClass('hdn');
			}

			function expandFAQ(expand) {

				collapseAll();
				$(expand).attr("aria-expanded","true");
				var documentid;
				var parent = $(expand).parents('.activefaq');
				
				if ($(expand).hasClass("mexpand") || $(expand).hasClass("mcollapse")) {
					documentid = parent.find('.mquestion').attr("documentid");
					parent.find('.mquestion').addClass('expanded');
					parent.find('.mquestion').attr("aria-expanded","true");
				} else {
					documentid = parent.find('.question').attr("documentid");
					parent.find('.question').addClass('expanded');
					parent.find('.question').attr("aria-expanded","true");
				}	
				
				parent.find('.answer').removeClass('hdn');
				parent.find('.expand').addClass('hdn');
				parent.find('.collapse').removeClass('hdn');
				parent.find('.collapse').css ({"display":"inline-block"});
				parent.find('.expand').css ({"display":"none"});
				parent.find('.mexpand').addClass('hdn');
				parent.find('.mcollapse').removeClass('hdn');
				parent.find('.db').height(parent.find('.enclose').height());
				trackContent(documentid, false);
			}
			
			
			parentEl.on("click", '.collapse', function(event) {
				collapseFAQ(event.target);
			});
			
			parentEl.on("click", '.expand', function(event) {
				expandFAQ(event.target);
			});
			
			parentEl.on("click", '.mactivefaq', function(event) {
				
				if (($(this).find('.mexpand')).hasClass('hdn')) {
					collapseFAQ($(this).find('.mcollapse'));
				} else {
					expandFAQ($(this).find('.mexpand'));
				}	 
			});
			
			
			parentEl.on("click", '.question', function(event) {
				var question = event.target;
				var parent = $(question).parents('.activefaq');
				if (parent.find('.answer').hasClass('hdn')) {
					expandFAQ(event.target);
				} else {
					collapseFAQ(event.target);
				}
				

			});
			
			
			function positionMobileVideo () {
				
				var videoOverlay = $('#videooverlay'), video = videoOverlay.find("iframe") , oW = $(window);
				
				video.width(oW.width() - 45);
				if (oW.width() <= 600) {
					video.css({
						"height" : 'auto'
					});
				} else {

					video.css({
						"height" : '200'
					});
				}
				$("#mvcancel").html(contentJsonObj.inflowcomponent.cancel);
			}
			
			
			function positionVideo() {
				var videoOverlay = $('#videooverlay'), video = videoOverlay.find("iframe"), innerOverLay = videoOverlay.find("#voly_2"), oW = $(window);
				 
				if(($.browser && $.browser.msie && $.browser.version <= 8.0)){
					$('#vdmclose').hide();
				}

				if (oW.width() <= 650){
					
					video.width(oW.width() - 45);
					if (oW.width() <= 600) {
						video.css({
							"height" : 'auto'
						});
					} else {

						video.css({
							"height" : '200'
						});
						innerOverLay.css({
							"top" : '0'
						});
					}
					videoOverlay.removeClass('hdn').css({
						"height" : '100%',
						"width" : '100%',
						"z-index" : '9999999',
						"position" : 'fixed'
					});
					videoOverlay.find('.closebar').css ({"display":''});
				} else {
					video.width(460).height(300);
					videoOverlay.find('.closebar').css ({"display":'none'});
					videoOverlay.removeClass('hdn').css({
						"height" : innerOverLay.outerHeight(),
						"width" : 500,
						"z-index" : '2002'
					});
					innerOverLay.css({
						"padding" : 20,
						"background-color" : '#FFFFFF',
						"z-index" : '2002'
					});
				}

				var width = videoOverlay.outerWidth(), height = videoOverlay.outerHeight(), client = {
					width : oW.width(),
					height : oW.height()
				};

				var top = Math.round((client.height - height) / 2), left = Math.round((client.width - width) / 2);

				videoOverlay.css({
					"top" : Math.max(top, 0),
					"left" : Math.max(left, 0),
					"position" : 'fixed'
				});

				
				$("#mvcancel").html(contentJsonObj.inflowcomponent.cancel);
				
			}


			parentEl.on("click", ".activevr", function(event) {
				currentVideoId = event.target;
				var vidAnch = $(this).children('a');

				$.when((function() {
					
					$.when($("#videooverlay").remove()).then(function () {
						var data = {};
						data.content = contentJsonObj;
						$(videoTemplate(data)).appendTo(ocsinflowoverlays);
						$('#vdragger').dragger({
							"target" : $('#videooverlay')
						});
					});
				})()).then((function(event) {
					var videooverlay = $('#videooverlay');
					trackContent(vidAnch.attr("documentid"), true);
					videooverlay.find('#videottl').text(vidAnch.text());
					videooverlay.find('#vdsec').html(vidAnch.attr("dest"));
					videooverlay.find('iframe').attr("id", "player").attr("tabindex", "3");
					var src = videooverlay.find("iframe").attr("src");
					
					if(document.location.protocol == "https:") {
						src = src.replace("http:","https:");
					}
					videooverlay.find("iframe").attr("src",src);
					if (!mask()) {
						return;
					}
					positionVideo();
					if (navigator.userAgent.indexOf("MSIE") != -1) {
						setTimeout(function() {
							videooverlay.find('#voly_2-ariadesc').text(vidAnch.text());
							$('#voly_2').focus();
						}, 300);
					} else {
						setTimeout(function() {
							$(window).width() < 600 ? $('#dl').focus() : $('#clsvd').focus();
						}, 300);
					}
				})(event));

			});
			
			
			ocsinflowoverlays.on("click", '.back', function(event) {
				if (window.history && window.history.pushState) {
					window.history.go(-1);
				} else {
					closeinflowoverlays();
				}
			});

			
			
			
			
			
			
			parentEl.on("click", ".mvideoli", function(event) {
				if (window.history && window.history.pushState) {
					var state = history.state;
					history.replaceState("intermediate", "intermediate", "intermediate");
					if (state == 'faqsecopen') {
						history.pushState("video2", "video2", "video2");
					}	else {
						history.pushState("video", "video", "video");
					}
					
					
					
				}	
				currentVideoId = event.target;
				var vidAnch = $(this).find('a');
				
				$.when((function() {
					$.when($("#videooverlay").remove()).then(function () {
						
							var data = {};
							data.content = contentJsonObj;
							$(mvideoTemplate(data)).appendTo(ocsinflowoverlays);
							var width = $(window).width();
							$('#videooverlay').css({
								"left" : width,
								"position" : 'fixed',
								"top" : 0
							});
							
							$('#videooverlay').animate({'left': '0px'}, 200);
							$('#vdragger').dragger({
								"target" : $('#videooverlay')
							});}
					);
				})()).then((function(event) {
					var width = $(window).width();
					$('#videooverlay').css({
						"left" : width,
						"position" : 'fixed',
						"top" : 0
					});
					
					$('#videooverlay').animate({'left': '0px'}, 200);
					var videooverlay = $('#videooverlay');
					$(videooverlay).addClass("mobile");
					trackContent(vidAnch.attr("documentid"), true);
					videooverlay.find('#videottl').text(vidAnch.text());
					videooverlay.find('#vdsec').html(vidAnch.attr("dest"));
					videooverlay.find('iframe').attr("id", "player").attr("tabindex", "2");
					var src = videooverlay.find("iframe").attr("src");
					
					if(document.location.protocol == "https:") {
						src = src.replace("http:","https:");
					}
					videooverlay.find("iframe").attr("src",src);
					videooverlay.find('#voly_2-ariadesc').text(vidAnch.text());
					if (!mask()) {
						return;
					}
					positionVideo();
					
				})(event));
					
				setTimeout(function() {
					$("#videooverlay").find(".voly").focus();
					$("#faqoverlay").attr("aria-hidden","true");
				},2000);
				
			});
			
			
						
			function handleSelect(inflowjson, isMobileSecLink) {

				var serviceArguments = getInflowParameterURL();

				if (isProactiveDisplayed == true) {
					serviceArguments = getInflowParameterURL(undefined,undefined,undefined,true);
				} else {
					 serviceArguments = getInflowParameterURL();
				}	 
				
				
				var jsonobj = $.parseJSON(inflowjson);
				if(jsonobj!=null && jsonobj.length!=0) {
					if ((jsonobj.property_name == "sectionId" && jsonobj.property_value != sectionName) || isMobileSecLink) {
						serviceArguments = serviceArguments + "&sectionId=" + jsonobj.property_value;
						sectionName = jsonobj.property_value;
						sectionTitle = jsonobj.section_title;
						callInflowService(serviceArguments,isMobileSecLink);
					}
				}	
			}

			$('div[inflow]').find("input,select,label").focus(function(event) {

				if (options.deviceId == 'DEVICE_DESKTOP') {
					var inflowjson = $(this).parents('div[inflow]').attr("inflow");
					handleSelect(inflowjson,false);
				}

			});
			
			
