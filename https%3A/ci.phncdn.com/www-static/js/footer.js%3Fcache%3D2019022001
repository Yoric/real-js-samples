function sendMessageToChildren(){var e=document.querySelector("body");newWidth=parseInt(e.clientWidth),document.querySelector("#main_top_right").contentWindow.postMessage({current_width:newWidth,cutoff_width:"1600"},"*")}var body=document.querySelector("body"),bodyHeight=parseInt(body.clientHeight),bodyWidth=parseInt(body.clientWidth),windowWidth,boxFade,marginValue;window.onresize=function(){try{sendMessageToChildren()}catch(e){}},handleChildResponse=function(e){"ad_loaded"==e.data&&sendMessageToChildren()};var addListener=window.addEventListener||window.attachEvent;addListener("message",handleChildResponse,!1),top===self||top.location.href.match(/^(http|https):\/\/(www.)*pornmd.com/gi)||(top.location.href=self.location.href);var sponsorModal=document.getElementById("sponsor-notification"),bodyVar=document.getElementsByTagName("body")[0];if(sponsorModal){var sponsor=new MG_Modal({content:sponsorModal,className:"sponsorNotificationWrapper",closeButton:!1,maxWidth:"750px"});sponsor.openModal()}