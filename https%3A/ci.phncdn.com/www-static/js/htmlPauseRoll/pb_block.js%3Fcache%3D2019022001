var Pb_block={options:{},timer:null,init:function(a){Pb_block.params=a},closeButtonAdBlock:function(){Pb_block.params.showBlock=!1,Pb_block.closeAdBlock()},closeAdBlock:function(){var a=this;clearTimeout(a.timer),Pb_block.params.adContainer&&Pb_block.params.adContainer.setAttribute("style","display:none;"),Pb_block.params.iframe_block&&Pb_block.params.iframe_block.setAttribute("src","")},openAdBlock:function(){1==Pb_block.params.showBlock&&(Pb_block.params.iframe_block&&Pb_block.params.iframe_block.setAttribute("src",Pb_block.params.iframe_src),Pb_block.params.adContainer&&(this.timer=setTimeout(function(){Pb_block.params.adContainer.setAttribute("style","display:block;")},100))),null!==Pb_block.params.getEntryCode&&(!1===Pb_block.params.isPostRoll?Pb_block.params.getEntryCode.setAttribute("data-entrycode","RemAds-pauseRoll"):Pb_block.params.getEntryCode.setAttribute("data-entrycode","RemAds-postRoll"))},resumeButton:function(){1==Pb_block.params.isPostRoll?(MHP1138.seek("playerDiv_"+Pb_block.params.ad_player_id,0,!0),Pb_block.params.isPostRoll=!1):MHP1138.play("playerDiv_"+Pb_block.params.ad_player_id)}};