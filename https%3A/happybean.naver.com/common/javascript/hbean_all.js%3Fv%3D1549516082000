hbean.scrap_blog = function (num)
{
var frm = document.scrapform[num];

if (frm.source_title == null) {
	frm = document.scrapform;
}

if (frm.source_title.value.split(" ").join("")=="") {
	alert("출처가 누락되었습니다. 다시 시도해주세요.");
	return false;
}

if (frm.title.value.split(" ").join("")=="") {
	alert("제목이 누락되었습니다. 다시 시도해주세요.");
	return false;
}

frm.source_contents.value = frm.source_contents.value + frm.source_footer.value;

window.open("","scrapPop","width=205,height=208");
frm.action = "https://blog.naver.com/post/scrap/scrapPop.jsp";
frm.target = "scrapPop";
frm.submit();
}

