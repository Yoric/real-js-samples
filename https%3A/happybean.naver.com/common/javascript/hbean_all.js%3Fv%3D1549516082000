//- File Name   : hbean_net.js
//- Description :
var hbeannet =
{
open : function(urlparam, target, width, height, scrollbars)
{
	var loc = new url(urlparam);
	var path = loc.path;
	if(path=="")
	{
		return;
	}
	if(loc.host!=document.domain)
	{
		path = loc.scheme + "://" + loc.host + path;
	}

	var frm = document.createElement("FORM");
	frm.action = path;
	frm.method = "GET";

	if(typeof(target) != "undefined")
	{
		if(target != "_blank")
		{
			var features = "menubar=no,toolbar=no";

			var winW = 300;
			var winH = 500;

			if(typeof(width) != "undefined") {
				winW = width;
			}
			if(typeof(height) != "undefined") {
				winH = height;
			}

			features += ",width=" + winW;
			features += ",height=" + winH;

			var windowX = Math.ceil( (window.screen.width  - winW) / 2 );
			var windowY = Math.ceil( (window.screen.height - winH) / 2 );

			features += ",left=" + windowX;
			features += ",top=" + windowY;

			if(typeof(scrollbars) != "undefined")
				features += ",scrollbars=" + scrollbars;
			else
				features += ",scrollbars=no";

			window.open("about:blank", target, features);
		}
		frm.target = target;
	}

	document.body.appendChild(frm);
	if(loc.query!="")
	{
		var params = new Array();
		var query = loc.query.split("&");
		for(i=0; i < query.length; i++)
		{
			var poz = query[i].indexOf("=");
			if(poz == -1) continue;
			var s = query[i].substr(0, poz);
			var v = query[i].substr(poz+1, query[i].length);
			if(s == "") continue;
			params.add({name:s,value:v});
		}
		for(i=0; i < params.length; i++)
		{
			var input = document.createElement("INPUT");
			input.type = "hidden";
			input.name = params[i].name;
			input.value = params[i].value;
			frm.appendChild(input);
		}
	}
	frm.submit();
},

getFormAttribute : function(id)
{
	var inputParam = "";
	var selectParam = "";
	var textAreaParam = "";

	var inputFrm		=	document.getElementById(id);
	var inputNodes		=	inputFrm.getElementsByTagName("input");
	var selectNodes		=	inputFrm.getElementsByTagName("select");
	var textareaNods	=	inputFrm.getElementsByTagName("textarea");

	if(inputNodes != null && inputNodes.length != 0)
	{
		inputParam = this.createParamforInput(inputNodes);
	}
	if(selectNodes != null && selectNodes.length != 0)
	{
		selectParam = this.createParam(selectNodes);
	}
	if(textareaNods != null && textareaNods.length != 0)
	{
		textAreaParam = this.createParam(textareaNods);
	}

	var resultParam = inputParam + selectParam + textAreaParam;

	return resultParam;
},

createParam : function(obj)
{
	var param = "";
	var size = obj.length;
	for(var i = 0; i < size; i++)
	{
		param = param + "&" + obj[i].name + "=" + encodeURIComponent(obj[i].value);
	}
	return param;
},

createParamforInput : function(obj)
{
	var param = "";
	var size = obj.length;
	for(var i = 0; i < size; i++)
	{
		if(obj[i].type == "checkbox" || obj[i].type == "radio")
		{
			if(obj[i].checked)
			{
				param = param + "&" + obj[i].name + "=" + encodeURIComponent(obj[i].value);
			}
		}
		else if(obj[i].type == "hidden" || obj[i].type == "text" || obj[i].type == "password")
		{
			param = param + "&" + obj[i].name + "=" + encodeURIComponent(obj[i].value);
		}
	}
	return param;
}
};

//- File Name   : hbean_prohibitword.js
//- Description :
function ProhibitWord() {
	this.banword_wrap = null;
	this.callBack = null;
	this.callBackObject = null;
	this.callBackParams = null;
	this.position = null;
	this.type = null;
	this.x = 150;
	this.y = 0;

	this.html = "<div id=\"bw_header\">" +
			"<h1>금칙어가 포함되어 있습니다</h1>" +
			"</div>" +
			"<div id=\"bw_content\">" +
			"<p class=\"bw_desc5\">작성하신 <strong id=\"ugcType\"></strong>되어 있습니다.</p>" +
			"<div class=\"bw_bx1\">" +
			"<div class=\"bw_bx1_shadow\">" +
			"<strong>해당되는 금칙어</strong>" +
			"<a href=\"도움\" onclick=\"return prohibitWord.helpProhibit();\"><img src=\"https://ssl.pstatic.net/static/common/popup/ico_help.gif\" alt=\"도움말\" width=\"14\" height=\"14\"></a>" +
			"<div id=\"words\" class=\"bw_bx1_shadow_in\"></div>" +
			"<div id=\"helpProhibit\" class=\"layer_bubble\" style=\"top:32px; left:83px; display:none;\">" +
			"<div class=\"layer_content\">" +
			"<div style=\"padding:10px 0 5px 4px; *padding:9px 0 6px 4px;\">" +
			"<p>네이버에서는 서비스 이용자 보호를 위해 금칙어 제도를<br>" +
			"운영하고 있습니다.<br></p>" +
			"<p class=\"bw_orange\">금칙어란 불법성, 음란성, 욕설로 의심되어<br>" +
			"자동으로 등록이 제한된 특정 단어나 URL을 의미합니다.</p>" +
			"</div>" +
			"<span class=\"rt_iefix\"></span>" +
			"</div>" +
			"<span class=\"rt\">" +
			"</span>" +
			"<span class=\"lb\"></span><span class=\"rb\"></span><span class=\"arrow top\"></span>" +
			"</div>" +
			"</div>" +
			"</div>" +
			"<div class=\"bw_bx2\">" +
			"<p class=\"bw_desc3\">금칙어를 삭제하신 후 다시 글을 등록하시기 바랍니다.<br>" +
			"건전한 인터넷 문화 조성을 위해 회원님의 적극적인 협조를 부탁드립니다.</p>" +
			"</div>" +
			"<p class=\"bw_desc4\">더 궁금하신 사항은 <a href=\"https://help.naver.com/ops/step2/faq.nhn?fcatid=12443\" target=\"helpCenter\">고객센터</a>로 문의하시면 자세히 알려드리겠습니다.</p>" +
			"</div>" +
			"<div id=\"bw_btn_footer\">" +
			"<a href=\"javascript:prohibitWord.closeProhibitAlert();\"><img src=\"https://ssl.pstatic.net/happyimg2/img3/happybean/img/btn_confirm4.gif\" width=\"48\" height=\"23\" alt=\"확인\"></a>" +
			"<a href=\"javascript:prohibitWord.closeProhibitAlert();\" class=\"btn_close\"><img src=\"https://ssl.pstatic.net/happyimg2/img3/happybean/img/btn_close2.gif\" width=\"15\" height=\"14\" alt=\"닫기\"></a>" +
			"</div>";

	this.alterProhibitWord = function(json) {
		this.banword_wrap = document.getElementById("banword_wrap");
		if (this.banword_wrap == null) {
			this.banword_wrap = document.createElement("div");
			this.banword_wrap.id = "banword_wrap";
			this.banword_wrap.style.position = "absolute";
			this.banword_wrap.style.zIndex = "1000";
			this.banword_wrap.innerHTML = this.html;

			var next = null;
			var viewPoint = null;

			if (this.position == null) {
				viewPoint = document.getElementById("content_w");
			} else {
				viewPoint = document.getElementById(this.position);
			}

			var obj = viewPoint;
			var curleft = 0;
			if (obj.offsetParent) {
				while (obj.offsetParent) {
					curleft += obj.offsetLeft;
					obj = obj.offsetParent;
				}
			} else if (obj.x) {
				curleft += obj.x;
			}

			obj = viewPoint;
			var curtop = 0;
			if (obj.offsetParent) {
				while (obj.offsetParent) {
					curtop += obj.offsetTop;
					obj = obj.offsetParent;
				}
			} else if (obj.y) {
				curtop += obj.y;
			}

			this.banword_wrap.style.left = curleft + this.x + "px";
			this.banword_wrap.style.top = curtop + this.y + "px";
			var topDiv = document.getElementById("wrap");

			// IFRAME 안에 있는 경우 topDiv를 찾을 수 없다.
			if(topDiv == null) {
				alert("입력하신 내용에 사용이 제한된 단어가 포함되어 있습니다.");
				return false;
			}
			topDiv.parentNode.insertBefore(this.banword_wrap, topDiv);
		}

		var ugcType = document.getElementById("ugcType");
		var words = document.getElementById("words");
		var type = "";
		var ugcTypes = json.ugcType;

		for ( var i = 0; ugcTypes.length > i; i++) {
			if (i > 0) {
				type = type + ", ";
			}

			type = type + ugcTypes[i].ugcTypeId;
		}

		ugcType.innerHTML = type + "에 금칙어가 포함";
		words.innerHTML = type + "에 금칙어가 포함";
		words.innerHTML = json.prohibitWords;
		this.banword_wrap.style.display = "";
		return false;
	};

	this.closeProhibitAlert = function() {
		window.location.reload();
	};

	this.helpProhibit = function() {
		try {
			var helpProhibit = document.getElementById("helpProhibit");
			if (helpProhibit.style.display != "none") {
				helpProhibit.style.display = "none";
			} else {
				helpProhibit.style.display = "";
			}
		} catch (e) {
		} finally {
			return false;
		}
	};

	this.checkProhibit = function(xsender, context) {
		try {
			var x = xsender.getX();

			if (x.status == 200) {
				var str = x.responseText;

				if (str) {
					str = str.trim();
					str = str.replace(/\0/g,'');
				}
				var json = JSON.parse(str);
				if (json.prohibitWord != true) {
					switch (context.type) {
					case 1: // 1은 검색시
						alert("검색하신 키워드에 사용이 제한된 단어가 포함되어 있습니다.");
						break;
					case 2: // 2은 특정 상황 - 팝업
						alert("입력하신 내용에 사용이 제한된 단어가 포함되어 있습니다.");
						break;
					default:
						context.alterProhibitWord(json);
						break;
					}

					return;
				}

				context.invoke();
			}
		} catch (e) {
			alert(e.message);
		}
	};

	this.invoke = function() {
		if (this.callBackObject != undefined) {
			this.callBackObject.invoke(this.callBackParams);
		} else if (context.callBack != null) {
			if (this.callBackObject.callBack.tagName == "FORM") {
				this.callBackObject.callBack.submit();
			} else {
				this.callBackObject.callBack(context.callBackParams);
			}
		}
	};

	this.prohibitWordFilter = function(params, callBack, callBackParams) {
		try {
			var filterUrl = "/wcomp/ProhibitWordFilter.json";
			this.callBack = callBack;
			this.callBackParams = callBackParams;
			ajaxSender.send(filterUrl, params, this.checkProhibit, this);
		} catch (e) {
			alert(e.message);
		} finally {
			return false;
		}
	};

	this.prohibitWordFilter4Object = function(params, callBackObject, position) {
		try {
			var filterUrl = "/wcomp/ProhibitWordFilter.json";
			this.callBackObject = callBackObject;
			this.position = position;
			ajaxSender.send(filterUrl, params, this.checkProhibit, this);
		} catch (e) {
			alert(e.message);
		} finally {
			return false;
		}
	};
	
//	this.mvcProhibitWordFilter4Object = function(params, callBackObject, position) {
