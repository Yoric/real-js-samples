function Widget(){
	this.userPopUpUi = "userPopUp";
	this.brandPopUpUi = "brandPopUp";
	this.userFundPopUpUi = "userFundPopUp";	//콩저금통 용
	this.brandFundPopUpUi = "brandFundPopUp";	//콩저금통 용
	this.postScrapPopUpUi = "postScrapPopUp";
	this.clubTidingsScrapPopUpUi = "clubTidingsScrapPopUp";
	this.fundScrapPopUpUi = "fundScrapPopUp";
	this.fundScrapPopUpUi2 = "fundScrapPopUp2";
	this.volunteerScrapPopUpUi = "volunteerScrapPopUp";
	this.sharePostscriptScrapPopUpUi = "sharePostscriptScrapPopUp"; //나눔 내역 후기 스크랩
	this.useLimitPopUpUi ="useLimitPopUp"
	this.itemKey = "itemKey";
	this.setUp = function(){
		this.setUpUserPopUpUi(); // 유저팝업
		this.setUpBrandPopUpUi(); // 브랜드로그팝업
		this.setUpUserFundPopUpUi(); // 콩저금통유저팝업
		this.setUpBrandFundPopUpUi(); // 콩저금통브랜드로그팝업
		this.setUpPostScrapPopUpUi(); // 포스트스크랩
		this.setUpClubTidingsScrapPopUpUi();
		this.setUpFundScrapPopUpUi(); // 콩저금통 스크랩
		this.setUpFundScrapPopUpUi2(); // 콩저금통 스크랩2
		this.setUpVolunteerScrapPopUpUi(); // 자원봉사 스크랩
		this.setUpSharePostscriptScrapPopUpUi();//나눔 내역 후기 스크랩
		this.setUpUseLimitPopUpUi();//이용제한 레이어
	}

	this.userCallBack = function(event, element, target)
	{
	    var id = element.id;
	    var menuid = id.substring(id.indexOf("item"));

        var userId = target.getAttribute(widget.itemKey);
        if(menuid == "itembrand")
	    {
	    	happylink.openBrandlog(userId);
	    }
	    if(menuid == "iteminfo")
	    {
	    	happylink.openPersonalInfo(userId);
	    }
	    else if(menuid == "itemblog")
	    {
	    	happylink.openBlog(userId);

	    }
	    else if(menuid == "itemgift")
	    {
	    	happylink.openGiftCong(userId);
	    }
	    else if(menuid == "itemrdona")
	    {
	    	happylink.openNaverNote(userId);
	    }
	}

