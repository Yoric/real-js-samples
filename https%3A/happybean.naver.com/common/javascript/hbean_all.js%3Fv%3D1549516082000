//  - Description : declare hbean global object
//                  -----------------------------------------------------------------------
//                  If you want to use the onload-call-queue, append the follow statements.
//                    window.onload = hbean.callLoadListener;
//                    hbean.observer.add("script-statements");
//                  -----------------------------------------------------------------------
//                  If you want to use the default input text message, refer a follow example.
//                    <input type="text" onfocus="hbean.onfocus(this)" onblur="hbean.onblur(this)"
//                           abbr="input a your message" value="input a your message" />
//                  -----------------------------------------------------------------------
//                  If you want to activate embeded objects when load a page,
//                  call the hbean.refreshObject function. Let's see a follow example.
//                    <div id='myobject'><object xxxx></object></div>
//                    <script type="text/javascript">hbean.refreshObject('myobject');</script>
//                  -----------------------------------------------------------------------
var envArray = ["local.", "dev.", "qa.", "pub.", "nokdu.", "gangnang.", "wandoo."
    , "dev-fund.", "dev-billing.", "dev-api.", "dev-biz.", "dev-campaign.",
    , "qa-fund.", "qa-billing.", "qa-api.", "qa-biz.", "qa-campaign.",
    , "pub-fund.", "pub-billing.", "pub-api.", "pub-biz.", "pub-campaign.",
    , "local-fund.", "local-billing.", "local-api.", "local-biz.", "local-campaign."];
var mobileEnvArray = ["dev-m.", "dev-m-fund.", "dev-m-event.", "dev-m-campaign."
	, "qa-m.", "qa-m-fund.", "qa-m-event.", "qa-m-campaign."
	, "pub-m.", "pub-m-fund", "pub-m-event.", "pub-m-campaign."];

var hbean = {
	isWin : (navigator.appVersion.indexOf("Win") != -1)
	,
	IE4 : false
	,
	NS4 : false
	,
	// instantly support to load object tag
	refreshObject : function(id)
	{
		var d = document.getElementById(id);
		if(d != undefined)
		{
			var p = d.parentNode;
			var w = document.createElement(d.tagName);
			w.innerHTML = d.innerHTML;
			p.insertBefore(w, d);
			p.removeChild(d);
		}
	}
	,
	// support onload queue calling
	observer : null
	,
	callLoadListener : function()
	{
		if(hbean.observer==null) return;
		hbean.observer.call();
	}
	,
	// default input message
	onfocus : function(e)
	{
		try{
			var oValue = e.value.replace(/\r/g, '');
			var oAbbr = e.getAttribute("abbr").replace(/\r/g, '');
			if (oValue == oAbbr) {
				e.value = "";
			}
		}catch(e){}
	}
	,
	onblur : function(e)
	{
		try{
			if (e.value == "") {
				e.value = e.getAttribute("abbr");
			}
		}catch(e){}
	}
	,
	getCookie : function(c)
	{
		var found = false;
		var start, end;
		var i = 0;

		while (i <= document.cookie.length) {
			start = i;
			end   = start + c.length;
			if (document.cookie.substring(start, end) == c) {
				found = true;
				break;
			}
			i++;
		}
		if (found == true)
		{
			start = end+1;
			end   = document.cookie.indexOf(";", start);
			if (end < start) {
				end = document.cookie.length;
			}
			return document.cookie.substring(start, end);
		}
		else
		{
			return "";
		}
	}
};

if(document.all)
{
	hbean.IE4 = true;
	hbean.NS4 = false;
}
else
{
	hbean.IE4 = false;
	hbean.NS4 = true;
}

function LoadEventListener()
{
	if(hbean.observer!=null)
	{
		return hbean._loadlistener;
	}

	this.listener = new Array();
	this.add = function(expression)
	{
		this.listener[this.listener.length] = expression;
	}
	this.call = function()
	{
		var length = this.listener.length;
		for(var i=0;i<length;i++)
		{
			eval(this.listener[i]);
		}
	}
	hbean.observer = this;
}

new LoadEventListener();

//- File Name   : hbean_ajax.js
//- Description : It support to call to XML-HTTP-Request.
//              MUST import hbean_common.js file before this file.
//              MUST import hbean_array.js file before this file.
//              default syncmode is synchronization(syncmode is 1).
function AjaxSender(syncmode)
{
//
// private member variables
//
this.syncmode = 1;
this.xmlhttp = null;
this.headers = new Array();

this.getX = function()
{
	if(this.xmlhttp!=null) return this.xmlhttp;

	var x=null;

	if(window.ActiveXObject){
		x = new ActiveXObject("Msxml2.XMLHTTP");
		if(!x){
			x = new ActiveXObject("Microsoft.XMLHTTP");
		}
	}else if(window.XMLHttpRequest){
		x = new XMLHttpRequest();
	}
	return this.xmlhttp = x;
};

this.encode = function(s)
{
	return window.encodeURIComponent?encodeURIComponent(s):escape(s);
};

this.send = function(url, data, callback, context)
{
	var x = this.getX();
	if(!x || url && !callback)
	{
		(new Image()).src = url;
		this.headers = new Array();
		return;
	}

	var method = (data)?"POST":"GET";
	this.headers.add({head:"ajax", value:"true"});
	if(method=="POST")
	{
		this.headers.add({head:"Content-Type", value:"application/x-www-form-urlencoded;charset=utf-8"});
	}

	x.open(method, url, (this.syncmode==1)?false:true);
	if(this.headers.length>0)
	{
		// append additional header
		for(iheader=0;iheader<this.headers.length;iheader++)
		{
			var header = this.headers[iheader];
			x.setRequestHeader(header.head, header.value);
		}
	}
	this.headers = new Array();

	var bsetevent = false;
	if(hbean.IE4)
	{
		if(callback) bsetevent = true;
	}
	else
	{
		bsetevent = (this.syncmode==0 && typeof(callback)=="function" );
	}

	if(bsetevent)
	{
		var xsender = this;
		x.onreadystatechange = function(){
			if(x.readyState==4) callback(xsender, context);
		};
	}

	if(method=="POST")
	{
		x.send(data);
	}
	else
	{
		x.send('');
	}

	if(!bsetevent && typeof(callback)=="function")
	{
		callback(this, context);
	}
};

if(typeof(syncmode)!="undefined") this.syncmode = syncmode;
}

/*
 * Deprecated ajax
 * Andorid 4.3 + 갤럭시 S3 에 ajax로 선언된 내용이 있음.
 * 사용할 때 생성해서 쓰거나 다른 변수로 할당해서 사용할 것
 * ajax 대신에 ajaxSender 사용
 */
var ajax = new AjaxSender(0);

var ajaxSender = new AjaxSender(0);

//- File Name   : hbean_array.js
//- Description :
Array.prototype.add = function(element)
{
len = this.length;
this[len] = element;
}

Array.prototype.remove = function(idx)
{
if(typeof(idx) != "number")
{
	for(i=0; i<this.length; i++)
	{
		if(this[i]==idx)
		{
			idx = i;
			break;
		}
	}
}

var temp = new Array();
var j=0;

for(i=0; i<idx; i++) {
	temp[j++] = this[i];
}

for(i=idx+1; i<this.length; i++) {
	temp[j++] = this[i];
}
this.length=0;
for(i=0; i<temp.length; i++) {
	this[i] = temp[i];
}
}


//- File Name   : hbean_base64.js
//- Description :
var base64 =
{
_setorginal : "ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789+/"
,
_sethttp : "ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789-_"
,
_encode: function(bytes, set)
{
	if(! bytes instanceof Array) return "";

	var idx = 0;
	var len = bytes.length;
	var b = new Array(0,0,0,0);
	var returnStr = "";
	var rstr;
	for(;idx<len;idx+=3)
	{
		b[3]=b[2]=0x40;
		b[0] = (bytes[idx] >> 2);
		b[1] = (bytes[idx] & 0x03) << 4;
		if(idx+1<len)
		{
			b[1] |= (bytes[idx+1] >> 4);
			b[2] = (bytes[idx+1] & 0x0f) << 2;
			if(idx+2<len)
			{
				b[2] |= (bytes[idx+2] >> 6);
				b[3] = (bytes[idx+2] & 0x3f);
			}
		}
		rstr="";
		for(i=0;i<b.length;i++)
		{
			if(b[i]>0x3f) rstr+="=";
			else rstr+=set.charAt(b[i]);
		}
		returnStr+=rstr;
	}
	return returnStr;
}
,
_decode: function(s, set)
{
	var _decode = function(param)
	{
		var bytes = new Array();
		var ch = new Array();
		var idx = 0;
		for(i=0;i<param.length;i+=4)
		{
			bytes[idx] = (param[i] << 2) & 0x00ff;
			if(i+1 >= param.length) break;
			bytes[idx++] |= (param[i+1] >> 4);

			bytes[idx] = (param[i+1] << 4) & 0x00ff;
			if(i+2 >= param.length) break;
			bytes[idx++] |= (param[i+2] >> 2);

			bytes[idx] = (param[i+2] << 6) & 0x00ff;
			if(i+3 >= param.length) break;
			bytes[idx++] |= param[i+3];
		}
		return bytes;
	}

	var str = String(s);
	var len = str.length;
	var b;
	var bytes = new Array();
	var idx = 0;
	for(i=0;i<str.length;i++)
	{
		b = set.indexOf(str.substr(i,1));
		if(b >= 0)
		{
			bytes[idx++] = b;
		}
	}
	return _decode(bytes);
}
,
encode: function(bytes)
{
	return base64._encode(bytes, base64._setorginal);
}
,
decode: function(bytes)
{
	return base64._decode(bytes, base64._setorginal);
}
,
encode4http: function(bytes)
{
	return base64._encode(bytes, base64._sethttp);
}
,
decode4http: function(bytes)
{
	return base64._decode(bytes, base64._sethttp);
}
}

// - File Name   : hbean_calendar_core.js
// - Description :
function calendar_core(){


	//USE_ABLE

	this.calculationPeriod = function(fromday, today)
	{
		return Math.floor(today.valueOf()/(24*60*60*1000)- fromday.valueOf()/(24*60*60*1000))
	}

	this.makezero = function(data)
	{
		if(Number(data) < 10)
		{
			return "0" + data;
		}
		else
		{
			return data;
		}
	}
	//USE_ABLE

	this.caldata = function(div,method,year,month,day) {
		this.year_script = "";
		this.month_script = "";
		this.date_script = "";
		this.year_view = true;
		this.month_view = true;
		this.date_view = true;

		this.monid;
		this.dayid;
		this.div = div;

		this.year = year;
		this.month = month;
		this.day = day;

		this.startposition;
		this.endday;
		this.method = method;

		this.prevmonth;
		this.prevyear;

		this.nextmonth;
		this.nextyear;

		this.startselday = 0;
		this.endselday = 0;
		this.totcol = 0;

		this.bgcolor = "#FFFFFF";
		this.selcolor = "#a0ca50";
		this.selfontcolor = "#FFFFFF";
		this.focuscloor = "#E3E4E5";

		this.fromdate =  "";
		this.todate =  "";
	};
	this.calendardata = [];



	this.cal_tot = 0;
	this.selcolor = "#FFEF64";

	this.addcaldata = function(div,method,year,month,day){
		this.calendardata[this.calendardata.length] = new this.caldata(div,method,year,month,day);
		return this.calendardata.length -1;

	}

	this.cleanColor = function(num, id)
	{
		for(i=1;i<=this.calendardata[num].totcol;i++){
			document.getElementById(id + i).style.backgroundColor = this.calendardata[num].bgcolor;

		}
	}
	this.cleanMonthColor = function(num, id)
	{

		for(i=1;i<=12;i++){
			if( i == this.calendardata[num].month) document.getElementById(id + i).style.backgroundColor = this.calendardata[num].selcolor;
			else document.getElementById(id + i).style.backgroundColor = this.calendardata[num].bgcolor;
		}
	}


	this.setWeek = function(num,id)
	{
		keyid = id.substr(0,7);
		this.cleanColor(num,keyid);

		if((id.substr(7, id.length) % 8) == 1){
			start = Number(id.substr(7, id.length));

			this.calendardata[num].startselday = start + 1;
			this.calendardata[num].endselday = start + 7;


			for(i=1;i<8;i++){
				document.getElementById(keyid + (1*start + i) ).style.backgroundColor = this.calendardata[num].selcolor;
			}
		}
		this.getDay(num);
	}



	this.getDay = function(num){

		obj = this.calendardata[num];

		obj.fromdate = this.calday(num, obj.startselday);
		obj.todate = this.calday(num, obj.endselday);
	}
	this.calday = function(num,position){

		if( position <= obj.startposition ){
			return obj.prevyear+"."+this.makezero(obj.prevmonth)+"."+this.makezero(document.getElementById(obj.dayid +position).outerText);
		}else if( position > obj.endday + obj.startposition + obj.totcol / 8-1){
			return obj.nextyear +"."+this.makezero(obj.nextmonth)+"."+this.makezero(document.getElementById(obj.dayid +position).outerText);
		}else{
			return obj.year+"."+this.makezero(obj.month)+"."+this.makezero(document.getElementById(obj.dayid +position).outerText);
		}

	}



	this.setday = function(num,year,month,id){
		keyid = id.substr(0,7);
		this.cleanColor(num,keyid);
		document.getElementById(id).style.backgroundColor = this.calendardata[num].selcolor;
		this.calendardata[num].startselday = Number(id.substr(7, id.length));
		this.calendardata[num].endselday = Number(id.substr(7, id.length));
		this.getDay(num);
	}

	this.focusbgcolor = function(num,obj){
		obj.style.backgroundColor = this.calendardata[num].focuscloor;

	}
	this.focusoutbgcolor = function(num,obj,index){

		if(index >= this.calendardata[num].startselday && index <=this.calendardata[num].endselday){
			obj.style.backgroundColor = this.calendardata[num].selcolor;
		}else{
			obj.style.backgroundColor = this.calendardata[num].bgcolor;
		}
	}

	this.focusoutMonthbgcolor = function(num,obj){
		id = obj.id;
		index = id.substr(5, id.length);
		if(index == this.calendardata[num].month) obj.style.backgroundColor = this.calendardata[num].selcolor;
		else obj.style.backgroundColor = this.calendardata[num].bgcolor;


	}

	this.changeyear = function(num,year){
		this.calendardata[num].year = year;
		this.calendardata[num].year_script = this.makeyear(num);
		this.calendardata[num].month_script = this.makemonth(num);
		this.calendardata[num].day = 0;
		this.calendardata[num].date_script = this.makeday(num);
		this.setcalendar(num);

	}

	this.changemonth = function(num,month){

		this.calendardata[num].month = month;
		this.calendardata[num].day = 0;
		this.calendardata[num].month_script = this.makemonth(num);
		this.calendardata[num].date_script = this.makeday(num);
		this.setcalendar(num);

	}

	this.makeyear = function(num){
		if(this.calendardata[num].year_view)
		{

			html = "<tr><td width=100%>";
			html = html + "<table width=100% height=30 border=0 cellspacing=0 cellpadding=0><tr>";
			html = html + "<td width=10%  style=\"cursor:hand;\" onClick=\"cal_core.changeyear("+num+","+(this.calendardata[num].year-1)+");\" ><img src=\"https://ssl.pstatic.net/happyimg2/img2/project_page/2008/crm/prew.gif\" alt=\"이전년도\"></td>";
			html = html + "<td align=\"center\" ><span class=\"cata_year_num\">"+this.calendardata[num].year+"</span></td>";
			html = html + "<td width=10%  style=\"cursor:hand;\" onClick=\"cal_core.changeyear("+num+","+(this.calendardata[num].year+1)+");\" ><img src=\"https://ssl.pstatic.net/happyimg2/img2/project_page/2008/crm/next.gif\" alt=\"다음년도\"></td></tr></table>";
			html = html + "</td></tr>";
			return html;
		}
		else
		{
			return "";
		}
	}


	this.makemonth = function(num){
		if(this.calendardata[num].month_view){
			caldata = this.calendardata[num];
			if(num.length = 1){
				caldata.monid = "M_0"+num+"_";
			}else{
				caldata.monid = "M_"+num+"_";
			}
			style = " style=\"cursor:hand;\" onmouseover=\"cal_core.focusbgcolor("+num+",this)\" onmouseout=\"cal_core.focusoutMonthbgcolor("+num+",this)\"  ";
			script = "" +
					"<tr>" +
					"	<td width=100%>" +
					"		<table border=0 cellpadding=3 cellspacing=0 align=center>" +
					"			<colgroup>"+
					"				<col width=\"10px\" align=right><col width=\"10px\" align=right><col width=\"10px\" align=right><col width=\"10px\" align=right>"+
					"				<col width=\"10px\" align=right><col width=\"10px\" align=right><col width=\"10px\" align=right><col width=\"10px\" align=right>"+
					"				<col width=\"10px\" align=right><col width=\"10px\" align=right><col width=\"10px\" align=right><col width=\"0px\" align=right>"+
					"			</colgroup>" +
					"			<tr>";
			for (i=1;i<=12;i++){

				if(!this.calendardata[num].date_view)
				{
					method = "cal_core.changemonth("+num+","+i+");" + caldata.method+"('"+caldata.year+"."+i+"' ); ";
				}else{
					method = "cal_core.changemonth("+num+","+i+");";
				}
				if(i == caldata.month) 	script = script+ "<td class=\"cata_mon_off\" id=\""+caldata.monid+i+"\" "+style+" bgcolor='" + caldata.selcolor + "'  onClick=\""+method+ "\" >" +i +"</td> <td></td>";
				else script = script+ "<td class=\"cata_mon_off\" id=\""+caldata.monid+i+"\" "+style+" onClick=\""+method+ "\" >" +i +"</td> <td></td>";
				if (i == 6) script = script + "</tr><tr>";
			}
			script = script + "</tr></table></td></tr>";
			return script;
		}
		else
		{
			return "";
		}
	}


	 this.makeday = function(num){
	 	if(this.calendardata[num].date_view){
			caldata.startselday = 0;
			caldata.endselday = 0;
			 if(num.length = 1){
				this.calendardata[num].dayid = "day_0"+num+"_";
			}else{
				this.calendardata[num].dayid = "day_"+num+"_";
			}

		 	caldata = this.calendardata[num];
		 	caldata.startposition =new Date(caldata.year, caldata.month-1, 1).getDay()+1;
			caldata.endday =new Date(caldata.year, caldata.month, 0).getDate();
			var date = new Date(caldata.year, caldata.month-1, 0);
			var prevendday =date.getDate();
			caldata.prevmonth = date.getMonth()+ 1;
			caldata.prevyear = date.getYear();


			date = new Date(caldata.year, caldata.month, 1);
			caldata.nextmonth = date.getMonth()+ 1;
			caldata.nextyear = date.getYear();

			j=0;
			tmpday=0;

			var Dow_FONT = "font-family : tahoma; font-color : #666666; font-weight:bold; font-size : 9px;";

			weekday="<tr align=right>";
			weekday=weekday + "<td><table border=0 cellpadding=3 cellspacing=0><tr><td width=13></td>";
			weekday=weekday + "<td><font color=#555555 style=\""+Dow_FONT+"\">&nbsp;&nbsp;</font></td>"; // week
			weekday=weekday + "<td><font color=#DE3E13 style=\""+Dow_FONT+"\">SU&nbsp;&nbsp;</font></td><td><font color=#666666 style=\""+Dow_FONT+"\">MO&nbsp;&nbsp;</font></td><td><font color=#666666 style=\""+Dow_FONT+"\">TU&nbsp;&nbsp;</font></td><td><font color=#666666 style=\""+Dow_FONT+"\">WE&nbsp;&nbsp;</font></td><td><font color=#666666 style=\""+Dow_FONT+"\">TH&nbsp;&nbsp;</font></td><td><font color=#666666 style=\""+Dow_FONT+"\">FR&nbsp;&nbsp;</font></td>";
			weekday=weekday + "<td><font color=#0A4FA3 style=\""+Dow_FONT+"\">SA</font></td><td width=13></td></tr></table>";

			weekday=weekday + "</td></tr><tr><td align=center><table border=0 cellspacing=0 cellpadding=0>";
			weekday=weekday + "<tr><td width=100%><table height=30 border=0 bordercolor=yellow cellspacing=0 cellpadding=0><tr>";

			if(caldata.method == null)
			{
				method = "'";
			}
			else
			{
				method = caldata.method+"(cal_core.calendardata["+num+"].fromdate, cal_core.calendardata["+num+"].todate);' ";
			}
			for (k=0;j<caldata.endday+caldata.startposition-1;){
				//?? ? ??
				weekday=weekday + "<tr align=center>";
				for (i=0 ; i<8 ; i++){
					k++;
					if( i == 0){
						weekday=weekday + "<td id=\""+caldata.dayid+k+"\" width=21 height=15 align=center style=\"cursor:hand;font-family:tahoma;font-size:9px;color:222222\" onClick='cal_core.setWeek("+num+", \""+caldata.dayid+k+"\");"+ method+" >-></td>";
					}else{
						j++;
						bgcolor="";
						fontcolor="";

						if(i==1){
							fontcolor="style=\"font-family:tahoma;color:#DE3E13;";
						} else if(i==7) {
							fontcolor="style=\"font-family:tahoma;color:#0A4FA3;";
						} else if(i==2 || i==3 || i==4 || i==5 || i==6){
							fontcolor="style=\"font-family:tahoma;color:333333;";
						}

						d_onclick="style=\"cursor:hand;\" onmouseover=\"cal_core.focusbgcolor("+num+",this)\" onmouseout=\"cal_core.focusoutbgcolor("+num+",this,"+k+")\"";
						if( j>= caldata.startposition && tmpday < caldata.endday) {
							fontcolor = fontcolor + "font-size:9px\"";
							tmpday++;
							if(tmpday == parseInt(caldata.day) ){
								bgcolor="bgcolor='"+caldata.selcolor+"'";
								caldata.startselday = k;
								caldata.endselday = k;
							}
							d_onclick = d_onclick+"onclick='cal_core.setday("+num+",cal_core.calendardata["+num+"].year,cal_core.calendardata["+num+"].month,\""+caldata.dayid+k+"\");"+ method;
							htmlcode="" + tmpday;
							weekday=weekday + "<td id=\""+caldata.dayid+k+"\" width=21 height=15 align=center " + fontcolor + bgcolor + d_onclick + " >";
							weekday=weekday + htmlcode + "</td>";

						}else if(j<caldata.startposition){
							fontcolor = fontcolor + "font-size:7px\"";

							htmlcode = prevendday - caldata.startposition + j+1;


							d_onclick = d_onclick+"  onClick='cal_core.setday("+num+",cal_core.calendardata["+num+"].prevyear, cal_core.calendardata["+num+"].prevmonth,\""+caldata.dayid+k+"\");"+method;

							weekday=weekday + "<td id=\""+caldata.dayid+k+"\" width=21 height=15 align=center " + fontcolor + d_onclick + " >";
							weekday=weekday + htmlcode + "</td>";



						} else {
							tmpday++;
							fontcolor = fontcolor + "font-size:7px\"";
							htmlcode = tmpday - caldata.endday;
							d_onclick = d_onclick+" onClick='cal_core.setday("+num+",cal_core.calendardata["+num+"].nextyear, cal_core.calendardata["+num+"].nextmonth,\""+caldata.dayid+k+"\");"+method;
							weekday=weekday + "<td id=\""+ caldata.dayid + k + "\" width=21 height=15 align=center " + fontcolor + d_onclick + " >";
							weekday=weekday + htmlcode + "</td>";
						}
					}
				}
				caldata.totcol = k;
			}
			return weekday;
		}
		else
		{
			return "";
		}
	}


	this.startmake = function (){
		return "<table width=100% height=100% style=\"font-size:11;border-collapse:collapse;border:1px solid #D6D6D6\" cellpadding=0 cellspacing=0 rules=none>";
	}
	this.endmake = function(){
		return "</tr> </table>";
	}

	this.setcalendar = function(num){
		this.calendardata[num].div.innerHTML = this.startmake()+ this.calendardata[num].year_script + this.calendardata[num].month_script + this.calendardata[num].date_script + this.endmake();
	}



}
var cal_core = new calendar_core();

// - File Name   : hbean_calendar.js
// - Description : 달력 공통 데이터 관리 객체.
function calendar(div,method,year,month,day)
{
	this.date = new Date();
	if(year == null) year = this.date.getFullYear();
	if(month == null) month = this.date.getMonth()+1;
	if(day == null) day = this.date.getDate();
	this.num = cal_core.addcaldata(div,method,year,month,day);

	this.showYear = function(flag)
	{
		cal_core.calendardata[this.num].year_view =  flag;
		cal_core.calendardata[this.num].year_script = cal_core.makeyear(this.num);
		cal_core.setcalendar(this.num);
	}
	this.showMonth = function(flag)
	{

		cal_core.calendardata[this.num].month_view =  flag;
		cal_core.calendardata[this.num].month_script = cal_core.makemonth(this.num);
		cal_core.setcalendar(this.num);
	}
	this.showDate = function(flag)
	{
		cal_core.calendardata[this.num].date_view =  flag;
		cal_core.calendardata[this.num].month_script =  cal_core.makemonth(this.num);
		cal_core.calendardata[this.num].date_script =  cal_core.makeday(this.num);
		cal_core.setcalendar(this.num);
	}
	this.getYear = function(){
		return cal_core.calendardata[this.num].year;
	}
	this.getMonth = function(){
		return cal_core.calendardata[this.num].month;
	}

	this.getFromDate = function(){
		return cal_core.calendardata[this.num].fromdate;
	}
	this.getToDate = function(){
		return cal_core.calendardata[this.num].todate;
	}
	cal_core.calendardata[this.num].year_script = cal_core.makeyear(this.num);
	cal_core.calendardata[this.num].month_script = cal_core.makemonth(this.num);
	cal_core.calendardata[this.num].date_script =  cal_core.makeday(this.num);
	cal_core.setcalendar(this.num);
}

//- File Name   : hbean_date.js
//- Description : Redeclare Date Object
Date.prototype.isDate = function(y, m, d)
{
var Y = eval(y);
var M = eval(m);
var D = eval(d);

var last_days = new Array('31','28','31','30','31','30','31','31','30','31','30','31');

if (y.length != 4 ) return false;
if (Y < 1900 || Y > 2100) return false;
if (M > 12 || M < 1) return false;

if (M == 2) {
	if (((Y % 4 == 0) && (Y % 100 != 0)) || (Y % 400 == 0))
		last_days[M-1] = 29;
	else
		last_days[M-1] = 28;
}

if ((D > last_days[M - 1]) || (D < 1)) {
	return false;
}

return true;
}


//- File Name   : hbean_declaration.js
//- Description :
function Declaration() {

	this.declaration = function(num, formname, isMobile) {
		if(formname == null || formname == '') {
			formname = 'declarationform';
		}

		var frm = eval("document." + formname)[num];

		if (frm.itemTitle == null) {
			frm  = eval("document." + formname);
		}

		if (frm.itemWriterId.value == "h") {
			alert("해피빈 운영자가 작성한 게시글은 신고할 수 없습니다.");
			return;
		}

		if (frm.itemTitle.value == null) {
			alert("제목이 누락되었습니다. 다시 시도해주세요.");
			return;
		}

		if (frm.itemWriterId.value == null ) {
			alert("작성자가 누락되었습니다. 다시 시도해주세요.");
			return;
		}

		document.charset="UTF-8";

		var popUpWidth = "530";
		var popUpHeight = "600";

		if(frm.itemType.value == 'CMNT') {
			popUpHeight = "550";
		}

		this.runenv = document.URL;
		this.runenv = this.runenv.substring(this.runenv.indexOf("//")+2,this.runenv.indexOf(".")+1);

		var declarationUrl = "";

		if(envArray.indexOf(this.runenv) == -1 & mobileEnvArray.indexOf(this.runenv) == -1) {
            declarationUrl = "https://srp.naver.com/main.nhn";
		} else {
            declarationUrl = "https://dev-srp.naver.com/main.nhn";
		}

		if(isMobile == undefined || isMobile == false)
		{
			window.open("","declarationPop","width=" + popUpWidth + ",height=" + popUpHeight);
			frm.target = "declarationPop";
		}
		frm.action = declarationUrl;
		frm.submit();
	};

	this.doLogin = function(isMobile) {
		if (confirm("로그인이 필요합니다.")) {
			var targetUrl = top.location.href;
			var hash = top.location.hash;
			//이벤트 iframe을 가진페이지에서 인증후 리다이렉션시 iframe쪽 url이 제대로 열리기 위
			//이벤트 캠페인 번호를 추가해주도록 한다. 이때 hash에 최근 선택한 이벤트 캠페인 번호가 들어있으므로
			//이값이 존재하는 경우에만 리턴 url에 새로운 캠페인 번호를 추가하도록 한다.
			if (hash.indexOf("#campaignNo=") >= 0) {
				targetUrl = targetUrl.replace(/&campaignNo=[0-9]+/g, '');
				targetUrl = targetUrl.replace("#", "&");
			}
			if(isMobile == true) {
				top.location.href="https://nid.naver.com/nidlogin.login?svctype=262144&url=" +  encodeURIComponent(targetUrl);
			}
			else {
				top.location.href="https://nid.naver.com/nidlogin.login?url=" +  encodeURIComponent(targetUrl);
			}
		}
	};


	this.ownerCheckAlert = function() {
		alert("본인이 작성하신 글에 대해서는 신고가 불가합니다.");
	}

	this.callDeclaration = function(loginUserId, writerId, formName){
		if (loginUserId == "") {
			//비로그인상태
			this.doLogin();
		}else if (loginUserId == writerId) {
			//로그인유저와 작성자가 같음
			this.ownerCheckAlert();
		}else{
			//로그인유저와 작성자가 다름
			this.declaration(0, formName);
		}

	}
}

//- File Name   : hbean_happylink.js
//- Description : A web editor for the Happybean service.
function HappyLink(){

	this.runenv = document.URL;

	this.runenv = this.runenv.substring(this.runenv.indexOf("//")+2,this.runenv.indexOf(".")+1);

	if(envArray.indexOf(this.runenv) == -1) {
		this.runenv = "";
	}

	this.postScrap  = function(postno,target){

		if(target=="print")
		{
			hbeannet.open("https://"+this.runenv+"happybean.naver.com/together/ThemeStoryViewPrint.nhn?hlg_artcl_no="+postno, "_postScrap", 800, 600, "yes");
		}
		else
		{
			hbeannet.open("https://"+this.runenv+"happybean.naver.com/together/PostScrap.nhn?artclNo="+postno+"&target=" + target, "_postScrap");
		}
	}
	/* 클럽 소식 스크랩 */
	this.clubTidingsScrap = function (clbTdgNo,target)
	{
		if(target=="print")
		{
			hbeannet.open("https://"+this.runenv+"happybean.naver.com/membersclub/MembersClubTidingsViewPrint.nhn?clbTdgNo="+clbTdgNo, "_clubTidingsPrint", 830, 700, "yes");
		}
		else
		{
			hbeannet.open("https://"+this.runenv+"happybean.naver.com/membersclub/MembersClubTidingsScrap.nhn?clbTdgNo="+clbTdgNo+"&target=" + target, "_clubTidingsScrap");
		}
	}

	this.fundScrap  = function(fundno,target){

		var openUrl = "/scrap/FundScrap.nhn?fundno=" + fundno;
		hbeannet.open(openUrl + "&info=" +target, "_fundScrap");
	}

	this.fundScrap2  = function(fundno,target){

		var openUrl = "/scrap/FundScrap.nhn?fundno=" + fundno;
		hbeannet.open(openUrl + "&info=" +target + "&type=campaign", "_fundScrap");
	}

	this.volunteerScrap  = function(vlntrscrapNo,target){

		var openUrl = "https://"+this.runenv+"happybean.naver.com/volunteer/VntScrap.nhn?vntArtclNo=" + vlntrscrapNo;
		hbeannet.open(openUrl + "&target=" +target, "_volunteerScrap");

	}


	this.openPersonalInfo  = function(userId){
		hbeannet.open("https://"+this.runenv+"happybean.naver.com/my/MyPersonalInfo.nhn?userid="+ userId, "_blank");
	}

	this.openBlog = function(userId){
		var idPrefix = userId.substr(0,5);

		if(idPrefix == "hlog_")
		{
			this.openBrandlog(userId);
		}
		else
		{
			hbeannet.open("https://blog.naver.com/"+ userId, "_blank");
		}
	}

	this.openNaverNote = function(userId){
		hbeannet.open("https://"+this.runenv+"happybean.naver.com/my/NaverNoteSelectUser.nhn?userid="+ userId, "popupNaverNote", 400, 450);
	}

	this.openGiftCong = function(userId){
		if (userId == null || userId == 'undefind')
	    {
	        hbeannet.open("https://"+this.runenv+"happybean.naver.com/my/GiftCongSelectUser.nhn", "popupGiftCong", 400, 450);
	    } else {
	        hbeannet.open("https://"+this.runenv+"happybean.naver.com/my/GiftCongSelectUser.nhn?userid="+ userId, "popupGiftCong", 400, 450);
	    }
	}



	//나눔 내역 후기 스크립/프린트
	this.sharePostscriptScrap  = function(artclNo,target){

		if(target=="print")
		{
			hbeannet.open("https://"+this.runenv+"happybean.naver.com/brandlog/SharePostscriptPrint.nhn?hpbnPsArtclNo="+artclNo, "_shareBoxPostscriptPrint",990,800);
		}
		else
		{
			hbeannet.open("https://"+this.runenv+"happybean.naver.com/brandlog/SharePostscriptScrap.nhn?hpbnPsArtclNo="+artclNo+"&target=" + target, "_shareBoxPostscriptScrap");
		}
	}

	/* 이용 제한 레이어 닫기 */
	this.closeUseLimit = function()
	{
		document.getElementById("useLimit_layer").style.display="none";
	}


}
var happylink = new HappyLink();

//
//- File Name   : hbean_hook.js
//- Description : capture a mouse click.
/**
* code sample :
*  after clicking a DIV tag the others would be clicked,
*  open a alert box which show a DIV innerHTML attribute value.
*
* hbeanhook.handlers.add(
* {
* 	capture : function (event, target)
* 	{
* 	    if(target.nodeName == "DIV")
* 	    {
*          //run when a DIV tag is clicked.
* 	        this.element = target;
*          alert("hook ok");
* 	        return true;
* 	    }
* 	}
* 	,
* 	release : function ()
* 	{
* 	    alert(this.element.innerHTML);
* 	}
* 	,
* 	element : null
* }
* );
*/

if(window.onload == null)
{
window.onload = hbean.callLoadListener;
}
var hbeanhook = {
handlers : new Array()
,
capturedhandler : null
,
setup : function()
{
	if (document.addEventListener)
	{
	    document.addEventListener('click', hbeanhook.onclick, true);
	}
	else if(hbean.NS4)
	{
		document.captureEvents(Event.CLICK);
		document.onclick = hbeanhook.onclick;
	}
	else if(hbean.IE4)
	{
		document.onclick = hbeanhook.onclick;
	}
}
,
onclick : function(event)
{
	if(!event) event = window.event;
	var click_target = (hbean.NS4) ? event.target : event.srcElement;

	if(hbeanhook.capturedhandler != null)
	{
		try
		{
			hbeanhook.capturedhandler.release(event);
		}
		catch(e){}
		hbeanhook.capturedhandler = null;
	}

	var handlers = hbeanhook.handlers;
	var handler = null;
	for(var i=handlers.length-1;i>=0;i--)
	{
		handler = handlers[i];
		if(handler.capture != null && typeof(handler.capture) == "function")
		{
			try
			{
				if(handler.capture(event, click_target))
				{
					break;
				}
			}
			catch(e){}
		}
		handler = null;
	}

	if(handler != null)
	{
		if(handler.release != null && typeof(handler.release) == "function")
		{
			hbeanhook.capturedhandler = handler;
		}
	}
}
}
hbean.observer.add("hbeanhook.setup();");


//- File Name   : hbean_menu.js
//- Description : A div popup menu.
/**
* code sample :
*  after clicking a DIV tag the others would be clicked,
*  open a alert box which show a DIV innerHTML attribute value.
*
* <style type="text/css">
* body, div, td, li {font-size:12px;color:#777777;}
* body, div, td, li {font-family:Dotum,AppleGothic,sans-serif}
* </style>
*
* <script type="text/javascript">
* var menuname = "sample_menu_";
* function setupmenu()
* {
* 	var scrap_ui = ""
* 	+"<table width=\"115\" border=\"0\" cellspacing=\"1\" cellpadding=\"0\" bgcolor=\"#B6B6B6\">"
* 	+"<tr><td> "
* 	+"<table width=\"100%\" border=\"0\" cellspacing=\"1\" cellpadding=\"0\" bgcolor=\"#FFFFFF\">"
* 	+"<tr> "
* 	+"<td style=\"padding: 3 0 0 10\" height=\"23\" id=\"sample_menu_item1\"><span><font color=#404040>first menu</font></span></td>"
* 	+"</tr><tr> "
* 	+"<td style=\"padding: 3 0 0 10\" height=\"23\" id=\"sample_menu_item1\"><span><font color=#404040>second menu</font></span></td>"
* 	+"</tr><tr> "
* 	+"<td style=\"padding: 2 0 0 10\" height=\"22\" id=\"sample_menu_itemc\"><span><font color=#404040>third menu</font></span></td>"
* 	+"</tr>"
* 	+"</table></td></tr></table>"
* 	+"";
* 	hbeanmenu.create(menuname, scrap_ui, function(event, element)
* 	{
* 		var id = element.id;
*      var menuid = id.substring(menuname.length+"item".length);
*      if(menuid == "1") alert("clicked a first or second menu item.");
*      if(menuid == "c") alert("clicked a third menu item.");
* 	}
* 	);
* }
* hbean.observer.add("setupmenu()");
* </script>
*/
if(window.onload == null)
{
window.onload = hbean.callLoadListener;
}
function __hbeanmenuclass(name, div, menuitems, callbackhandler)
{
this.name = name;
this.div = div;
this.items = menuitems;
this.target;
this.callbackhandler = callbackhandler;
this.onmouseover = function(event, item)
{
	item.style.backgroundColor = hbeanmenu.color.backgroundon;
};
this.onmouseout = function(event, item)
{
	item.style.backgroundColor = hbeanmenu.color.backgroundoff;
};
this.onclick = function(event, item)
{
	if(this.callbackhandler)
	{
		try
		{
			this.callbackhandler(event, item, this.target);
		}catch(e){}
	}
};
this.onchangevisible = function(visible)
{
};

if(menuitems != null && menuitems.length != null)
{
	if(hbean.IE4)
	{
		for(var i=0;i<menuitems.length;i++)
		{
			var setupitem = function(menu, menuitem)
			{
				menuitem.style.cursor="pointer";
				menuitem.attachEvent("onmouseover", function recall_(event){menu.onmouseover(event, menuitem);});
				menuitem.attachEvent("onmouseout", function recall_(event){menu.onmouseout(event, menuitem);});
				menuitem.attachEvent("onclick", function recall_(event){menu.onclick(event, menuitem);});
			}
			setupitem(this, menuitems[i]);
		}
	}
	else if(hbean.NS4)
	{
		for(var i=0;i<menuitems.length;i++)
		{
			var setupitem = function(menu, menuitem)
			{
				menuitem.style.cursor="pointer";
				menuitem.addEventListener("mouseover", function(event){menu.onmouseover(event, menuitem);}, true);
				menuitem.addEventListener("mouseout", function(event){menu.onmouseout(event, menuitem);}, true);
				menuitem.addEventListener("click", function(event){menu.onclick(event, menuitem);}, true);
			}
			setupitem(this, menuitems[i]);
		}
	}
}
}

var hbeanmenu = {
color : {backgroundon : "#E4FF75", backgroundoff : "#FFFFFF", foregroundon : "#FFFFFF", foregroundoff : "#000000"}
,
menus : new Array()
,
setup : function()
{
	hbeanhook.handlers.add(
	{
		capture : function (event, target)
		{

			if(target.nodeName == "A" || target.parentNode.parentNode.nodeName == "A" || (target.nodeName == "INPUT" && target.type == "image") || (target.parentNode.nodeName == "A" && target.nodeName == "IMG"))
			{
				var __value__ = "";

				if(target.nodeName == "A") {
					__value__ = target.toString();
				} else if(target.parentNode.nodeName == "A" && target.nodeName == "IMG") {
					__value__ = target.parentNode.toString();
					target = target.parentNode;
				} else if(target.parentNode.parentNode.nodeName == "A") {
					__value__ = target.parentNode.parentNode.toString();
					target = target.parentNode.parentNode;
				} else {
					__value__ = target.value.toString();
				}
				var __pattern__ = /javascript[:]showmenu\s*\((.*)\)\s*[;]?\s*$/i;
				var __matchs__ = __value__.match(__pattern__);

				if(__matchs__)
				{
					//check parameter
					__value__ = __matchs__[1];

					__pattern__ = /(?:['][^']*[']|(?:["][^"]*["]|[^,)]+))/;
					__matchs__ = __value__.match(__pattern__);

				}

				if(__matchs__)
				{
					var __menuname__ = '';
					try
					{
						__menuname__ = eval(__matchs__[0]);
					}
					catch(e){}

					var menus = hbeanmenu.menus;
					for(var i=0;i<menus.length;i++)
					{
						if(menus[i].name == __menuname__)
						{
							try
							{
								menus[i].target = target;
								hbeanmenu.show(event, menus[i]);
							}catch(e){}
							return true;
						}
					}
				}
			}
		}
		,
		release : function (event)
		{
			try
			{
				hbeanmenu.hide();
			}catch(e){alert(e);}
		}
	});
}
,
create : function(name, htmltext, callbackhandler)
{
	if(window.document.body == null)
	{
		return null;
	}

	var menuitems = new Array();
	var div = document.createElement("DIV");
	div.style.position="absolute";
	// 이벤트 레이어 위로 적용되는 경우때문에 z-index속성추가 -by iluulm225
	div.style.zIndex=1000;
	// 이벤트 레이어 위로 적용되는 경우때문에 z-index속성추가 -by iluulm225
	div.style.top=0;
	div.style.left=0;
	div.style.display="none";
	div.innerHTML = htmltext;
	var searchitems = function(node)
	{
		var childs = node.childNodes;
		for(var i=0;i<childs.length;i++)
		{
			if(childs[i].nodeName != "#text")
			{
				if(childs[i].id.indexOf(name+"item")==0)
				{
					menuitems.add(childs[i]);
				}
				searchitems(childs[i]);
			}
		}
	}
	searchitems(div);

	var menuobject = null;
	try
	{
		hbeanmenu.menus.add(menuobject = new __hbeanmenuclass(name, div, menuitems, callbackhandler));
		window.document.body.appendChild(div);
	}catch(e){}
	return menuobject;
}
,
show : function(event, menu)
{
	var div = menu.div;
	var body = document.body;

	var x = 0;
	var y = 0;

	var scrollX=0, scrollY=0;
    if(hbean.NS4)
    {
		x = event.pageX + "px";
		y = event.pageY + "px";

		if (document.location.href != parent.document.location.href) {
			x = Math.min(event.pageX, document.documentElement.clientWidth-130) + "px";
			y = event.pageY + "px";
		}

		scrollX = document.documentElement.scrollLeft;
		scrollY = document.documentElement.scrollTop;
	}
	else if(hbean.IE4)
	{
		if( document.body && ( document.body.scrollLeft || document.body.scrollTop ) ) {
			//DOM compliant
			scrollX = document.body.scrollLeft;
			scrollY = document.body.scrollTop;
		} else if( document.documentElement && ( document.documentElement.scrollLeft || document.documentElement.scrollTop ) ) {
			//IE6 standards compliant mode
			scrollX = document.documentElement.scrollLeft;
			scrollY = document.documentElement.scrollTop;
		}
		x = scrollX + event.clientX;
		y = scrollY + event.clientY;

		x = x +"px";
		y = y +"px";
	}

    div.style.left = x;
	div.style.top = y;

	div.style.display = "";
	var menuwidth = div.offsetWidth;
	var menuheight = div.offsetHeight;
	if(x + menuwidth > scrollX + document.body.clientWidth)
	{
		x-=menuwidth;
	}
    if(y + menuheight > scrollY + document.body.clientHeight)
    {
    	y-=menuheight;
    }

    div.style.left = x;
    div.style.top = y;

	menu.onchangevisible(true);
}
,
hide : function()
{
	var menus = hbeanmenu.menus;
	for(var i=0;i<menus.length;i++)
	{
		if(menus[i].div.style.display == "")
		{
			menus[i].onchangevisible(false);
			menus[i].div.style.display = "none";
		}
	}
}
};

hbean.observer.add("hbeanmenu.setup()");
function showmenu(menuname){} //fired after show menu


