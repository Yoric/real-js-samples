STK.register("v6.pl.ugSEO.rightFeed.source.init",function(a){var b=a.core.dom.contains;return function(b){var c={isAnchorElementOrHasActionType:function(a){return a.tagName&&a.getAttribute?a.tagName.toUpperCase()==="A"||!!a.getAttribute("action-type"):!1}},d={eachFeedClick:function(b){var d=b.el,e=b.evt.target||b.evt.srcElement,f=d.getAttribute("href");if(!!f){var g=0,h=!1;while(e){if(g++>10)break;if(!a.contains(d,e))break;c.isAnchorElementOrHasActionType(e)&&(h=!0);e=e.parentNode}h||window.open(f)}}},e=a.delegatedEvent(b),f={};e.add("feed_list_item","click",d.eachFeedClick);f.destroy=function(){e.remove("feed_list_item","click",d.eachFeedClick)};return f}});
