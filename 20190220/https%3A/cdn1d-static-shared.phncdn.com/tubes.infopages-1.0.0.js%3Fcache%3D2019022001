var MG_information_menu=function(){var a=this;a.init=function(b){a.params=b;a.params.active_class="active";a.params.link_menu_id="linkMenu_";a.params.links_list=[];a.params.menu_id_data=a.menu_list_array();document.getElementById(a.params.container_id).innerHTML='<ul id="'+a.params.ul_list_id+'"></ul>';a.build_tab_list();a.add_menu_listener()};a.menu_list_array=function(){var e=[],f=a.params.menu_data,d=0,c=f.length,b=new RegExp(a.params.my_platform,"ig");for(;d<c;d++){if(f[d].platforms.match(b)!==null){e.push(f[d].tab_id)}}return e};a.build_tab_list=function(){var b="",e='<li id="'+a.params.tab_menu_prefix+'[[tab_id]]">[[label]]</li>',g=function(h){if(a.params.menu_data[h].url===undefined){return a.params.menu_data[h].label}else{return'<a id="'+a.params.link_menu_id+a.params.menu_data[h].label+'" href="'+a.params.menu_data[h].url+'">'+a.params.menu_data[h].label+"</a>"}},d=0,c=a.params.menu_id_data.length;for(;d<c;d++){var f=e;f=f.replace(/\[\[tab_id]]/g,a.params.menu_id_data[d]);f=f.replace(/\[\[label]]/g,g(d));b+=f}document.getElementById(a.params.ul_list_id).innerHTML=b};a.add_menu_listener=function(){var k=document,b=window.location.hash.replace("#",""),l="click",h=a.params.menu_id_data,e=0,d=h.length;for(;e<d;e++){var f=document.getElementById(a.params.link_menu_id+h[e]);if(f===null){var g=k.getElementById(a.params.tab_menu_prefix+h[e]),c=k.getElementById(a.params.footer_menu_prefix+h[e]);if(g.addEventListener!==undefined){g.addEventListener(l,a.modern_btn_action);c.addEventListener(l,a.modern_btn_action)}else{g.attachEvent("on"+l,a.btn_action);c.attachEvent("on"+l,a.btn_action)}}}a.show_hide_content(b===""?a.params.menu_id_data[0]:b)};a.modern_btn_action=function(){var b=this.id.split("_");a.show_hide_content(b[1])};a.btn_action=function(b){var c=b.srcElement.id.split("_");a.show_hide_content(c[1])};a.show_hide_content=function(c){var e=a.params.menu_id_data,d=0,b=e.length;window.location.hash=c;for(;d<b;d++){a.class_handler(e[d],c===e[d]?"add":"remove")}};a.class_handler=function(e,c){var b=[a.params.tab_menu_prefix,a.params.content_prefix],g=0,d=b.length;for(;g<d;g++){var f=document.getElementById(b[g]+e);if(f!==null){if(f.classList!==undefined){if(c==="add"){f.classList.add(a.params.active_class)}else{f.classList.remove(a.params.active_class)}}else{if(c==="add"){f.className+=" "+a.params.active_class}else{f.className=f.className.replace(new RegExp("(^|\\b)"+a.params.active_class.split(" ").join("|")+"(\\b|$)","gi")," ")}}}}}},my_menu=new MG_information_menu();my_menu.init({container_id:page_params.mg_information_menu.container_id,ul_list_id:page_params.mg_information_menu.ul_list_id,footer_menu_prefix:page_params.mg_information_menu.footer_menu_prefix,tab_menu_prefix:page_params.mg_information_menu.tab_menu_prefix,content_prefix:page_params.mg_information_menu.content_prefix,menu_data:page_params.mg_information_menu.menu_data,my_platform:page_params.myPlatform});