function promoBanner(){var e=document.querySelectorAll(".premiumPromoBannerWrapper");!e.length||""==smallImageFg&&""==smallImageBg||(MG_Utils.addEventHandler(document.querySelector(".premiumPromoBannerClose"),"click",function(e){e.preventDefault?e.preventDefault():e.returnValue=!1;var r=this.getAttribute("data-root");"persistant"==persistantMode?setCookie("promoBannerPersistant","1",{expires:2,domain:r}):setCookie("premiumPromoBanner"+promobannerId,"1"),this.closest(".premiumPromoBannerWrapper").style.display="none"}),"persistant"==persistantMode?void 0==getCookie("promoBannerPersistant")&&(e[0].style.display="block"):void 0==getCookie("premiumPromoBanner"+promobannerId)&&(e[0].style.display="block"),MG_Utils.addEventHandler(document.querySelector(".promoBannerClassAnchor"),"click",function(){mixpanel.track("Promo Banner",promoClick)}))}MG_Utils.domReady(promoBanner);