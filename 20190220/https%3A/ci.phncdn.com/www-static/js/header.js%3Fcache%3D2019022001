function ajaxPost(e,t){if(XMLHttpRequest){var o=new XMLHttpRequest;o.open("POST",e,!0),o.setRequestHeader("Content-Type","application/x-www-form-urlencoded; charset=UTF-8"),o.onreadystatechange=function(){if(4===o.readyState&&o.status>=200&&o.status<400&&t){document.querySelectorAll(t)[0].innerHTML=o.responseText}},o.send()}return!1}function rankingNumberFormatter(e){if("string"==typeof e){e=e.replace(/[.,\s]/g,"")}return e>999999?parseInt((e/1e6).toFixed(0))+"M":e>999?parseInt((e/1e3).toFixed(0))+"K":e+""}function displayNumbers(){var e=document.querySelectorAll(".formattedCounter");if(e.length>0)for(var t=0;t<e.length;t++)e[t].innerText=rankingNumberFormatter(e[t].innerText),e[t].classList.remove("hidden")}var $j=jQuery.noConflict(),yesNoModal={show:function(e,t,o){var n=document.getElementById("yesNoModal");MG_Utils.setText(n.querySelector(".text"),e);var l=new MG_Modal({content:n,className:"yesNoModalContainer"});null!==typeof l&&l.openModal(function(){var e=document.querySelector(".yesNoModalContainer"),n=e.querySelector(".mobileFriedly.yesBtn"),r=e.querySelector(".mobileFriedly.noBtn"),a=e.querySelector(".mobileFriedly.modal-close");MG_Utils.removeEventHandler(n,toggleClickEvent),r&&MG_Utils.removeEventHandler(r,toggleClickEvent),a&&MG_Utils.removeEventHandler(a,toggleClickEvent),MG_Utils.addEventHandler(n,toggleClickEvent(),function(e){e.preventDefault?e.preventDefault():e.returnValue=!1,null!==typeof l&&l.closeModal(),"function"==typeof t&&t()});var i=[];r&&i.push(r),a&&i.push(a),i.forEach(function(e,t){MG_Utils.addEventHandler(e,toggleClickEvent(),function(t){if(t.preventDefault?t.preventDefault():t.returnValue=!1,null!==typeof l){var n=$j(e)[0].dataset.video,r="premiumTemplate_"+n,a=$j("#"+r),i=$j("label[for='premiumTemplate_"+n+"']");n&&(a.show(),$j("html, body").animate({scrollTop:a.offset().top},500),a.hide(),i.addClass("evidentLabel"),i.effect("highlight",{},2e3),a.prop("checked",!0),$j("#premiumCheck_"+n).val(1),$j(".privacyDropdown").toggle()),l.clos