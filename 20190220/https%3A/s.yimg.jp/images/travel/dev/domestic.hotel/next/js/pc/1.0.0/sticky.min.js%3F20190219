(function(g){var n=g(document.body),f=g(window),b=f.height()*1,s=g(".featureStickBanner").height(),t=g(".featureStickBanner").hide(),c=g("#trvFooter"),p=(YAHOO.JP.travel&&YAHOO.JP.travel.istop)||false,q=(YAHOO.JP.travel&&YAHOO.JP.travel.is_cookie)||false,i='<a class="%stickyclass" href="%stickyhref"><img class="%imgclass" alt="%imgalt" src="%imgsrc"></a><div class="featureStickBanner-close"><a class="featureStickBanner-close-button" href="#">閉じる</a></div>',l=false,m,j,o,a,r,h,e,u;if(!q){return}if(c.length==0){c=g("#contentsFt")}g.ajax({url:"/api/sticky/stickyinfo",type:"GET",contentType:"application/json",dataType:"json",cache:false,timeout:5000}).done(function(v){if(v.ResultSet.Status=="000000"){i=i.replace(/%stickyclass/g,v.ResultSet.Result.className);i=i.replace(/%stickyhref/g,v.ResultSet.Result.imageLinkUrl);i=i.replace(/%imgclass/g,v.ResultSet.Result.imageClassName);i=i.replace(/%imgalt/g,v.ResultSet.Result.imageAlt);i=i.replace(/%imgsrc/g,v.ResultSet.Result.imagePath);g("#stickyBanner").html(i)}d()}).fail(function(){d()});function d(){t.show();c.css("marginBottom",s+"px");g(".featureStickBanner-close-button").off().on({click:function(v){v.preventDefault();v.stopPropagation();g.ajax({url:"/api/sticky/off",type:"GET",contentType:"application/json",dataType:"json",cache:false}).done(function(w){f.off(".sticky");k();c.css("marginBottom","0px")}).fail(function(){});return false}});t.appendTo(n)}function k(){t.fadeOut("fast");l=false}})($);