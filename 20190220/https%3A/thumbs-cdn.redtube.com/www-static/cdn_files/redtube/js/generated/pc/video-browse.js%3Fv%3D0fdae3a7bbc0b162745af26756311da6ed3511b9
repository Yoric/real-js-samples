var Tabs_System=function(){"use strict";var l=this;l.defaultSettings={label_class:"tab-block-label",active_class:"tab-block-active",lazy_class:"tab-lazy",reset_active:!1},l.init=function(a){l.params=$.extend(!0,l.defaultSettings,a),l.add_listeners()},l.add_listeners=function(){$("."+l.params.label_class).on("click",function(a,s){l.params.reset_active?$("#"+l.get_tab_id(this)).hasClass(l.params.active_class)?($("."+l.params.active_class).removeClass(l.params.active_class),$(l.params.tab_block_content_wrap).hide()):($(l.params.tab_block_content_wrap).show(),l.showTab(this)):l.showTab(this)})},l.get_tab_id=function(a){var s=$(a).data("tabid");return void 0!==s&&s},l.lazyLoad=function(a){var s=$("#"+a);if(s.hasClass(l.params.lazy_class))try{var t=new LazyLoadImage,e={sections:[a],thumbUrl:"thumb_url"};t.init(e),s.removeClass(l.params.lazy_class)}catch(a){console.log("error loading LazyLoadImage object")}},l.showTab=function(a){var s=l.get_tab_id(a);0!=s&&($("."+l.params.active_class).removeClass(l.params.active_class),$(a).addClass(l.params.active_class),$("#"+s).addClass(l.params.active_class),"undefined"!=typeof lazyloadV2&&lazyloadV2 instanceof MG_Lazyload?lazyloadV2.forceLoad("#"+s):l.lazyLoad(s)),l.params.hide_tab_on_change&&($("."+l.params.tab_block_content_wrap).hide(),$("."+l.params.tab_block_content_wrap+"."+l.params.active_class).show())}};try{var my_Tabs_System=new Tabs_System;my_Tabs_System.init(page_params.tabs_system_setup)}catch(a){console.log("ERROR === Favorite_System ==="),console.log(a)}