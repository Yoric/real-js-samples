naml.createNamespace("naml.module").MaterialAdditionalList=najindo.$Class({$init:function(a){this._initVar(a)},_initVar:function(a){this._htTemplate="desktop"==a.type?naml.module.MaterialAdditionalList.DESKTOP_TEMPLATE:naml.module.MaterialAdditionalList.MOBILE_TEMPLATE,this._htListData=null,this._bCreateList=!1},setData:function(a){this._htListData=a},generateList:function(a){this._welBase=najindo.$Element(a),this._welBase&&(najindo.$("naml_material_additional_list")&&najindo.$Element("naml_material_additional_list").leave(),this._welBase.appendHTML('<div id="naml_material_additional_list">'+this._makeList()+"</div>"),this._bCreateList=!0)},isCreateList:function(){return this._bCreateList},_makeList:function(){var a=this._htListData,t=[];for(var i in a)t.push(this._htTemplate[i].process({listData:a[i]}));return t.join("")}}).extend(najindo.Component||najindo.m.Component),naml.module.MaterialAdditionalList.DESKTOP_TEMPLATE={book:najindo.$Template(['<div id="naml_book_material_additional_list" class="na_material_additional_lst"><table cellspacing="0" border="1" summary="도서 정보">',"<caption>전체 도서 리스트</caption>",'<colgroup><col width="*"><col width="120"></colgroup><tbody>','<tr><th colspan="2" style="padding-left:20px;">전체 도서 리스트</th></tr>',"{for data in listData}",'<tr><td>{if data.url != ""}<a href="{=data.url}" onclick="naml.nclicks(this,\'mat.info\',\'\',\'\', null);" target="_blank">{/if}<strong>{=data.title}</strong> <span class="info">({=data.author} 저, {=data.publishDate}, {=data.publisher})</span>{if data.url != ""}</a>{/if}</td>','<td><a href="http://book.naver.com/bookdb/price.nhn?bid={=data.bid}#book_price" onclick="naml.nclicks(this,\'mat.book\',\'\',\'\', null);" target="_blank" id="_naml_book_sell_btn_{=data.bid}" class="na_button_type2" {if data.bookSale == false}style="display:none"{/if}><span>도서구매</span></a>&nbsp;','<a href="http://book.naver.com/bookdb/price.nhn?bid={=data.bid}#ebook_price" onclick="naml.nclicks(this,\'mat.ebook\',\'\',\'\', null);" target="_blank" id="_naml_ebook_sell_btn_{=data.bid}" class="na_button_type2" {if data.ebookSale == false}style="display:none"{/if}>eBook구매</a>',"</td></tr>","{/for}","</tbody></table></div>"].join(""))},naml.module.MaterialAdditionalList.MOBILE_TEMPLATE={book:najindo.$Template(['<div id="naml_book_material_additional_list" class="na_material_additional_lst"><ul><li><span class="txt">전체 도서 리스트</span></li>',"{for data in listData}",'<li>{if data.url != ""}<a href="{=data.url}" onclick="naml.nclicks(this,\'mat.info\',\'\',\'\', null);" target="_blank">{/if}<strong>{=data.title}</strong> ({=data.author} 저, {=data.publishDate}, {=data.publisher}){if data.url != ""}</a>{/if}','<div class="btn_wrapper"><a href="http://m.book.naver.com/bookdb/book_detail.nhn?showTab=3&biblio.bid={=data.bid}" onclick="naml.nclicks(this,\'mat.book\',\'\',\'\', null);" class="sell_btn" {if data.bookSale == false}style="display:none"{/if}>도서구매</a>','&nbsp;<a href="http://m.book.naver.com/bookdb/book_detail.nhn?showTab=3&biblio.bid={=data.bid}#buyEBookATag" onclick="naml.nclicks(this,\'mat.ebook\',\'\',\'\', null);" class="sell_btn" {if data.ebookSale == false}style="display:none"{/if}>EBOOK</a>',"</div></li>","{/for}","</ul></div>"].join(""))};