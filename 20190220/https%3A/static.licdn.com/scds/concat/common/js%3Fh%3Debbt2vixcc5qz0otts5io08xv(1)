window.track=window.track||{};(function(track,setTimeout,clearTimeout,JSON,window){var TIMEOUT=2e4,NOOP=function(){};var xhr=track.xhr={},pool=[],create;xhr.post=function(o){request("post",o)};xhr.get=function(o){request("get",o)};function request(method,o){var req=pool.length?pool.pop():create(),data=null,aborted,n;o.callback=o.callback||NOOP;req.open(method,noCache(o.url),true);req.setRequestHeader("X-Requested-With","XMLHttpRequest");req.setRequestHeader("X-IsAJAXForm","1");if(method.toLowerCase()==="post"){req.setRequestHeader("Content-Type","application/x-www-form-urlencoded");if(o.data){try{data="data="+JSON.stringify(o.data)}catch(err){}}}req.onreadystatechange=function(){if(aborted){o.callback({status:504,text:null})}else if(req.readyState!==4){return}else{if(n){clearTimeout(n);n=null}o.callback({status:req.status,text:req.responseText})}req.onreadystatechange=null;pool.push(req)};req.send(data);n=setTimeout(function(){aborted=true;req.abort()},TIMEOUT)}function noCache(url){var q=url.indexOf("?");if(q===-1){url+="?"}else{url+="&"}url+="_"+ +new Date;return url}function standardXHR(){return new window.XMLHttpRequest}function msAXO(){return new window.ActiveXObject("Microsoft.XMLHTTP")}try{standardXHR();create=standardXHR}catch(e){msAXO();create=msAXO}})(window.track,window.setTimeout,window.clearTimeout,window.JSON,window);