var BBS={goBBS:function(b,a,c){var d="";if(b!=null){d="?docId="+b}if(a!=null){PAGE.nClickForNoLink(a,c)}var e=topPath+"bbs/notice.help"+d;PAGE.goLink(e,"IN");return false}};var SERVICE={goService:function(d,b,e,a,c){PAGE.nClickForNoLink(a,c);if(b=="LINK01"){e=topPath+"service/main.help?serviceNo="+d;PAGE.goLink(e,"IN")}else{if(b=="LINK02"){PAGE.goLink(e,"IN")}else{if(b=="LINK03"){PAGE.goLink(e,"OUT")}}}return false}};var PAGE={goLink:function(d,b,a,c){if(a!=null){this.nClickForNoLink(a,c)}if(b==null){b="IN"}if(b=="IN"){location.href=d}else{if(b=="OUT"){window.open(d)}}return false},change:function(a){location.href=topPath+a;return false},nClickForNoLink:function(a,c){var b=new Object();clickcr(b,a,"","",c)}};var CATEGORY={change:function(i,b,f,h,g,a,c){if(c!=null){PAGE.nClickForNoLink(c,a)}else{if(g=="GRP01"){PAGE.nClickForNoLink("ctg.howlist",a)}else{if(g=="GRP02"){PAGE.nClickForNoLink("ctg.helplist",a)}}}if(f=="VC05"){window.open(h)}else{if(f=="VC06"){jQuery(location).attr("href",h)}else{var j=jQuery('<form id="moveToContents" style="display:none"></form>');var e=jQuery('<input type="hidden" name="serviceNo" value="'+i+'"/>');var d=jQuery('<input type="hidden" name="categoryNo" value="'+b+'"/>');j.append(e).append(d);jQuery("#content").append(j);j.prop("action",topPath+"contents/contents.help");if(f=="VC01"){j.prop("action",topPath+"service/main.help")}j.prop("method","get");j.submit()}}return false}};jQuery.fn.firstClickForInput=function(){var b=jQuery(this);var a=true;this.isNotFirstClicked=function(){return a};return this.each(function(){b.one("click",function(){a=false;jQuery(this).val("")})})};jQuery.fn.searchInitialSound=function(d){var h=this;var g=jQuery(this);var f=true;var b="";var e=-1;var a=-1;var c=["ㄱ","ㄲ","ㄴ","ㄷ","ㄸ","ㄹ","ㅁ","ㅂ","ㅃ","ㅅ","ㅆ","ㅇ","ㅈ","ㅉ","ㅊ","ㅋ","ㅌ","ㅍ","ㅎ"];this.openAutoKeyword=function(){d.show();g.addClass("fcs")};this.closeAutoKeyword=function(){d.hide();g.removeClass("fcs");e=-1;a=-1;d.find("li").removeClass("on")};this.makeServiceNameList=function(i){d.find("li").remove();jQuery.ajax({type:"POST",url:topPath+"json/getServiceGroup.help",data:{initialSound:i},dataType:"json",success:function(j){jQuery.each(j,function(l,k){d.find("ul").append(h.makeHtml(k))});h.linkEvent(d.find("li"));h.openAutoKeyword()}})};this.makeHtml=function(i){var k=i.serviceName;var n=i.serviceNo;var j=k.substring(0,1);var m=k.substr(1);var l='<li><a href="#" code="'+n+'" text="'+k+'"><strong>'+j+"</strong>"+m+"</a></li>";return l};this.setValue=function(j,i){g.val(j);g.attr("code",i)};this.linkEvent=function(j){var i=j.size()-1;var k;var l;g.unbind("keydown");g.keydown(function(m){if(m.keyCode==38){a=e;if(--e<0){e=i}k=jQuery(j[e]);l=k.find("a");jQuery(j[a]).removeClass("on");k.addClass("on");h.setValue(l.attr("text"),l.attr("code"))}else{if(m.keyCode==40){a=e;if(++e>i){e=0}k=jQuery(j[e]);l=k.find("a");jQuery(j[a]).removeClass("on");k.addClass("on");h.setValue(l.attr("text"),l.attr("code"))}}});j.mouseover(function(){jQuery(this).addClass("on")}).mouseout(function(){jQuery(this).removeClass("on")});j.find("a").click(function(){h.setValue($(this).attr("text"),$(this).attr("code"));h.closeAutoKeyword()})};this.getFirstSpelling=function(i){var j=i-44032;if(j>-1&&j<11172){result=c[Math.floor(j/588)]}else{result=String.fromCharCode(i)}return result};return this.each(function(){g.keyup(function(){var j=g.val();if(j.length>0){var i=h.getFirstSpelling(j.charCodeAt(0));if(b!=i){b=i;h.makeServiceNameList(b)}else{h.openAutoKeyword()}}else{b="";h.closeAutoKeyword()}}).focusout(function(){f=true}).click(function(){h.keyup();f=false});$(document).click(function(){if(f){h.closeAutoKeyword()}})})};function goTop(a){setTimeout(function(){jQuery.scrollTo(jQuery(a),100,{offset:-10})},100)};