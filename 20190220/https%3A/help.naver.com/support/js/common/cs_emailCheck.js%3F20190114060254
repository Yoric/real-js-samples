function _trim(a){var b;if(a!=""){b=a.value;b=b.replace(/^\s*/,"").replace(/\s*$/,"");a.value=b}return b}function inputNullCheck(a){if(_trim(a)==""){alert("메일주소를 정확히 입력해주세요.");a.focus();return false}else{return true}}function checkEmailDomain_naver(b){var d=true;var a=new Array("jr.naver.com","navah.com","navar.com","navar.net","nave.co","nave.com","naver.co.kr","naver.com.kr","naverr.com","navet.com","navev.com","navr.com","never.com","www.naver.com","navver.com","haver.com","never.net","naver.cov","naver.coom","naver.conm","naver.con","naver.come","naver.com.com","naver.com.co.kr","naver.coa","naver.co","naver.cmm","naver.cm","naver.cdm","naver.ccom","naver.cam","naven.com","navef.com","naveer.com","naveeer.com","navee.com","navear.com","nave.rcom","nave.net","nave.moe","nave.kr.com","navcr.com","navar.co.kr","navaer.com","navab.com","nava.com","nauer.com");for(var c=0;c<a.length;c++){if(b.toLowerCase()==a[c]){d=false;break}}return d}function checkEmailDomain_etc(b){var d=true;var a=new Array("hanmail.com","daum.com","hanmil.net","hanamil.net","kr.yahoo.com","hanmeil.net","hanmai.net","daum.co.kr","nate.net");for(var c=0;c<a.length;c++){if(b.toLowerCase()==a[c]){d=false;break}}return d}function extractHighestDomain(a){return a.substring(a.lastIndexOf(".")+1,a.length)}function checkEmailDomain_highest(a){var e=false;var b=extractHighestDomain(a);var d=new Array("ASIA","AF","AL","AS","AD","AO","AI","AQ","AG","AR","AM","AW","AC","AU","AT","AZ","AN","AE","AERO","BS","BH","BD","BB","BY","BZ","BT","BJ","BE","BM","BO","BA","BW","BV","BR","BN","BG","BF","BI","BIZ","CM","CA","CV","CF","CL","CN","CX","CC","CO","KM","CD","CG","CK","CR","CI","CU","CY","CZ","COM","CH","COOP","DK","DJ","DM","DO","DZ","DE","EC","EG","ER","EE","ET","EU","EDU","ES","EH","FJ","FI","FR","FK","FO","FM","GB","GF","GH","GI","GR","GL","GD","GP","GU","GT","GG","GN","GW","GY","GQ","GOV","GA","GM","GE","GS","HT","HM","HR","HN","HK","HU","ID","IR","IQ","IE","IM","IL","IT","IO","IS","IN","INT","INFO","JM","JP","JE","JO","KZ","KE","KI","KP","KR","KW","KG","KH","KY","KN","LB","LS","LR","LY","LI","LT","LU","LA","LV","LC","LK","MD","MC","MN","MS","MA","MZ","MM","MW","MY","MV","ML","MT","MH","MQ","MR","MU","ME","MO","MK","MG","MX","MP","MOBI","MIL","MUSEUM","NA","NR","NP","NL","NC","NZ","NI","NE","NG","NU","NET","NF","NO","NAME","OM","ORG","PK","PW","PA","PG","PY","PE","PH","PN","PL","PT","PR","PF","PM","PS","QA","RE","RO","RU","RW","SG","SK","SI","SB","SO","SV","SM","ST","SA","SN","SC","SL","SH","SD","SR","SJ","SZ","SE","SY","TW","TJ","TZ","TH","TG","TK","TO","TT","TN","TR","TM","TC","TV","TD","TP","TF","US","UM","UY","UZ","UG","UA","UK","VE","VN","VG","VI","VA","VC","VU","WF","WS","YT","YE","YU","ZR","ZM","ZW","ZA");for(var c=0;c<d.length;c++){if(b.toUpperCase()==d[c]){e=true;break}}return e}function isActiveNaverEmail(a){var b=false;jQuery.ajax({type:"post",url:"/support/naverEmailInfo.help",dataType:"xml",data:"userid="+encodeURIComponent(a),async:false,success:function(c){if(jQuery(c).find("UserInfo").find("Is_active").text()=="1"){b=true}else{b=false}},error:function(e,c,d){b=true}});if(!b){alert("답변 받을 수 없는 메일주소 입니다. 메일주소를 수정하여 주세요.")}return b};