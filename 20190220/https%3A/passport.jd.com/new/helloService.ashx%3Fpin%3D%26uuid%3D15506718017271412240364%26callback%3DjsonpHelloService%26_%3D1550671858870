        _proto.trigger('timeAjust', _proto.now);
      }, function () {
        _proto.now = new Date().getTime();
        _proto.trigger('timeAjust', _proto.now);
      });
    } */
    if (!_useLocal) _proto.now = new Date(pageConfig.timestamp)
    else _proto.now = new Date().getTime()

    pageConfig.promos = {}
    window.Promos = Promos

