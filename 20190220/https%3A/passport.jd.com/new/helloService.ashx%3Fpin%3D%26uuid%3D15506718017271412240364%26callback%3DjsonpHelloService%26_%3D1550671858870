      seajs.use('ad_noob', function (noob) {
        var deferred = [],
          data = [],
          floorclog,
          fsbgclog
        $.each(noob, function (k, v) {
          if (v.id == 'floor') {
            floorclog = v.clog
            return
          }
          if (v.id == 'fsbgclog') {
            fsbgclog = v.clog
            return
          }
